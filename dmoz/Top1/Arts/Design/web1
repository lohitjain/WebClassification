function di20(id,newSrc) {
	document.images[id].src = eval(newSrc+".src");
}




function popUp(URL) {
window.open(URL,'','location=yes,scrollbars=yes,status=yes,toolbar=yes,resizable=yes,width=380,height=450,screenX=10,screenY=10,top=10,left=10');
}


function openaddwindow(page) {
addwin = window.open(page, "addwin", "toolbar=0,location=0,directories=0,status=1,scrollbars=1,menubar=0,resizable=0,height=440,width=420");
addwin.focus();
}

var currentpic = "";
function openjumbowindow(page) {
	if(page != ''){
		jumbo = window.open(page, "jumbo", "toolbar=0,location=0,directories=0,status=1,scrollbars=0,menubar=0,resizable=0,height=500,width=500");
		jumbo.focus();
	}
	else if(page == '' && currentpic != ''){
		//jumbo = window.open('http://www.designaddict.com/' + currentpic, "jumbo", "toolbar=0,location=0,directories=0,status=1,scrollbars=0,menubar=0,resizable=0,height=500,width=500");

		
