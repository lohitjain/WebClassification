<script src="http://www.artscatalyst.org/scripts/jquery-ui-1.7.2.custom.min.js" type="text/javascript" charset="utf-8"></script>
<script src="http://www.artscatalyst.org/scripts/jquery.cookie.js" type="text/javascript" charset="utf-8"></script>                                                                               
<script src="http://www.artscatalyst.org/scripts/jquery.validate.min.js" type="text/javascript" charset="utf-8"></script>
<script src="http://www.artscatalyst.org/scripts/jquery.pngFix.js" type="text/javascript" charset="utf-8"></script>
<script src="http://www.artscatalyst.org/scripts/jquery.fancybox-1.2.1.js" type="text/javascript" charset="utf-8"></script>
<script src="http://www.artscatalyst.org/scripts/swfobject.js" type="text/javascript" charset="utf-8"></script>    
<script src="http://www.artscatalyst.org/scripts/sifr.js" type="text/javascript" charset="utf-8"></script>


	<script type="text/javascript" charset="utf-8">
	//<![CDATA[ 
		//JS Back button
		    function goback(){history.back();}
	
		//=============================================================	
			//Swfobject
		//=============================================================
				
					swfobject.registerObject("imagerotator", "7.0.0", "http://www.artscatalyst.org/site_assets/flash/expressInstall.swf");
				
				swfobject.registerObject("video_player", "7.0.0", "http://www.artscatalyst.org/site_assets/flash/expressInstall.swf");

	//***=============================================================***	
		//JQUERY
	//***=============================================================***
		$(document).ready(function(){	
			//=============================================================	
				//Globally applied values
			//=============================================================

			// png fix
			$("div#content").pngFix();
		
			// move form labels inside input
				 // set all input.text default value according to alt attribute
				        $("input[type='text']").each(function(){$(this).val($(this).attr("alt"));});
			     // clear input.text on focus, if still in default
				        $("input[type='text']").focus(function() {
				          if($(this).val()==$(this).attr("alt")) {$(this).val("");}
				        });
			      // if field is empty afterward, add text again
			 	       $("input[type='text']").blur(function() { 
				          if($(this).val()=="") {$(this).val($(this).attr("alt"));}
				        });	
			
			// remove 'nojava' class from sliding captions
			$("span").removeClass("nojava");
			$("div.detail").removeClass("nojava");
			$("li.list_style").removeClass("nojava");		

			// info box slider
				$('li.list_item.small').hover(function(){
					$("span.imageview", this).stop().animate({top:'0px'},{queue:false,duration:500});
				}, function() {
					$("span.imageview", this).stop().animate({top:'65px'},{queue:false,duration:500});
				});

				$('.list_item.medium').hover(function(){
					$("span", this).stop().animate({top:'0px'},{queue:false,duration:700});
				}, function() {
					$("span", this).stop().animate({top:'145px'},{queue:false,duration:700});
				});
				
				// setting dynamic height for homepage slider
				var boxheight = $('div.detail').height();
				$("div.detail").css({'top' :'-' + boxheight + 'px'});
				$('.large').hover(function(){
					$("div.detail", this).stop().animate({top:'0px'},{queue:false,duration:500});
				}, function() {
					$("div.detail", this).stop().animate({top:'-' + boxheight + 'px'},{queue:false,duration:500});
				});
		
 
		// fancybox
			$("a.group").fancybox();	
			$("a.group:has(img)").addClass("zoom");
			$("a.zoom").append('<div class="icon"></div>');
	
			//Add .hover class to elements that aren't anchor links
						$(".link").hover(
							function(){$(this).addClass("hover");},
							function(){$(this).removeClass("hover");}
						);

					
			//add 'last' class to event listings items		
			
		
			
			
					

			//add 'first' and 'last' class to projects filtering dates/letters
			
	
			
			
			
		

			/* add last class to items on left side column on blog */
			$("div div.recent:last-child").addClass("last");
		

			// switch between text and image
			$("li.list_style.text").click(function () {
				$("div#main_content div.list span.imageview").css({top:'0px'});
				$("li.list_style.image").css({color:'#323232'});
				$(this).css({color:'#ed0282'});				
				$("div#main_content div.list span").removeClass("imageview");
				$("div#main_content div.list").addClass("textview");					
			});
			
			$("li.list_style.image").click(function () {
				$("div#main_content div.list").removeClass("textview");
				$("li.list_style.text").css({color:'#323232'});
				$(this).css({color:'#ed0282'});				
				$("div#main_content div.list span").addClass("imageview");			
				$("div#main_content span.imageview").css({top:'65px'})	
			});

			
				//Validate forms       	       			
				
				//Add custom method 	
					$.validator.addMethod("keywords_reqstring", function(value, element) {
					    return (value != "Search");},"Please enter a keyword");	
					
					$.validator.addMethod("section_keywords_reqstring", function(value, element) {
					    return (value != "Quick find");},"Please enter a keyword");											 	


					
					
					
					
					$("form#mailing_list").validate({
						rules: {
							name: {required: true},
							email: {required: true, email:true} 
						},
						messages: {
							name: {required:"Please enter your name"},
							email: {required:"Please enter your email address"}
						}
					});
					
					$("form#mailing_list02").validate({
						rules: {
							compact_name: {required: true},
							compact_email: {required: true, email:true} 
						},
						messages: {
							compact_name: {required:"Please enter your name"},
							compact_email: {required:"Please enter your email address"}
						}
					});
					
					$("form#global_search").validate({
						rules: {
							keywords: {required: true, minlength: 3, keywords_reqstring: "#keywords"}
						},
						messages: {
							keywords: {required:"Please enter a keyword", minlength:"Minimum 3 characters"}
						}
					});
					
					$("form#section_search").validate({
						rules: {
							section_keywords: {required: true, minlength: 3, section_keywords_reqstring: "#section_keywords"}
						},
						messages: {
							section_keywords: {required:"Please enter a keyword", minlength:"Minimum 3 characters"}
						}
					});
					
		//=============================================================	
			//Secion-specific values
		//=============================================================	
			
				//Home page - tabbable content
				$("div.tabs").tabs({
					event: 'click',
					fx: { opacity: 'toggle' },
					cookie: { expires: 30 }
				});
				
				
		});
