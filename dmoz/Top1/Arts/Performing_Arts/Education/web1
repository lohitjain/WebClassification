<script>
jQuery(document).ready(function($) {
	 var mobile = (/iphone|ipad|ipod|android|blackberry|mini|windows\sce|palm/i.test(navigator.userAgent.toLowerCase())); 
    if (mobile) { 
	$('#full-bg img').css('display', 'none');
	$('body.home .wrapper').css('background', 'url(http://www.lcpa.co.uk/wp-content/themes/lcpa2013/images/banner/banner1.jpg) no-repeat top center');
	var images = ["http://www.lcpa.co.uk/wp-content/themes/lcpa2013/images/banner/banner2_mob.jpg", "http://www.lcpa.co.uk/wp-content/themes/lcpa2013/images/banner/banner3_mob.jpg", "http://www.lcpa.co.uk/wp-content/themes/lcpa2013/images/banner/banner4_mob.jpg", "http://www.lcpa.co.uk/wp-content/themes/lcpa2013/images/banner/banner5_mob.jpg", "http://www.lcpa.co.uk/wp-content/themes/lcpa2013/images/banner/banner6_mob.jpg", "http://www.lcpa.co.uk/wp-content/themes/lcpa2013/images/banner/banner1_mob.jpg"];
                var index = -1;
 window.setInterval(function () {
                    index = (index + 1 < images.length) ? index + 1 : 0;
                    $('body.home .wrapper').css("background", "url('" + images[index] + "')");
					$('body.home .wrapper').css('transition','opacity 1s ease-in-out','-webkit-transition', 'opacity 1s ease-in-out');}, 4000);

	} else {
		setInterval(function(){
        $('#full-bg img.active').animate({opacity:0},500, function(){
            $(this).removeClass('active');
        })
        if($('#full-bg img.active').next().length>0)
            $('#full-bg img.active').next().animate({opacity:1},500).addClass('active');
        else
            $('#full-bg img:first').animate({opacity:1},500).addClass('active');
 
    } ,4000);
$('#full-bg img:first').animate({opacity:1},400).addClass('active');
	}
});
</script>
