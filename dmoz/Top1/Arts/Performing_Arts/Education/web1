<script type="text/javascript">
    (function ($) {

        // TABULATE CHANNELS
        $(".channels").each(function () {
            // get the channels
            var channels = $(this).find(".channel");
            if (channels.length > 0) {

                // build tab navivation
                $(this).prepend("<div class=\"tab-nav\"><ul class=\"tabs\"></ul></div>");
                var tabs = $(this).find(".tabs");

                channels.each(function (i, channel) {
                    // get the curretn channel and add the tab class
                    var current = $(channel);
                    current.addClass("tab");

                    // if its the first tab, make it active
                    if (i == 0)
                        current.addClass("active");

                    // get the tab title from the channel title
                    var tabTitle = $(channel).find(".channel-title").text();

                    // add the tab li element
                    tabs.append("<li class=\"" + (i == 0 ? "active" : "") + "\"><a href=\"javascript:void(0);\">" + tabTitle + "</a></li>");

                    current.find(".floatleft").hide();

                    // hide the channels header
                    current.find(".channel-header").hide();
                });

                //set the click events
                $(this).find(".tabs li a").click(function (event) {
                    // get the li item
                    var parent = $(this).parent();

                    // if its not active
                    if (!parent.hasClass("active")) {

                        // remove all active class from siblings
                        parent.siblings().removeClass('active');

                        // add active class to li item
                        parent.addClass('active');

                        // get the active tabs index
                        var activeTabIndex = parent.parent().find("li").index(parent);

                        // get all the tabs
                        var tabs = $(this).closest(".channels").find(".tab");

                        // remove active class from all the tabs
                        tabs.removeClass("active");

                        // add active class to active tab
                        tabs.eq(activeTabIndex).addClass("active");
                    }
                });

                // get the floatleft element
                var floatLeft = channels.eq(0).find(".floatleft");
                //floatLeft.show();

                var tabNav = $(this).find(".tab-nav");

                // insert it after the tabs ul
                $(floatLeft).insertAfter(tabNav);

                // show floatleft in after tabnav
                tabNav.siblings(".floatleft").show();
            }
            // end if
        });
    })(jQuery);
</script>
        </div>
