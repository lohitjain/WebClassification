                <div class="box head_box theme_light_background_hook theme_whats_on whats_on_box"><div class="odd"><h2><a href="http://www.gsmd.ac.uk/about_the_school/home/view_all_events.html" title="Opens internal link in current window" target="_top" class="heading heading_padded theme_border_hook serif" >What's on</a></h2></div><div class="even"><script>
						function goLastMonth(month, year){
							// If the month is January, decrement the year
							if(month == 1){
								--year;
								month = 13;
							}
							document.location.href = 'http://www.gsmd.ac.uk/?month='+(month-1)+'&year='+year+'#calendar';
						}
						//next function
						function goNextMonth(month, year){
							// If the month is December, increment the year
							if(month == 12){
							++year;
							month = 0;
							}
							document.location.href = 'http://www.gsmd.ac.uk/?month='+(month+1)+'&year='+year+'#calendar';
						}  
						function remChars(txtControl, txtCount, intMaxLength)
						{
							if(txtControl.value.length > intMaxLength)
								txtControl.value = txtControl.value.substring(0, (intMaxLength-1));
							else
								txtCount.value = intMaxLength - txtControl.value.length;
						}
						</script>
						
