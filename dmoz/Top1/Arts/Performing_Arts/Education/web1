<script type="text/javascript">jQuery.noConflict();</script> <link rel="stylesheet" href="/commonspot/commonspot.css" type="text/css" id="cs_maincss" /> </head><body lang="en" class="CS_Document"><a name="__topdoc__"></a><script src="/commonspot/pagemode/always-include-common.js" type="text/javascript"></script><script src="/commonspot/pagemode/always-include-ns.js" type="text/javascript"></script><script language="JavaScript">
	function check_options()
	{
		var val="";
		for(i=0; i<document.seek1.search_type.length; i++)
		{
			if(document.seek1.search_type[i].checked==true)
			{
				val=document.seek1.search_type[i].value;
			}
		}          
		return val;
	}
	function initSearch(form) {
		var sSearchType = check_options();
		switch(sSearchType) {
			case 'Site' :
				document.Site.q.value = form.searchFor.value;
				document.Site.submit();
				return(true);
			case 'SubSite' :
				document.Site.q.value = form.searchFor.value;
				document.Site.as_sitesearch.value = form.sitesearch.value;
				document.Site.submit();
				return(true);
			case 'Phonebook' :
				document.Phonebook.name.value = form.searchFor.value;
				document.Phonebook.submit();
				return(true);
			case 'Web' :
				document.Web.q.value = form.searchFor.value;
				document.Web.submit();
				return(true);
			case 'Calendar' :
				document.Calendar.keyword.value = form.searchFor.value;
				document.Calendar.submit();
				return(true);
			default :
				return(false);
		}
	}
</script>
