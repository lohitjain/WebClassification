<script language="javascript">
	
	function ShowNoAccessFBMessage()
	{
		alert('Access is restricted to Equity members in good standing only.');
	}
	if (getQueryVariable("sm") == "y")
		ShowNoAccessMessage();
		
	if (getQueryVariable("sm") == "yfb")
		ShowNoAccessFBMessage();
		
	if (getQueryVariable("nw") != "")
		popUp(URLDecode(getQueryVariable("nw")));
		
	function getQueryVariable(variable) 
	{  
		var query = window.location.search.substring(1);  
		var vars = query.split("&");  
		
		for (var i=0;i<vars.length;i++) 
		{    
			var pair = vars[i].split("=");    
			if (pair[0] == variable) 
				return pair[1]; 
		} 
		
		return "";
	}
	
	function URLDecode(psEncodeString) 
	{
	  return unescape(psEncodeString); 
	}
	
</script>
