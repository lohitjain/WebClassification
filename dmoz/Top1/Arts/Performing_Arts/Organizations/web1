			}
			
			function popUp(url)
			{
				sealWin=window.open(url,"win",'toolbar=0,location=1,directories=0,status=1,menubar=1,scrollbars=1,resizable=1,width=680,height=450');self.name = "mainWin";
			}

			function StartUp()
			{
				
				
				document.getElementById('NewHeadlines').innerHTML = headlines[0];
				
				if (headlines.length > 1)
					setTimeout('UpdateNewHeadlines()', 5000);
				
				return true;
			}
			
			var headlines = new Array(12)
headlines[0] = '<a href="http://staging-server/EquityWeb/NewsAndEvents/News/2013/DuesReferendum/">2014 Dues Referendum<a>';
headlines[1] = '<a href="http://list.web.net/archives/caea-l/2013-November/011393.html">Equity Dental Plan In Effect in 2014</a>';
headlines[2] = '<a href="/EquityWeb/EquityLibrary/Default.aspx?Expand=Insurance">ACE LIfe insurance forms</a>';
headlines[3] = '<a href="/EquityWeb/EquityLibrary/Newsletters/Default.aspx">2014 Winter/Spring EQ online</a>';
headlines[4] = '<a href="/EquityWeb/AboutEquity/ServiceStandards.aspx">Service Standards</a>';
headlines[5] = '<a href="http://www.caea.com/EquityWeb/Governance/Council2012-2015.aspx">Contact an Equity Councillor</a>';
headlines[6] = '<a href="/EquityWeb/EquityLibrary/Publications/HST-GST.pdf" target="_new">GST/HST information update</a>';
headlines[7] = '<a href="/EquityWeb/MemberServices/Retirement/RRSPlist.aspx">RRSP funds on hold</a>';
headlines[8] = '<a href="/EquityWeb/EquityLibrary//MembersOnly/default.aspx">Monitoring Reports available online</a>';
headlines[9] = '<a href="/EquityWeb/MemberServices/DefaultingEngagers/Default.aspx">Defaulting engagers list</a>';
headlines[10] = '<a href="/EquityWeb//MemberServices/Retirement/QuarterlyPerformance.aspx">RRSP quarterly summary</a>';
headlines[11] = '<a href="/EquityWeb/MemberServices/Fees.aspx">Pay Equity dues through online tele-banking</a>';


			var currentShade = 0;
			var direction = 1;
			var currentHeadline = 0;
			
			function UpdateNewHeadlines()
			{
				currentShade += direction;
				var delay = 50;
				
				document.getElementById('NewHeadlines').setAttribute('class', 'Headline' + currentShade);
				document.getElementById('NewHeadlines').setAttribute('className', 'Headline' + currentShade);  // IE fix

				if (currentShade == 0)
				{
					direction = 1;
					delay = 6000;
				}
				
				if (currentShade == 8)
				{
					currentHeadline++;
					if (currentHeadline == headlines.length) currentHeadline = 0;
					
					document.getElementById('NewHeadlines').innerHTML = headlines[currentHeadline];

					direction = -1;
					delay = 500;
				}
					
				setTimeout('UpdateNewHeadlines()', delay);
			}
			
			
		</script>
	</head>
	<body topmargin=0 leftmargin=0 marginwidth=0 marginheight=0 onload="return StartUp();">

