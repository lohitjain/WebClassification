          google.friendconnect.container.openSocialSiteId = siteId;
          google.friendconnect.requestSignOut();
        });
      }
    </script>
<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/1285009/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/1285009/comments/default";
          
          holder.currentBlogUrl = "http://storytelling.blogspot.com/";
          holder.currentBlogId = "1285009";
        
        return holder;
      });
    }
  </script>
<script type="text/javascript">
  if (!window.registeredCommonBloggerCallbacks) {
    window.registeredCommonBloggerCallbacks = true;

