<STYLE TYPE="text/css">
#butterfly { position:absolute; z-index:1; left:-100px; top:120px; background: transparent; }
</STYLE>
<SCRIPT LANGUAGE="JavaScript" SRC="sdom.js"></SCRIPT>
<SCRIPT LANGUAGE="JavaScript">


<!--
//      Cross-Browser Dynamic Html Script
//      Written By David E Haasnoot
//      Copyright © 1998 WriteDesign


var coordArray=new Array();
coords = "-100,200,-80,180,-60,190,-40,170,-20,180,0,160,20,170,40,150,60,160,80,140,100,150,120,130,140,140,160,110,180,120,200,90,220,100,240,70,260,80,280,50,300,60,320,30,340,40,360,30,380,40,400,30,400,30,400,30,400,30,400,30,400,30,400,30,400,30,400,30,400,30,400,30,420,60,440,45,460,75,480,60,500,90,520,75,540,105,560,90,580,120,600,105,620,135,640,120,660,150,680,135,700,165,720,140,740,170,760,155,780,185,800,160,820,190,840,175,-100,200";
coordArray = coords.split(",");
count=0;
points=coordArray.length;
var myButterfly;


function moveit(which,count){
	if(ie||n||n6){
		if(count<points){
			myButterfly.moveTo(coordArray[count], coordArray[count+1]);
			//eval(layerRef+'["'+which+'"]'+styleRef+'.left='+coordArray[count]);
			//eval(layerRef+'["'+which+'"]'+styleRef+'.top='+coordArray[(count+1)]);
			count+=2;
			eval("setTimeout(\"moveit('"+which+"',"+count+")\",100)");
		}else{
			count = 0;
			eval("setTimeout(\"moveit('"+which+"',"+count+")\",100)");
		}
	}
}


function setup(){
	myButterfly = new dynLayer("butterfly");
	moveit("myButterfly",0);
}


//      Not To Be Used Without Permission
//-->


</SCRIPT>
