<script type="text/javascript">
//<![CDATA[
if (window != top)
    {
        top.location.href = location.href;
    }

function month(intMonth)
    {
        var MonthArray = new Array("January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December");
        return MonthArray[intMonth];
    }

function getDateStr()
    {
        var today = new Date();
        var year = today.getYear();
        if(year<1000) year+=1900;
        var todayStr = month(today.getMonth()) + " " + today.getDate() + ", " + year;
        return todayStr;
    }


sfHover = function() {
    var sfEls = document.getElementById("nav").getElementsByTagName("LI");

    var stnNav = document.getElementById("stn_nav");
    if (stnNav != null) { sfEls = concatCollections(sfEls, stnNav.getElementsByTagName("LI")); }

    for (var i=0; i<sfEls.length; i++) {
        sfEls[i].onmouseover=function() {
            this.className+=" sfhover";
        }
        sfEls[i].onmouseout=function() {
            this.className=this.className.replace(new RegExp(" sfhover\\b"), "");
        }
    }
}
//if (window.attachEvent) window.attachEvent("onload", sfHover);
if (document.all && window.attachEvent) window.attachEvent("onload", sfHover);
//]]>

</script>

