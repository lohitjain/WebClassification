							eventMouseover: function(calEvent,jsEvent) {
									var offset = $(this).offset();
									var addheight = 23;
									$("body").append(calEvent.tooltip);
									$("#tooltip")
										.maxZIndex({ inc: 5 })
										.css("top",(offset.top+addheight) + "px")
										.css("left",offset.left + "px")
										.fadeIn("slow");
							},
							eventMouseout: function(calEvent,jsEvent) {
								$("#tooltip").remove();	
							}
				});
				
				}, 250);
				
				
				if($('.calendarFilter > div').size() < 1){
					$('.calendarFilter').append('<div class="fc-event">ALL</div><div class="calendarCategory1">Storytelling</div><div class="calendarCategory2">Song</div><div class="calendarCategory3"></div><div class="calendarCategory4">Music</div><div class="calendarCategory5">Folk Monthly</div><div class="calendarCategory6">About</div><div class="calendarCategory7">Dance</div>');
				}
				$('.calendarFilter > div').click(function(){
					$('#calendar .fc-event').hide();
					$('.'+$(this).attr('class')).show();
				});
			});
		}
		else{
			$('#calendarWrapper').fadeToggle('slow',function(){$('#wrapper').fadeToggle('slow');});
		}
		return false;
	});
	
	$('.musicPlayerButton').popupWindow({ 
	height:260, 
	width:290, 
	top:50, 
	left:50 
	}); 
	
	function fadeHeader(){
		if($("#headers > div:visible").size() == 1){
			$("#headers > div:last").fadeIn(6000,function(){$("#headers > div").show();setTimeout(fadeHeader,6000);});
		}
		else{
			$("#headers > div:visible:last").fadeOut(6000,function(){setTimeout(fadeHeader,6000);});
		}
	}
	setTimeout(fadeHeader,6000);

});

</script>

<script language="javascript" type="text/javascript" src="js/swfobject.js"></script>



<script>

	function validateNewsletter() {
	
		if (document.getElementById('newsletter_email').value == "") {
			alert("Please enter an email address");
			return false;
		} else {
			return true;	
		}
	
	}
	
	
	</script>

