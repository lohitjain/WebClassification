<script type="text/javascript">
var timer;
function set_interval()
{
timer = setInterval("auto_logout()",300000);
}
 
function reset_interval()
{
clearInterval(timer);
timer = setInterval("auto_logout()",300000);
}
 
function auto_logout()
{
window.location="logout.html";
}
