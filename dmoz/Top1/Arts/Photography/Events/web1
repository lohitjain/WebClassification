<script type="text/javascript">
  // <![CDATA[
  //var galleries = new Array('video','pictures','tribute','awards','slideshow_2010');
 var galleries = new Array('video','lucy-grand-rex','tribute','awards','slideshow_2010');

  function getGallery(curGal) {
    if (document.getElementById('gallery_' + curGal)) {
	if (document.getElementById('gallery_' + curGal).className=="active") return false;
	else {
	  for (var i=0; i<galleries.length; i++) {
	      if (document.getElementById('gallery_' + galleries[i])) {
		if (galleries[i] != curGal) document.getElementById('gallery_' + galleries[i]).className="";
		else document.getElementById('gallery_' + galleries[i]).className="active";
              }
	  }
        }
     }
     if (curGal == 'video') {
       so = new SWFObject("../../content/home/flv/videoplayer2.swf", "map", "715", "436", "8", "#000000");
       so.addVariable("videoUrl", "visa_promo.flv");
     }  
     else if (curGal == 'video2') {
       so = new SWFObject("../../content/home/flv/videoplayer2.swf", "map", "715", "436", "8", "#000000");
       so.addVariable("videoUrl", "montage_nichols.flv");
     } 
     else if (curGal == 'video3') {
       so = new SWFObject("../../content/home/flv/videoplayer2.swf", "map", "715", "436", "8", "#000000");
       so.addVariable("videoUrl", "mitterrand_interview.flv");
     }
	 else if (curGal == 'gallery_slideshow_2010') {
       so = new SWFObject("./assets/swf/slideshow.swf", "map", "715", "436", "8", "#000000");
       so.addVariable("xmlUrl", "./content/xml/" + curGal + ".xml");
     } 
	 else {
       so = new SWFObject("./assets/swf/slideshow.swf", "map", "715", "436", "8", "#000000");
       so.addVariable("xmlUrl", "./content/xml/" + curGal + ".xml");
     }
     so.addParam("allowScriptAccess", "sameDomain");
     so.addParam("allowFullScreen", "false");
     so.addParam("quality", "high");
     so.addParam("wmode", "transparent");
//     so.write("flash_homepage");
     return false;
  }
getGallery('video'); //comment this line to disable video onLoad

// ]]>
</script>
