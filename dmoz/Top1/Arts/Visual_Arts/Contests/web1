<script language="javascript">
function addOpportunity(id,e) {
	xc = e.clientX+document.documentElement.scrollLeft;
	yc = e.clientY+document.documentElement.scrollTop;
	document.getElementById("addOpportunity_menu").style.left = xc+"px";
	document.getElementById("addOpportunity_menu").style.top = yc+"px";
	var Values	=	"";
	var success = function(t){ addOpportunitycb(response,Values,id);}
	var failure = function(t){ editFailed(response,Values);}
	var url     =  'addOpportunity.php';
	var pars    = 'id='+id;
	//var myAjax  = new Ajax.Request(url, {method:'post', postBody:pars, onSuccess:success, onFailure:failure});
	var response = $.ajax({type: "post",url:url,data: pars,success: success});
}


function addOpportunitycb(t,Values,id) {
	
	document.getElementById("addOpportunity_menu").style.display = 'block';
	document.getElementById("addOpportunity_menu").innerHTML = t.responseText;
	document.getElementById(id).src = "images/fav.gif";
	
}

function hideaddOpportunity_disp() {
	if(document.getElementById("addOpportunity_menu")) document.getElementById("addOpportunity_menu").style.display = 'none';
}

function getposition(e) {
	xc = e.clientX+document.documentElement.scrollLeft;
	yc = e.clientY+document.documentElement.scrollTop;
	document.getElementById("upgradesearch_menu").style.left = xc+"px";
	document.getElementById("upgradesearch_menu").style.top = yc+"px";
	document.getElementById("upgradesearch_menu").style.display = '';
	document.getElementById("upgradesearch_menu").innerHTML = "<table border=0 cellpadding=0 cellspacing=0><tr><td style='text-align:right; cursor: pointer;  padding-right: 10px; padding-top: 5px'><span onclick='hideUpgradeSearch_disp()' style='color:#990000;'><img src='https://www.theartlist.com/images/close2.gif'></span></td></tr><tr><td style='padding-left:10px; padding-right:10px; font-size: 14px' class='search_popup'>Premium Membership Required <span onClick='upgradeNowSearch_disp()' style='cursor:pointer; color:#990000'><strong>Click Here</strong></span> to upgrade your account now.</td></tr></table>";
}

function upgradeNowSearch_disp() {
	window.location.href = "upgrade.php";
	document.getElementById("upgradesearch_menu").style.display = 'none';
}

function hideUpgradeSearch_disp() {
	document.getElementById("upgradesearch_menu").style.display = 'none';
}

</script>

