function testimonial() {
	if (document.getElementById("testimonial"))	
	document.getElementById("testimonial").innerHTML = TESIMONIALS[t];
	var tmp = '<div  align="right" style="border:0px solid red;" class="destlocat"> <a href="http://www.theartlist.com/testimonials.php"><img src="http://www.theartlist.com/images/moretestimonials.gif" hspace="8" vspace="8"/></a></div>';
	document.getElementById("testimonialname").innerHTML = TESIMONIALSNAME[t] + tmp;
	if (t<33) {
		t = t + 1;
	} else {
		t = 0;
	};

 
};


function rotatetestimonial(){
	
	setInterval('testimonial()',9000);
};
	var strHeight = document.getElementById("testimonial-tp").style.height;
	var numHeight = strHeight.split("px");
	if(numHeight[0] < 150) {
	//alert(numHeight[0])
	document.getElementById("testimonial-tp").style.height = '150px';
	} else {
	 var height = eval(numHeight[0]) + 50;
	 //var height = eval(numHeight[0]);
	 //alert(height);
	 document.getElementById("testimonial-tp").style.height = height+'px';
	}
