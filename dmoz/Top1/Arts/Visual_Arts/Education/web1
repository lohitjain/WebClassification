function fadeIt(ele,url){
	Spry.Effect.DoFade('photoholder',{ duration: 1000, from: 100, to: 0, finish: function() {
		Spry.Utils.updateContent(ele, url, function() {
			Spry.Effect.DoFade('photoholder',{ duration: 1000, from: 0, to: 100 });
		});
  }
 });
}
</script>
