$(".button_bar a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'home page buttons', 'open', $(this).attr('href')]);
}
});

$("#footer a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'Fat footer links', 'open', $(this).attr('href')]);
}
});

});
