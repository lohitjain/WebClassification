$("#list3bottom a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'Text column 3 links', 'open', $(this).attr('href')]);
