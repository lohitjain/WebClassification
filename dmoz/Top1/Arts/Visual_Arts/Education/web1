 $("#list1top a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'Icons column 1 links', 'open', $(this).attr('href')]);
}
});

$("#list2top a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'Icons column 2 links', 'open', $(this).attr('href')]);
}
});

$("#list3top a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'Icons column 3 links', 'open', $(this).attr('href')]);
}
});

$("#list4top a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'Icons column 4 links', 'open', $(this).attr('href')]);
}
});

$("#list1bottom a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'Text column 1 links', 'open', $(this).attr('href')]);
}
});

$("#list2bottom a").mouseup(function(){
if($(this).attr('href').toLowerCase() != "javascript:;" && $(this).attr('href') != "#") {
_gaq.push(['_trackEvent', 'Text column 2 links', 'open', $(this).attr('href')]);
