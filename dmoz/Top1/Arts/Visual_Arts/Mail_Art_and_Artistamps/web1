<SCRIPT LANGUAGE="JavaScript1.2">

var hoy = new Date()
  var meses = new Array("ENERO","FEBRERO","MARZO","ABRIL","MAYO","JUNIO","JULIO","AGOSTO","SEPTIEMBRE","OCTUBRE","NOVIEMBRE","DICIEMBRE")                                                          
  var i = hoy.getMonth()
  var mes = meses[i] 
  var any = hoy.getFullYear()

var ctext = mes+" "+any;
var speed = 1000;
var x = 0;
var color = new Array(
"#cc9999", 
"#996666", 
"#664444",
"#101010"
);
ie4 = (document.all)
ns6 = (document.getElementById)

if (ie4 || ns6) {
document.write('<div id="c" style="font-family:Arial;font-size:10pt"><center>'+ctext+'</center></div>');
}
function chcolor(){ 

if (ie4) {
document.all.c.style.color = color[x];
}
if (ns6) {
document.getElementById("c").style.color = color[x];
}
(x < color.length-1) ? x++ : x = 0;
}

setInterval("chcolor()",1000);
// End -->
</SCRIPT>  

