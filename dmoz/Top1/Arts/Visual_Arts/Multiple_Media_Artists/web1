december(today.getDate());
document.month="decembre 2014";
}
document.day=""
if(day_of_week==0)
document.day="dimanche &nbsp;&nbsp;&nbsp;";
if(day_of_week==1)
document.day="lundi&nbsp;&nbsp;&nbsp;";
if(day_of_week==2)
document.day="mardi&nbsp;&nbsp;&nbsp;";
