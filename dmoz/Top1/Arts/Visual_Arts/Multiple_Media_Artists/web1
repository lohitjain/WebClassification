document.day="samedi&nbsp;&nbsp;&nbsp;";
}
function january(date) {
if(date==1)
date_statement="";
}
function february(date) {
if(date==14)
date_statement="";
}
function march(date) {
if(date==21)
date_statement="";
}
function april(date) {
if(date==1)
date_statement="";
}
function may(date) {
if(date==1)
date_statement="";
}
function june(date) {
if(date==21)
date_statement="";}
function july(date) {
if(date==24)
date_statement="";
}
function july(date) {
if(date==14)
date_statement="";
}
function august(date) {
}
function september(date) {
if(date==21)
date_statement="";
}
function october(date) {
if(date==31)
date_statement="";
}
function november(date) {
}
function december(date) {
if(date==21)
date_statement+="";
if(date==25)
date_statement="";
if(date==31)
date_statement="";

}
function time_of_day() {
var time=today.getHours();
time_statement=""
if(time>=6 && time<8)
time_statement=""
if(time>=8 && time<12)
time_statement=""
if(time>=12 && time<13)
time_statement=""
if(time>=13 && time<17)
time_statement=" "
if(time>=17 && time<22)
time_statement=" "
if(time>=22 || time<6)
time_statement=""
}
// -- End Hiding Here --></script>
