<script type="text/javascript">
function validateForm()
{
var x=document.forms["purchaseForm"]["name"].value;
if (x==null || x=="")
  {
  alert("Name must be filled out");
  return false;
  }

var x=document.forms["purchaseForm"]["email"].value;
var atpos=x.indexOf("@");
var dotpos=x.lastIndexOf(".");
if (atpos<1 || dotpos<atpos+2 || dotpos+2>=x.length)
  {
  alert("Not a valid e-mail address");
  return false;
  }

var x=document.forms["purchaseForm"]["phone"].value;
if (x==null || x=="")
  {
  alert("Phone Number must be filled out");
  return false;
  }

}
</script>
