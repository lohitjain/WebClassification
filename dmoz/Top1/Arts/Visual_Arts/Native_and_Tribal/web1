<hr>

<p><font size="2">To contact us write: Arte Maya Tz'utuhil, P.O. Box 40391, San 
Francisco, CA 94140.&nbsp; Telephone: (415) 282-7654. I will be away from 
Thursday August 15 until Saturday August 23. 

<script type='text/javascript'> 
// <!-- 1771600489
// This script is (C) Copyright 2004 Jim Tucek
// Leave these comments alone!  For more info, visit
// www.jracademy.com/~jtucek/email/ 
 
function bet(friendship,appearance,squirrel) {
friendship += ' ';
var cleverness = friendship.length;
var way = 0;
var singer = '';
for(var cavern = 0; cavern < cleverness; cavern++) {
way = 0;
while(friendship.charCodeAt(cavern) != 32) {
way = way * 10;
way = way + friendship.charCodeAt(cavern)-48;
cavern++;
}
singer += String.fromCharCode(shoe(way,appearance,squirrel));
}
parent.location = 'm'+'a'+'i'+'l'+'t'+'o'+':'+singer;
}
 
function keep(companion,courtesy,thing) {
companion += ' ';
var day = companion.length;
var physicist = 0;
for(var shoulder = 0; shoulder < day; shoulder++) {
physicist = 0;
while(companion.charCodeAt(shoulder) != 32) {
physicist = physicist * 10;
physicist = physicist + companion.charCodeAt(shoulder)-48;
shoulder++;
}
//document.write('&');
//document.write('#');
//document.write(shoe(physicist,courtesy,thing));
document.write(String.fromCharCode(shoe(physicist,courtesy,thing)));
}
}
 
function shoe(icon,individual,privacy) {
if (privacy % 2 == 0) {
law = 1;
for(var optician = 1; optician <= privacy/2; optician++) {
nest = (icon*icon) % individual;
law = (nest*law) % individual;
}
} else {
law = icon;
for(var computer = 1; computer <= privacy/2; computer++) {
nest = (icon*icon) % individual;
law = (nest*law) % individual;
}
}
return law;
}
// -->
</script>
 
