<script>

function select_link(obj) {

  var expandObj = (obj.className.match('selected') == null);
  $$('.selected').each(function(e){ 
  e.removeClassName('selected')
    $(e.id.concat('-list')).hide();
  });

  if (expandObj)  {
    obj.addClassName('selected');
    $(obj.id.concat('-list')).show();
  }
}

$$('.navlink').each(function(e){
  e.observe('click', function() {
    select_link($(this));
  });
});

document.observe('dom:loaded', function(){
  if('/' == '/') {
    // select_link($('special-link'));
  } else {
    var current_uri = document.location.href;
    //var current_uri = 'http://www.savvycollector.com/';
      $$('.sitemap-groups').each(function(e){
        $$('#' + e.id + ' a').each(function(link){
          if(link.href == current_uri) {
            $(e.id.concat('-link')).addClassName('selected');
            $(e.id.concat('-link-list')).show();
            $break;
       }
     })
   }) 
  }
});
</script>

