		<script type="text/javascript">
			function set_text(field,default_text) {
				if($(field).value==default_text) {

					$(field).value = "";

				} else if($(field).value.empty()) {

					$(field).value=default_text;

				}
			}

			$('nav-search').observe('click', function(event) {
				if($('search-container').getStyle('display') == 'none') {
					$('search-container').setStyle({display:'block'});
					$('nav-search').removeClassName('nav-search-unselected');
					$('nav-search').addClassName('nav-search-selected');
				}
				else {
					$('search-container').setStyle({display:'none'});
					$('nav-search').addClassName('nav-search-unselected');
					$('nav-search').removeClassName('nav-search-selected');
				}
			});

			
				$('remove_border').observe('click', function(event) {
					if($('search-container').getStyle('display') == 'none') {
						$('search-container').setStyle({display:'block'});
						$('nav-search').removeClassName('nav-search-unselected');
						$('nav-search').addClassName('nav-search-selected');
					}
					else {
						$('search-container').setStyle({display:'none'});
						$('nav-search').addClassName('nav-search-unselected');
						$('nav-search').removeClassName('nav-search-selected');
					}
				});

			
			$('search_keyword').observe('focus', function() {
				set_text('search_keyword','Enter Keyword...');
			});

			$('search_keyword').observe('blur', function() {
				set_text('search_keyword','Enter Keyword...');
			});

			$$(".aux-keywords").each(function(e) {
				e.observe('focus',
				function() {
					if(this.value == this.readAttribute('retval')) {
						this.value = "";
					} else if(this.value.empty()) {
						this.value = this.readAttribute('retval');
					}

				});

				e.observe('blur',
				function() {
					if(this.value == this.readAttribute('retval')) {
						this.value = "";
					} else if(this.value.empty()) {
						this.value = this.readAttribute('retval');
					}

				});

			});

			$('search_form').observe('submit', function() {
				$$(".aux-keywords").each(function(e) {
					if(e.value == e.readAttribute('retval')) {
						e.value = '';
					}
				});

				if ($('search_keyword').value == 'Enter Keyword...') {
					$('search_keyword').value = '';
				}

				if ($('invnum_keyword').value == 'Enter Inventory #...') {
					$('invnum_keyword').value = '';
				}

				return true;
			});

			

							
				$('search_price_low').observe('focus', function() {
					set_text('search_price_low','low');
				});

				$('search_price_low').observe('blur', function() {
					set_text('search_price_low','low');
				});


				$('search_price_high').observe('blur', function() {
					set_text('search_price_high','high');
				});

				$('search_price_high').observe('focus', function() {
					set_text('search_price_high','high');
				});
				
			
			
					</script>

	</div>

<!-- END nav.search.tpl -->
	
</div>


<script type="text/javascript">
	if ($('artist-dropdown'))
		new Dropdown({container:'artist-dropdown'});

	if ($('exhibition-dropdown'))
		new Dropdown({container:'exhibition-dropdown'});

	if ($('type-dropdown'))
		new Dropdown({container:'type-dropdown'});
		
	if ($('architects-dropdown'))
		new Dropdown({container:'architects-dropdown'});
</script>

<!-- END nav.level.2.tpl -->
	
	
</div>
<!-- END frame.header.tpl -->		<div id="frame-modules">
		
