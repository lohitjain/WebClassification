<script type='text/javascript'>

Event.observe($('per_page'), 'change', function() {
  var loc = window.location.href;
  var delim = '?';
  if (loc.include('?') && !loc.include('?per_page')) {
    delim = '&';
  }
  loc = loc.gsub(/(\?|&)per_page=.*/, '');
  loc = loc.gsub(/\/#/, '');
  window.location = loc + delim + 'per_page=' + $('per_page').value;
});

</script>


        
