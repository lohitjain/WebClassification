<TABLE BORDER=0 CELLPADDING=1 CELLSPACING=0 width=250>
	<tr>
	  <td bgcolor="#286fc7">
	  	<TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" bgcolor="#FFFFFF" width="250">
				<tr>
					<td bgcolor="#FFFFFF" COLSPAN=4><IMG SRC="b.gif" HEIGHT="1" width="1" border="0"></td>
				</tr>

			<tr>

				<td bgcolor= "#286fc7" height="1" valign="middle"></td>
				<td bgcolor= "#286fc7" height="1" valign="middle" align="right</td>
				<td bgcolor="#FFFFFF" height="1" width=1 align="right"></td>
