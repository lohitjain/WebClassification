<script language="JavaScript"><!--

var field=0; var ready=0; var possible=0;
var autoon=0; var cano=9; var pono=9; var veloc=250; var timer;

var cardno= new Array(9);
cardno[0]=9; cardno[1]=9; cardno[2]=9; cardno[3]=9;
cardno[4]=9; cardno[5]=9; cardno[6]=9; cardno[7]=9; cardno[8]=9;
var atpool= new Array(9);
atpool[0]=1; atpool[1]=1; atpool[2]=1; atpool[3]=1;
atpool[4]=1; atpool[5]=1; atpool[6]=1; atpool[7]=1; atpool[8]=1;
var posno=new Array(9);
posno[0]=9; posno[1]=9; posno[2]=9; posno[3]=9; posno[4]=9; posno[5]=9;
posno[6]=9; posno[7]=9; posno[8]=9;

var b = new Array(9);

b[0] = new Array(4); b[0][0]= new Image(); b[0][0].src = "a11.gif";
                                   b[0][1]= new Image(); b[0][1].src = "a12.gif";
                                   b[0][2]= new Image(); b[0][2].src = "a13.gif";
                                   b[0][3]= new Image(); b[0][3].src = "a14.gif";
b[1] = new Array(4); b[1][0]= new Image(); b[1][0].src = "a21.gif";
                                   b[1][1]= new Image(); b[1][1].src = "a22.gif";
                                   b[1][2]= new Image(); b[1][2].src = "a23.gif";
                                   b[1][3]= new Image(); b[1][3].src = "a24.gif";
b[2] = new Array(4); b[2][0]= new Image(); b[2][0].src = "a31.gif";
                                   b[2][1]= new Image(); b[2][1].src = "a32.gif";
                                   b[2][2]= new Image(); b[2][2].src = "a33.gif";
                                   b[2][3]= new Image(); b[2][3].src = "a34.gif";
b[3] = new Array(4); b[3][0]= new Image(); b[3][0].src = "a41.gif";
                                   b[3][1]= new Image(); b[3][1].src = "a42.gif";
                                   b[3][2]= new Image(); b[3][2].src = "a43.gif";
                                   b[3][3]= new Image(); b[3][3].src = "a44.gif";
b[4] = new Array(4); b[4][0]= new Image(); b[4][0].src = "a51.gif";
                                   b[4][1]= new Image(); b[4][1].src = "a52.gif";
                                   b[4][2]= new Image(); b[4][2].src = "a53.gif";
                                   b[4][3]= new Image(); b[4][3].src = "a54.gif";
b[5] = new Array(4); b[5][0]= new Image(); b[5][0].src =  "a61.gif";
                                   b[5][1]= new Image(); b[5][1].src = "a62.gif";
                                   b[5][2]= new Image(); b[5][2].src = "a63.gif";
                                   b[5][3]= new Image(); b[5][3].src = "a64.gif";
b[6] = new Array(4); b[6][0]= new Image(); b[6][0].src =  "a71.gif";
                                   b[6][1]= new Image(); b[6][1].src = "a72.gif";
                                   b[6][2]= new Image(); b[6][2].src = "a73.gif";
                                   b[6][3]= new Image(); b[6][3].src = "a74.gif";
b[7] = new Array(4); b[7][0]= new Image(); b[7][0].src =  "a81.gif";
                                   b[7][1]= new Image(); b[7][1].src = "a82.gif";
                                   b[7][2]= new Image(); b[7][2].src = "a83.gif";
                                   b[7][3]= new Image(); b[7][3].src = "a84.gif";
b[8] = new Array(4); b[8][0]= new Image(); b[8][0].src =  "a91.gif";
                                   b[8][1]= new Image(); b[8][1].src = "a92.gif";
                                   b[8][2]= new Image(); b[8][2].src = "a93.gif";
                                   b[8][3]= new Image(); b[8][3].src = "a94.gif";

var thecard=new Array(9);
 thecard[0]= new Array(4); thecard[0][0]=1; thecard[0][1]=6; thecard[0][2]=7; thecard[0][3]=4;
 thecard[1]= new Array(4); thecard[1][0]=5; thecard[1][1]=2; thecard[1][2]=4; thecard[1][3]=8;
 thecard[2]= new Array(4); thecard[2][0]=8; thecard[2][1]=5; thecard[2][2]=2; thecard[2][3]=3;
 thecard[3]= new Array(4); thecard[3][0]=5; thecard[3][1]=1; thecard[3][2]=2; thecard[3][3]=6;
 thecard[4]= new Array(4); thecard[4][0]=3; thecard[4][1]=4; thecard[4][2]=7; thecard[4][3]=5;
 thecard[5]= new Array(4); thecard[5][0]=2; thecard[5][1]=8; thecard[5][2]=6; thecard[5][3]=4;
 thecard[6]= new Array(4); thecard[6][0]=7; thecard[6][1]=3; thecard[6][2]=1; thecard[6][3]=5;
 thecard[7]= new Array(4); thecard[7][0]=2; thecard[7][1]=1; thecard[7][2]=7; thecard[7][3]=6;
 thecard[8]= new Array(4); thecard[8][0]=5; thecard[8][1]=3; thecard[8][2]=2; thecard[8][3]=8;

function imageout()
{for(j=0; j<=8; j++){if(cardno[j]!=9){document.images[j].src=b[cardno[j]][posno[j]].src}
                                               else{document.images[j].src="a00.gif"};
                                               if(atpool[j]==1){document.images[j+9].src=b[j][0].src}
                                               else{document.images[j+9].src="a00.gif"}}}

function disoutput()
{var i=0;
 with (window.document.laytheA) { i=cardno[0]; if (i!=9){Dis0.value=i+1}else{Dis0.value=''};
                                                      i=cardno[1]; if (i!=9){Dis1.value=i+1}else{Dis1.value=''};
                                                      i=cardno[2]; if (i!=9){Dis2.value=i+1}else{Dis2.value=''};
                                                      i=cardno[3]; if (i!=9){Dis3.value=i+1}else{Dis3.value=''};
                                                      i=cardno[4]; if (i!=9){Dis4.value=i+1}else{Dis4.value=''};
                                                      i=cardno[5]; if (i!=9){Dis5.value=i+1}else{Dis5.value=''};
                                                      i=cardno[6]; if (i!=9){Dis6.value=i+1}else{Dis6.value=''};
                                                      i=cardno[7]; if (i!=9){Dis7.value=i+1}else{Dis7.value=''};
                                                      i=cardno[8]; if (i!=9){Dis8.value=i+1}else{Dis8.value=''};
                                                      i=posno[0]; if (i!=9){Diq0.value=i+1}else{Diq0.value=''};
                                                      i=posno[1]; if (i!=9){Diq1.value=i+1}else{Diq1.value=''};
                                                      i=posno[2]; if (i!=9){Diq2.value=i+1}else{Diq2.value=''};
                                                      i=posno[3]; if (i!=9){Diq3.value=i+1}else{Diq3.value=''};
                                                      i=posno[4]; if (i!=9){Diq4.value=i+1}else{Diq4.value=''};
                                                      i=posno[5]; if (i!=9){Diq5.value=i+1}else{Diq5.value=''};
                                                      i=posno[6]; if (i!=9){Diq6.value=i+1}else{Diq6.value=''};
                                                      i=posno[7]; if (i!=9){Diq7.value=i+1}else{Diq7.value=''};
                                                      i=posno[8]; if (i!=9){Diq8.value=i+1}else{Diq8.value=''}}}

function tellme(){imageout(); disoutput()}


function begin()
{if(autoon==0){field=0; ready=0; possible=0; cano=9; pono=9; veloc=250;
                         for (i=0; i<=8; i++){cardno[i]=9; atpool[i]=1; posno[i]=9};
                         tellme()}}

function turn(a)
{if(autoon==0){if (cardno[a] != 9){posno[a]=posno[a]+1; if(posno[a]==4){posno[a]=0};
                          tellme()}}}

function intothepool(a)
{if(autoon==0){if(cardno[a] !=9){atpool[cardno[a]]=1; cardno[a]=9;posno[a]=9; if(a<field){field=a}}; tellme()}}

function putto(a)
{if(autoon==0){if (atpool[a]==1){field=0; while(cardno[field]!=9){field=field+1};
                                                        cardno[field]=a; posno[field]=0; field=field+1; atpool[a]=0; tellme()}}}

function here(fieldno,Posnr)
{var i, j; i=cardno[fieldno]; j=Posnr-posno[fieldno]; if (j<0){j=j+4}; return thecard[i][j]}

function poolmin(k)
{if(k<8){for (i=k+1; i<=8; i++){if(atpool[i]==1){return i}}}; return 9}

function check()
{possible=0;
  if(cardno[field]==9){possible=1; cano=poolmin(-1); pono=0}
  else{if(posno[field]<3){cano=cardno[field]; pono=posno[field]+1; possible=1}
           else{cano=poolmin(cardno[field]); if(cano==9){pono=9}else{pono=0; possible=1}}}}

function trackback()
{var kr=0;
 for(i=field; i<=8; i++){kr=cardno[i]; if (kr !=9){atpool[kr]=1;
document.images[kr+9].src=b[kr][0].src;
document.images[i].src="a00.gif"; cardno[i]=9; posno[i]=9}}}

function laydown()
{trackback(); cardno[field]=cano; posno[field]=pono; atpool[cano]=0;
 document.images[field].src=b[cano][pono].src; document.images[cano+9].src="a00.gif"}

function cardfits()
{var p=0;
 if(field==0){p=1};
 if(field==1){if(here(0,3)+here(1,1)==9){p=1}};
 if(field==2){if(here(1,3)+here(2,1)==9){p=1}};
 if(field==3){if(here(0,2)+here(3,0)==9){p=1}};
 if(field==4){if(here(1,2)+here(4,0)==9){if(here(3,3)+here(4,1)==9){p=1}}};
 if(field==5){if(here(2,2)+here(5,0)==9){if(here(4,3)+here(5,1)==9){p=1}}};
 if(field==6){if(here(3,2)+here(6,0)==9){p=1}};
 if(field==7){if(here(4,2)+here(7,0)==9){if(here(6,3)+here(7,1)==9){p=1}}};
 if(field==8){if(here(5,2)+here(8,0)==9){if(here(7,3)+here(8,1)==9){p=1}}};
 return p}

function autoauto()
{if(ready==0 && autoon==1){check();
                                                 if(possible==1){laydown(); if(cardfits()==1){if(field==8){ready=1}
                                                                                                                        else{field=field+1}}}
                                                 else{if(field==0){ready=1}
                                                          else{trackback(); field=field-1}}};
 disoutput(); if (ready==0 && autoon==1){timer=window.setTimeout("autoauto()",veloc)}}

function auto()
{if(autoon==1){window.clearTimeout(timer)};
 autoon=1; window.document.laytheA.velo.value=veloc; autoauto()}

function autooff()
{if(autoon==1){window.clearTimeout(timer);
 autoon=0; ready=0; window.document.laytheA.velo.value=''; tellme()}}

function rset()
{if(autoon==1){window.clearTimeout(timer)};
 document.laytheA.reset(); autoon=0; begin()}

function notready()
{ready=0; if (autoon==1){autoauto()}}

function velocplus()
{if(autoon==1){window.clearTimeout(timer);
 veloc=Math.round(veloc*1.51); if (veloc>2000){veloc=2000};
 window.document.laytheA.velo.value=veloc; autoauto()}}

function velocminus()
{if(autoon==1){window.clearTimeout(timer);
 veloc=Math.floor(veloc/1.5); if (veloc<1){veloc=1};
 window.document.laytheA.velo.value=veloc; autoauto()}}

//--></script></head>
