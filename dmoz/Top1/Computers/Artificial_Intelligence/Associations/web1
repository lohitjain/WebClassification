====================<http://www.eccai.org/>====================
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<title>European Coordinating Committee for Artificial Intelligence (ECCAI)</title>
<meta NAME="description" CONTENT="ECCAI European Coordinating Committee for Artificial Intelligence Homepage">
<meta NAME="keywords" CONTENT="ECCAI European Coordinating Committee for Artificial Intelligence">
<script src="/jquery.js" type="text/javascript"></script>
<script type="text/javascript">

	$(document).ready(function(){
		$.ajax({
				type: "GET",
				url: "/config.xml",
				dataType: "xml",
				success: function(xml) {
					processConfig(xml);

				}
		});		 
	});

	var flag = new Array; 
	var country = new Array;
	var label = new Array;
	var linkimage = new Array;
	var linklabel = new Array;
	
	function processConfig(xml){
		$(xml).find("country").each(function (){
			country = country.concat($(this).attr("name")); 
			flag = flag.concat($(this).attr("image")); 
			label = label.concat($(this).attr("label")); 
			linkimage = linkimage.concat($(this).attr("linkimage"));
			linklabel = linklabel.concat($(this).attr("linklabel"));
		});
		
	var myDiv=document.getElementById("flag_div");
	doit(600,480,flag,country,label,linkimage,linklabel,90,myDiv); // 215
		
	}
	
	function doit(width,height,flag,country,label,linkimage,linklabel,a,txt){
		n=flag.length;
		xs=new Array(n);
		ys=new Array(n);
		centerx=width/2;
		centery=height/2;
		alpha = 2*Math.PI/n;
		R = 250;
		for (i=0; i<n; i++) {
  			xs[i] = centerx + R*Math.sin(i*alpha);
  			ys[i] = centery - R*Math.cos(i*alpha);
		
			//document.write(beta + ";");
			txt.innerHTML +='<div style="position:absolute; left:'+xs[i]+'; top:'+ys[i]+';text-align:center;width:80px;font-size:55%;color:#666666"><a href="'+linkimage[i]+'"><img style="border:solid 1px black" src="/img/flags/'+flag[i]+'" height="23" width="33"></a><br/>'+country[i]+ '<a href="'+linklabel[i]+'"><br/>'+label[i]+'</a></div>';
		}
		
		txt.innerHTML += '<div style="position:absolute; left:170; top:200;text-align:center;width:320px"><font size=2 color=#00007f>ECCAI, the European Coordinating Committee for Artificial Intelligence, was established in July 1982 as a representative body for the European Artificial Intelligence community. Its aim is to promote the study, research and application of Artificial Intelligence in Europe.</font></div>';
	}

</script>

