// Function to 'activate' images. Below, menu_top.gif is the default menu_top image.
function imgOn(imgName) {
        if (document.images) {
            document[imgName].src = eval(imgName + "on.src");
                document["holder"].src = eval(imgName + "ad.src");
        }
}

// Function to 'deactivate' images.
function imgOff(imgName) {
        if (document.images) {
            document[imgName].src = eval(imgName + "off.src");
                document["holder"].src = "capinfo.gif";
        }
        }
// Function to 'activate' menu item.
function menuOn(imgName) {
        if (document.images) {
            document[imgName].src = eval(imgName + "on.src");
            }
            }
//Function to 'deactivate' menu item.
function menuOff(imgName) {
        if (document.images) {
            document[imgName].src = eval(imgName + "off.src");
            }
}
