<script type="text/javascript">
function resizeIMG(wdth)
{
	var images = document.getElementsByTagName('img');
	for(var i=0; i< images.length; i++){
		if(images[i].offsetWidth > wdth)
		images[i].style.width= wdth +'px';
	}
}
</script>
