<style>
.worktime-on { padding:0 !important; margin:0 2px 1px 0 !important; float:right; cursor:pointer; cursor:hand; font-size:12px; color:green; border:none !important; }
.worktime-on span { padding:1px 3px 1px 4px; margin-right: 20px; font-size:11px; background:#111; color:#ccfd37; display:block; float:right; }
.worktime-off { padding:0 !important; margin:0 2px 1px 0 !important; float:right; cursor:pointer; cursor:hand; font-size:12px; color:red; border:none !important; }
.worktime-off span { padding:1px 3px 1px 4px; margin-right: 20px; font-size:11px; background:#111; color:#ffae00; display:block; float:right;}
