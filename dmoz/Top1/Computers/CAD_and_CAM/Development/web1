<script type="text/javascript">
function workTime(offs, from, upto){
	var day = new Date();
	
	var hour = day.getHours() + day.getTimezoneOffset()/60;
	if(offs) { hour = hour + offs; }
	if(hour <0 ) { hour += 24; }
	
	var minutes = day.getMinutes();
	if (minutes < 10) { minutes = "0" + minutes; }
	
	var title = "We are online. Call us!";
	var message = "online";
	var on_of = "-on";
	if (day.getDay() == 0 || day.getDay() == 6 || hour < from || hour >= upto) { on_of = "-off"; message = "offline"; title = "We are offline. Please use e-mail or call another destination.";}
	
	var ap = "am";
	if (hour > 11) { ap = "pm"; }
	if (hour > 12) { hour = hour - 12; }
	if (hour == 0) { hour = 12; }
	if (hour < 10) { hour = "0" + hour; }
	
	document.write("<div class='worktime" + on_of + "' title='" + title + "'><span><strong>" + hour + ":" + minutes + "</strong> " + ap + "</span></div>");
}
</script>

