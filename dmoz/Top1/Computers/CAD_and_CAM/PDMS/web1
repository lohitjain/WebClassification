    <script type="text/jscript">

        function ShowTab(targetUrl, title) {

            var tabStrip = $("#tabstrip").data("kendoTabStrip");

            // Get Tab Id
            var tabId = title.replace(/\s/g, '').replace(':', '');

            var innerText = title + " <img src='../../Content/images/close-12x12.png' style='padding-left: 10px;' onclick='DeleteTab(this);' /><span id='" + tabId + "'/>";
            //var innerText = title + "<span id='" + tabId + "'/>";

            var currentTab = $("#" + tabId).closest('.k-item');

            if (currentTab.length) {
                tabStrip.select(currentTab);
            }
            else {

                tabStrip.append({ text: innerText, encoded: false, contentUrl: targetUrl });
                tabStrip.select((tabStrip.tabGroup.children("li").length - 1));

                resizeTabs();
            }
        }

        function DeleteTab(imgObj) {

            var tabStrip = $("#tabstrip").data("kendoTabStrip");

            var deleteIndex = $(imgObj).closest("li").index();
            tabStrip.remove(deleteIndex);

            // Select next tab
            if (deleteIndex > 0) {
                var nextTab = tabStrip.select(deleteIndex);

                if (nextTab.length) {
                    tabStrip.select(deleteIndex);
                }
                else {
                    tabStrip.select(deleteIndex - 1);
                }
            }
            else {
                tabStrip.select(0);
            }
        }

    </script>
