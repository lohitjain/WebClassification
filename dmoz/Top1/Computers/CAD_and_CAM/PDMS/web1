        function resizeTabs() {
            var paneHeight = $("#tabstrip").closest(".k-pane").innerHeight();
            var tabsHeight = $("#tabstrip > .k-tabstrip-items").outerHeight();
            $("#tabstrip > div").height(paneHeight - tabsHeight - 14);
        }

        $("#vertical").kendoSplitter({
            orientation: "vertical",
            panes: [
            //{ collapsible: false, scrollable: false, resizable: false, size: "80px" },
        { collapsible: false, scrollable: false, resizable: false, size: "30px" },
        { collapsible: false, scrollable: false }
    ]
        });

        $("#horizontal").kendoSplitter({
            panes: [
        { collapsible: true, size: "270px" },
        { collapsible: false, scrollable: true }
    ],
            resize: resizeTabs
        });

        $("#tabstrip").kendoTabStrip();

        resizeTabs();

