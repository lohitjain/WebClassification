<div id="quick-search">
    <script>
        (function () {
            var cx = '004085358937423836264:vvapdvykqhw';
            var gcse = document.createElement('script');
            gcse.type = 'text/javascript';
            gcse.async = true;
            gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
                '//www.google.com/cse/cse.js?&cx=' + cx;
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(gcse, s);
        })();

        $(document).ready(function () {
            var searchStr = 'Search';

            function pollSiteSearch(count) {
                if ($('#gsc-i-id1')[0] === undefined && count < 100) {
                    setTimeout(function(){ pollSiteSearch(++count) }, 100);
                } else if (!$('#gsc-i-id1').is(":focus")){
                    $('#gsc-i-id1').attr("value", searchStr);
                    $('#gsc-i-id1').attr("title", searchStr);
                }
            } pollSiteSearch(0);

            $('#quick-search').on('blur', '#gsc-i-id1', function () {
                if ($('#gsc-i-id1').attr("value") == "") {
                    $('#gsc-i-id1').attr("value", searchStr);
                }
            });
            $('#quick-search').on('focus', '#gsc-i-id1', function () {
                if ($('#gsc-i-id1').attr("value") == searchStr) {
                    $('#gsc-i-id1').attr("value", "");
                }
            });
            $('#quick-search').on('DOMSubtreeModified', function (e) {
                if ($('.gsc-results-wrapper-overlay').css("visibility") == "visible") {
                    $('input').blur();
                }
            });
            
        });
    </script>
    <style>
        #header #quick-search .gsc-above-wrapper-area-container {
            background: url(/~/media/4ACB2F8A45D14499AAEC9FB7DB903CB9.ashx?lang=en) no-repeat;
            background-position: 50% 7px;
            height: 55px;
        }
    </style>
    <gcse:search linktarget="_parent"></gcse:search>
</div><!--end: quick-search -->
                
