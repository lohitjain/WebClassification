              if ((!immersion_adcode && !immersionCompliment)  || (immersion_adcode && immersionCompliment)) {
                document.write('<iframe src="http://ad.doubleclick.net/N4140/adi/ostg.sourceforge/cons_none_p84_medrec;pg=%2Fprojects;psrch=0;logged_in=0;tpc=dapple;tpc=c;tpc=emulators;tpc=system;aud=developers;aud=endusers;dc_ref=http%3A%2F%2Fsourceforge.net%2Fprojects%2Fdapple%2F;tile=6;sz=300x250;u=e%3Ab02a4ec4-9e75-11e3-870b-0200ac1d1d97;ord=4509614725348760?" width="300" height="250" marginwidth="0" marginheight="0" hspace="0" vspace="0" frameborder="0" scrolling="no" seamless></iframe>');
              }
            })();
       </script>
        </section>
        
