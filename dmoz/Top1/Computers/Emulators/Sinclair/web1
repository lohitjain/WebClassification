====================<http://www.vavasour.ca/jeff/ts1000/>====================
<html>

<head>
<meta http-equiv="Content-Type"
content="text/html; charset=iso-8859-1">
<meta name="GENERATOR" content="Microsoft FrontPage Express 2.0">
<title>Timex/Sinclair 1000 Emulator</title>
</head>

<body bgcolor="#000000" text="#FFFFFF" link="#C0C0FF"
vlink="#00FFFF" alink="#FFFFFF" onload="load_program()"
onunload="exit_program()">

<p align="center"><font color="#C0C0FF" size="5"><strong>Timex/Sinclair
1000 Emulation in a Browser Window<br>
</strong></font><font size="2">Return to </font><a
href="http://www.vavasour.ca"><font size="2">Vavasour family page</font></a><font
size="2"> : </font><a href="../index.html"><font size="2">Jeff's
home page</font></a><font size="2"> : </font><a
href="../trs80.html#ts1000"><font size="2">Jeff's emulation page</font></a></p>

<hr>

<p align="center">This is an emulation of the Timex/Sinclair 1000
(a.k.a. Sinclair ZX-81) home computer. If the emulator does not
respond when you type, try clicking on the emulator's window. For
programs and samples, visit the <a href="library.html"><strong>T/S
1000 Software Library</strong></a>.<script language="JavaScript"><!--
function load_window() {
	document.emulator.PauseSimulation('true');
	saver = window.open('upload.html', 'tsfile',
		'toolbar=no,location=no,directories=no,status=no,resizable=yes,scrollbars=yes,width=600,height=415');
	document.emulator.PauseSimulation('false');
	saver.focus();
}

function save_window() {
	document.emulator.PauseSimulation('true');
	document.control.listing.value = document.emulator.ProgramText("program");
	document.emulator.PauseSimulation('false');
	document.control.submit();
}

function submit_window() {
	window.open('submit.html', '_blank');
}

var suppresserror = false;

function errorhandler() {
	return suppresserror;
}

window.onerror = errorhandler;
	
function load_program() {
	suppresserror = true;
	if (opener && !opener.closed && opener.document.program && opener.document.program.listing) {
		document.emulator.SetProgram(opener.document.program.listing.value);
		if (opener.document.program.listing.value.indexOf("restart") == 0) opener.close();
	}
	if (opener && !opener.closed && opener.document.viewer) {
		if (opener.document.viewer.PreviewAutorun() == 'true') document.emulator.EnableAutoRun();
		document.emulator.SetProgram(opener.document.viewer.PreviewProgram());
	}
	suppresserror = false;
}

function show_instructions() {
	instpage = window.open('', 'Instructions',
		'toolbar=no,location=no,directories=no,status=no,resizable=yes,scrollbars=yes,width=400,height=300'); 
	instpage.document.open();
	instpage.document.write('<head><title>Instructions for T/S 1000 library program</title></head>');
	instpage.document.write('<body><p><strong>Instructions:</strong></p>');
	instpage.document.write(instructions);
	instpage.document.write('</body>');
	instpage.document.close();
}

if (top != self) {
	top.location.href = self.location.href;
}

suppresserror = true;
if (opener && !opener.closed && opener.document.viewer) {
	instructions = opener.document.viewer.PreviewInstructions();
	if (instructions != '') {
		document.write('</p><p align="center"><a href="javascript:show_instructions()"><font ');
		document.write('color="#E0E0FF" size="4">View instructions for this program</font></a>');
	}
}
suppresserror = false;

function exit_program() {
	document.emulator.PauseSimulation('true');
	SetCookie("checkbox", document.control.confirm.checked, exp);
	if (document.control.confirm.checked) {
		if (!confirm('Discard any program loaded in the emulator?')) {
			reopen = window.open('');
			reopen.document.open();
			reopen.document.write(document.emulator.ProgramHTML());
			reopen.document.close();
		}
	}
}

var expDays = 3000;
var exp = new Date(); 
exp.setTime(exp.getTime() + (expDays*24*60*60*1000));

function getCookieVal (offset) {  
	var endstr = document.cookie.indexOf (";", offset);  
	if (endstr == -1)    
		endstr = document.cookie.length;  
	return unescape(document.cookie.substring(offset, endstr));
}

function GetCookie(name) {  
	var arg = name + "=";  
	var alen = arg.length;  
	var clen = document.cookie.length;  
	var i = 0;  
	while (i < clen) {    
		var j = i + alen;    
		if (document.cookie.substring(i, j) == arg)      
			return getCookieVal (j);    
		i = document.cookie.indexOf(" ", i) + 1;    
		if (i == 0) break;   
	}  
	return null;
}

function SetCookie(name, value) {  
	var argv = SetCookie.arguments;  
	var argc = SetCookie.arguments.length;  
	var expires = (argc > 2) ? argv[2] : null;  
	var path = (argc > 3) ? argv[3] : null;  
	var domain = (argc > 4) ? argv[4] : null;  
	var secure = (argc > 5) ? argv[5] : false;  
	document.cookie = name + "=" + escape (value) + 
	((expires == null) ? "" : ("; expires=" + expires.toGMTString())) + 
	((path == null) ? "" : ("; path=" + path)) +  
	((domain == null) ? "" : ("; domain=" + domain)) +    
	((secure == true) ? "; secure" : "");
}
// --></script></p>

