var hash = location.hash;
if (hash.indexOf("#") === 0) {
hash = hash.substring(1);
}
var hashParameters = hash.split("&");
var newHashParameters = [];
var newHash = "";
for (var index = 0; index < hashParameters.length; index++) { 
var param = hashParameters[index];
if (!param.startsWith('__utm')) {
newHashParameters.push(param);
}
}
if(newHashParameters.length > 0) {
newHash = "#" + newHashParameters.join("&");
}
window.history.replaceState({}, "", document.location.href.split("#")[0] + newHash);
}
});
