  <script type="text/javascript">

          var url = "http://www.epoka.com/customer/account/login/";
      var label = "Log in";
      var isLogged = false;
    
    jQuery(function ($) {
      var link = $('div.block-header-top-line li.item-login a');
      if (link.length)
      {
        link
          .text(label)
          .attr('href', url);
      }
      if (isLogged)
      {
        $('div.block-header-top-line li.item-my-account').show();
      }
    });
  </script>


