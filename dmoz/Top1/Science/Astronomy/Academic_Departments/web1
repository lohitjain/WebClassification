<!--
// please keep these lines on when you copy the source
// made by: Nicolas - http://www.javascript-page.com

var currentdate = 0
var core = 0

function StringArray (n) {
  this.length = n;
  for (var i =1; i <= n; i++) {
    this[i] = ' '

  }
}

image = new StringArray(9)
image[0] = 'img/backwards.jpg'
image[1] = 'img/n5090b.gif'
image[2] = 'img/acsvla0313x.jpg'
image[3] = 'img/am1316color.jpg'
