image[6] = 'img/rnew3081b.gif'
image[7] = 'img/vlahst.gif'
image[8] = 'img/stephan_big.jpg'


var ran = 60/image.length

function ranimage() {
  currentdate = new Date()
  core = currentdate.getSeconds()
  core = Math.floor(core/ran)
    return(image[core])
}

document.write("<img src='" +ranimage()+ "' width=300>")

