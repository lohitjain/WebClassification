	$(document).ready(function() {
		
		$('.section').click(function() {
			$('.subsection').slideUp('normal');
			if($(this).next().is(':hidden') == true) {			
				$(this).next().slideDown('normal');
				$(this).children().removeClass('hidden');
			} 	  
		});
		$('.subsection').hide();				
		
	});
</script>
