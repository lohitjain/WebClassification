$(document).ready(function() {
	$('#searchbox').focus(function() {
		$('#searchbtn2').slideDown('fast');		
	});
	$('#searchbox').blur(function() {
		if ($('#searchbox').val() != "") {
			//LEAVE OPEN
		} else {
			$('#searchbtn2').slideUp('fast');
		}
	});
	$('#searchbtn2').hide();		
	$('#resourcestab a').click(function() {
		if ($('#resourcesinfo').is(':hidden')) {
			$('#resourcesinfo').slideDown('fast');	
		} else {
			$('#resourcesinfo').slideUp('fast');
		}
	});
	$('#resourcesinfo').hide();
	$('#ie7note').hide();	
	
	$("a[rel^='prettyPhoto']").prettyPhoto({overlay_gallery: false});
	
	//FIX FOR NAVIGATIONS IN IE7
	if ($.browser.msie && $.browser.version == 7)
	{
		$("#bodycolumnINT").wrap("<td id='bodycolumnINT' />");
		$("#interiornav").wrap("<td id='interiornav' />");
		$("#pagecopyINT").wrap("<table bgcolor=white><tr></tr></table>");
		
		$("#eventscolumn").wrap("<td width=300 />");
		$("#newscolumn").wrap("<td width=300 />");
		$("#misccolumn").wrap("<td width=200 />");
		$("#pagecopy").wrap("<table><tr></tr></table>");
		
		$('#ie7note').slideDown('fast');
	}
	
	//ADAPTIVE
	$('#menudropkey').click(function() {
		if ($('#navbar').is(':hidden')) {
			$('#navbar').slideDown('fast');	
		} else {
			$('#navbar').slideUp('fast');
		}
		if ($('#secondarynav').is(':hidden')) {
			$('#secondarynav').slideDown('fast');	
		} else {
			$('#secondarynav').slideUp('fast');
		}
	});

	
});
</script>
