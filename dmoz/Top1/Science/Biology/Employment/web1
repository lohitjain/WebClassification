<script type="text/javascript">

Cufon.replace('.col_left h2, h4, .col_right h2, #footer h2', { fontFamily: 'TitilliumText22L-Light', textShadow: '1px 1px 4px #000', hover: true }); 
Cufon.replace('.col_left p, .are_you, label, .col_left ul', { fontFamily: 'TitilliumText22L-Regular', textShadow: '1px 1px 4px #000', hover: true }); 
Cufon.replace('.col_left h3, .col_left b, .enter, .col_left strong', { fontFamily: 'TitilliumText22L-Bold', textShadow: '1px 1px 4px #000', hover: true }); 
Cufon.replace('.home_text h1 ', { fontFamily: 'petitaLight', textShadow: '1px 1px 10px #000', hover: true }); 
Cufon.replace('.home_text h1 strong, .latest_box h2', { fontFamily: 'Harabara Bold', textShadow: '1px 1px 10px #000', hover: true }); 

var windw = this;

$.fn.followTo = function ( pos ) {
    var $this = this,
        $window = $(windw);
    
    $window.scroll(function(e){
        if ($window.scrollTop() > pos) {
            $this.css({
                position: 'absolute',
                top: pos
            });
        } else {
            $this.css({
                position: 'fixed',
                top: 0
            });
        }
    });
};

$(document).ready(function() {

/*if (navigator.userAgent.match(/Android/i) ||
   navigator.userAgent.match(/webOS/i) ||
   navigator.userAgent.match(/iPhone/i) ||
   navigator.userAgent.match(/iPad/i) ||
   navigator.userAgent.match(/iPod/i) ||
   navigator.userAgent.match(/BlackBerry/) || 
   navigator.userAgent.match(/Windows Phone/i) || 
   navigator.userAgent.match(/ZuneWP7/i)
   ) {

 } else {
  
	$('body').videoBG({
		position:"fixed",
		zIndex:-1,
		mp4:'/video/background.mp4',
		ogv:'/video/background.ogv',
		webm:'/video/background.webm',
		poster:'/video/background.jpg'
	});
   
}*/

	var divh = $('.container_left_sub').height();
	
	$('.col_right').followTo(divh - 765);

	$('form select').customStyle();
	$("input[title]").ghost();
	
	$("form").submit(function() {
		$(this).find("input.ghosted").val("");
	});
	
	$("textarea[title]").ghost();
	
	$(".advanced_search").hide();
	$(".advanced").click(function() {
		$(".advanced_search").slideToggle();
	});
		
	
	$(".home_text h1").delay(400).fadeIn({duration: 2000, easing: "easeInSine"});
	
	$("#header").hoverIntent( makeTall, makeShort )
	
	function makeTall(){
		$('#menu_tab').hide();
		$(this).animate({"height": "248"}, {duration: 400, easing: "easeInQuad"});
	}

	function makeShort(){
		$('#menu_tab').show();
		$(this).animate({"height": "75"}, {duration: 400, easing: "easeOutQuad"});
	}
	
	var rand = Math.floor(Math.random()*10);
	$('html').css({ "background-image" : "url(images/main_bg"+rand+".jpg)" });
	$('#bg_img').attr("src", "images/main_bg"+rand+".jpg");
	
	//$('html').css({ "background-image" : "url(/video/background.jpg)" });
	//$('#bg_img').attr("src", "/video/background.jpg");
	
	$('.latest_newsbox').vTicker({ 
		speed: 500,
		pause: 6000,
		animation: 'fade',
		mousePause: false,
		showItems: 1,
		direction: 'down'
	});
	
	$('ul#logo_scroll').innerfade({
		speed: 1000,
		timeout: 5000,
		type: 'sequence',
		containerheight: '220px'
	});
		
	$('a[rel*=facebox]').facebox();
	
	if ($.browser.msie && $.browser.version < 9) $('.subselect_role, .subselect_job')
    .bind('focus mouseover', function() { $(this).addClass('expand').removeClass('clicked'); })
    .bind('click', function() { $(this).toggleClass('clicked'); })
    .bind('mouseout', function() { if (!$(this).hasClass('clicked')) { $(this).removeClass('expand'); }})
    .bind('blur', function() { $(this).removeClass('expand clicked'); });
    
  
	
});

</script>
