<script type="text/javascript">
    rewritethelinks('a');
    rewritethelinks('area');
    function rewritethelinks(typeoflink) {
        var links = document.getElementsByTagName(typeoflink);
        var len = links.length;
        if (len > 0) {
            for (var i = 0; i < len; i++) {
                var fdfdfdfd = links[i].href
                if (fdfdfdfd.length > 5) {
                    if (fdfdfdfd.substr(-4) == '.pdf' && document.URL.indexOf("Policy-and-Government-Affairs") != -1) {
                        links[i].href = 'http://www.faseb.org/pdfviewer.aspx?loadthis=' + encodeURIComponent(fdfdfdfd);
                        links[i].target = "_self";
                    }
                    fdfdfdfd = '';
                }
            }
        }
    }
    </script>
