				<script>
					function toggleTabs(tab) {
						var showTab = tab;
						var hideTab = tab == 'js' ? 'emp' : 'js';

						$(hideTab + '_tab_content').hide();
						$('tab_content_box').removeClassName(hideTab + '_tab_box');
						$('jt_' + hideTab + '_tab').removeClassName('jt_active');

						$(showTab + '_tab_content').show();
						$('tab_content_box').addClassName(showTab + '_tab_box');
						$('jt_' + showTab + '_tab').addClassName('jt_active');
					}
				</script>
				<ul class="jt_tabs">
					<li id="jt_js_tab"  onclick="toggleTabs('js'); return false;">Job Seekers</li>
					<li id="jt_emp_tab" onclick="toggleTabs('emp'); return false;" class="jt_active">Employers</li>
				</ul>
			
			
			<div id="tab_content_box" class="emp_tab_box">
				
