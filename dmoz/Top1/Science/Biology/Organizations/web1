	<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"></script>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/jquery-ui.min.js"></script>
<script src="js/jquery.paginate.js" type="text/javascript"></script>
	<script type="text/javascript">
		
		function ShowHoverMoreButton(Elem)
		{
			Elem.src = "img/MoreButtonHover.png";
		}
		
		function ShowMoreButton(Elem)
		{
			Elem.src = "img/MoreButton.png";
		}
		
		function ShowFooterHoverMoreButton(Elem)
		{
			Elem.src = "img/FooterMoreHover.png";
		}
		
		function ShowFooterMoreButton(Elem)
		{
			Elem.src = "img/FooterMore.png";
		}
		
		$(document).ready(function(){
  			$("ul li ul li").hover(function(){
    			$(this).children(':first').children(':first').children(':first').css("display","block");
   			 },function(){
    			$(this).children(':first').children(':first').children(':first').css("display","none");
  			});
		});
		$(document).ready(function(){
  			$(".images img").hover(function(){
				$gdna = $(this).attr("src");
				switch($gdna)
				{
					case 'Slider/images/image11.jpg':
						$(this).attr("src","Slider/images/image1.jpg");
						break;
					case 'Slider/images/image22.jpg':
						$(this).attr("src","Slider/images/image2.jpg");
						break;
					case 'Slider/images/image33.jpg':
						$(this).attr("src","Slider/images/image3.jpg");
						break;
				}
   			 },function(){
    			$(this).attr("src",$gdna);
  			});
		});
   </script>
