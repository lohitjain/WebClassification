<script type="text/javascript">

    function IssueSearch() {
        var query = $.trim($('#searchText').val());

        if (query != "") {
            window.location = "/Search/SearchResults.aspx?q=" + query;
        }
        else {
            alert('Search query cannot be blank.');
            return false;
        }

        return false;
    }

    document.write('');

</script>

