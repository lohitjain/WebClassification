	<div id="lxT1464"><script language=javascript> 
		function checkEnter(e){ //e is event object passed from function invocation 
		var characterCode //literal character code will be stored in this variable 
		
		if(e && e.which){ //if which property of event object is supported (NN4) 
		e = e; 
		characterCode = e.which; //character code is contained in NN4's which property?
		} 
		else{ 
		e = event; 
		characterCode = e.keyCode; //character code is contained in IE's keyCode property 
		} 
		
		if(characterCode == 13){ //if generated character code is equal to ascii 13 (if enter key) 
		//Action Goes Here 
		(document.getElementById('Email').value=='Your email address' || document.getElementById('Email').value=='')?alert('Your email address'):SignUp();
		return false; 
		} 
		else{ 
		return true; 
		} 
		
		} 
function SignUp()
{
	if ((typeof(Page_SmartValidate) == 'function') && Page_SmartValidate())
	{
		
		myRef = window.open('http://capwiz.com/faseb/mlm/signup/?email='+document.getElementById("Email").value,'Signupwin');

		//__doPostBack('dnn$ctr1464$dnn','SignUp,SignUp,,M');
		
		
	}
	else 
	{
		Page_IsSmartExecuted=false;

		return false;
	}
	
}
</script> 

