		var Lasti = '<font face="Calibri">Last</font>';
		var Firsti = '<font face="Calibri">First</font>';
		
		$(function() {
			$("#demo5").paginate({
				count 		: 2,
				start 		: 1,
				display     : 5,
				border					: true,
				border_color			: '#fff',				
				text_color  			: '#5A5A5A',
				background_color    	: '#fff',	
				border_hover_color		: '#ccc',
				text_hover_color  		: '#fff',
				background_hover_color	: '#5A5A5A', 
				images					: false,
				mouse					: 'press',
				onChange     			: function(page){
											$('._current','#paginationdemo').removeClass('_current').hide();
											$('#p'+page).addClass('_current').show();
										  }
			});
		});
		</script>

