    <script type="text/javascript">
        $(document).ready(function($) {
            $(".tooltip").add("a[title]").tooltip({
                bodyHandler: function() {
                    var text = this.tooltipText;
                    text = text.replace(/\n/g, '<br/>');
                    return text;
                }
            });
            $(".jqCollapse").hide();
            setTimeout(function() {
                var $el = $(".jqCollapse");
                $el.show(800);
            }, 1000);

            $('img').load(function(){
                $(this).data('loaded', true);
            });
            $.extend($.expr[':'], {
                loaded: function(el) {
                    return $(el).data('loaded');
                }
            });
            $.fn.whenloaded = function(cb) {
                if ($(this).length > 1) {
                    $(this).each(function() {
                        $(this).whenloaded(cb);
                    });
                } else {
                    if ($(this).filter(':loaded')) {
                        cb.call(this);
                    } else {
                        $(this).load(cb);
                    }
                }
            };
            $('[placeholder]').each(function(){
                var el = $(this);
                var placeholder = el.attr('placeholder');
                el.val(placeholder)
                el.data('placeholder-set', true);
                el.focus(function() {
                    if (el.data('placeholder-set')) {
                        el.val('');
                    }
                });
                el.blur(function() {
                    if (el.val() === '') {
                        el.val(placeholder)
                        el.data('placeholder-set', true);
                    } else {
                        el.data('placeholder-set', false);
                    }
                });
            });
        });
    </script>
