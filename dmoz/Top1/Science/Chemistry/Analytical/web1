function change_image(id) {
	var src;
	switch (id) {
		case ('image1') :{ 
		src='../images/bb_about.gif';
		break;
		}
		case ('image2') :{ 
		src='../images/bb_proprties.gif';
		break;
		}
		case ('image3') :{ 
		src='../images/bb_sign.gif';
		break;
		}
		case ('image4') :{ 
		src='../images/bb_nomenclature.gif';
		break;
		}
		case ('image5') :{ 
		src='../images/bb_database.gif' ;
		break;
		}
		case ('image6') :{ 
		src='../images/bb_primer.gif' ;
		break;
		}
		case ('image7') :{ 
		src='../images/bb_biblio.gif';
		break;
		}
		case ('image8') :{ 
		src='../images/bb_news.gif' ;
		break;
		}
		case ('image9') :{ 
		src='../images/bb_links.gif' ;
		break;
		}
	} //fin du switch
	document.getElementById(id).src=src;		
}
//-->
</script>

