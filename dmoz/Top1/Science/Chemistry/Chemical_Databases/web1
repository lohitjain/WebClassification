    <script type="text/javascript">
        $(function () {
            var currentPath = window.location.pathname;
            if (currentPath == "/") {
                $("nav #home").addClass("current");
            }
            else if (currentPath.indexOf("/chemicals/") == 0) {
                $("nav #chemicals").addClass("current");
            }
            else if (currentPath.indexOf("/energy/") == 0) {
                $("nav #energy").addClass("current");
            }
            else if (currentPath.indexOf("/fertilizers/") == 0) {
                $("nav #fertilizers").addClass("current");
            }
            else if (currentPath.indexOf("/services/") == 0) {
                $("nav #services").addClass("current");
            }
        });
    </script>

        <script type="text/javascript">
            $(document).ready(function () {
                IcisRubySearch.initialiseQuickSearchAutoSuggestControl(false, false);
            });
        </script>
    
    <script type="text/javascript">
        $(document).ready(function () {

            var defaultChannel = ''.toLowerCase();

            var markUp = ["<select id='productfinder-select'>"], $a;
            markUp.push("<option value='' selected='selected'>Select a commodity ...</option>");
            $("#ProductFinder .results").find("ul.expander > li> span > a").each(function () {
                $a = $(this);
                var ch = "";
                var href = $a.attr("href").toLowerCase();

                if (href.indexOf("/chemicals/") >= 0) {
                    ch = "chemicals";
                } else if (href.indexOf("/energy/") >= 0) {
                    ch = "energy";
                } else if (href.indexOf("/fertilizers/") >= 0) {
                    ch = "fertilizers";
                }

                if (defaultChannel !== '') {
                    if (defaultChannel === ch)
                        markUp.push("<option class='shown' channel='" + ch + "' value='" + $a.attr("href") + "'>" + $a.text() + "</option>");
                    else
                        markUp.push("<option disabled class='' channel='" + ch + "' value='" + $a.attr("href") + "'>" + $a.text() + "</option>");
                } else {
                    markUp.push("<option class='shown' channel='" + ch + "' value='" + $a.attr("href") + "'>" + $a.text() + "</option>");
                }
            });

            markUp.push("</select>");

            $("#dd-container #dd-count").after(markUp.join(''));
            updateCount();

            $("#productfinder-select").change(function () {
                window.location = $(this).val();
            });

            $("#mob-filter > label > input[type=checkbox]").change(function () {
                var ch = $(this).attr("name");
                var options = $("#productfinder-select").find("option[channel='" + ch + "']");
                if (this.checked) {
                    $(options).each(function () {
                        $(this).show();
                        if (!$(this).hasClass("shown")) {
                            $(this).addClass("shown");
                            $(this).removeAttr("disabled");
                        }
                    });
                } else {
                    $(options).each(function () {
                        $(this).hide();
                        $(this).removeClass("shown");
                        $(this).attr({ disabled: 'disabled' });
                    });
                }

                updateCount();
            });
        });

        function updateCount() {
            $("#dd-container #dd-count").text("Found " + $("#dd-container select option.shown").size() + " commodities.");
        }
    </script>


    
