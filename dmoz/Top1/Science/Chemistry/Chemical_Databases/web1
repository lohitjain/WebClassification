    $(window).load(function () {

        var leftContentHeight = $('#contentSection .content_left').innerHeight();
        var rightContentHeight = $('#contentSection .content_right').innerHeight();

        var leftContentHeight = $('.double_column_Redesign .double_column_Redesign_left').innerHeight();
        var rightContentHeight = $('.double_column_Redesign .double_column_Redesign_right').innerHeight();

        if (leftContentHeight < rightContentHeight) {

            var newHeightVal = rightContentHeight - leftContentHeight;

            if (leftContentHeight > rightContentHeight) {
                $('.double_column_Redesign_left').css('height', leftContentHeight + newHeightVal);
                $('.double_column_Redesign_right').css('height', leftContentHeight + newHeightVal);

            }
            else if (rightContentHeight > leftContentHeight) {
                $('.double_column_Redesign_left').css('height', rightContentHeight + newHeightVal);
                $('.double_column_Redesign_right').css('height', rightContentHeight + newHeightVal);

            }
        } else {
            if (leftContentHeight > rightContentHeight) {
                $('.double_column_Redesign_left').css('height', leftContentHeight);
                $('.double_column_Redesign_right').css('height', leftContentHeight);

            }
            else if (rightContentHeight > leftContentHeight) {
                $('.double_column_Redesign_left').css('height', rightContentHeight);
                $('.double_column_Redesign_right').css('height', rightContentHeight);

            }
        }

    });



//]]>
