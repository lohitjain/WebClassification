  <script type="text/javascript" language="javascript" >
           
            function URLOpen()
            {
             var ctrl = document.getElementById('ctl11_siteHeader_drpquickLink');
             var selectedindex = ctrl.selectedIndex;
             var URLlink = ctrl.options[selectedindex].value;
                if (URLlink != "")
                    {
                    window.open(URLlink);
                    }
            }

            function SearchURLOpen()
            {
               
             var ctrl = document.getElementById('ctl11_siteHeader_txtSearch');
           
             var URLlink = "http://sitesearch.cas.org/search?q="+ctrl.value +"&btnG=Go&entqr=0&ud=1&sort=date%3AD%3AL%3Ad1&output=xml_no_dtd&oe=UTF-8&ie=UTF-8&client=default_frontend&proxystylesheet=default_frontend&site=default_collection" ;
                if (URLlink != "")
                    {
                    window.open(URLlink,'_self');
                    }
            }

 	function clickButton(e){   
  
         var evt = e ? e : window.event;  
          if (evt.keyCode == 13){   
 	       SearchURLOpen();
               return false;   

        }   

      }  
  </script>
