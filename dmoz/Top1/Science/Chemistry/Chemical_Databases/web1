  <script type="text/javascript">

  function isCompatibleBrowser(navigatorAppName,navigatorAppVersion) {
  // This returns true for:
  //    MSIE 6.0
  //    Firefox 1.x on windows and mac
  //    Safari 1.2 on mac (which seems to work with our stuff as of 11/17/05)
  // @TODO: look for a less hacky way to do this stuff.
  var primaryAppVersion = parseInt(navigatorAppVersion)
  var msieAppVersionIdx = navigatorAppVersion.indexOf("MSIE ")
  if (msieAppVersionIdx > -1)
    msieAppVersion = navigatorAppVersion.substring(msieAppVersionIdx+5,msieAppVersionIdx+6)
  else
    msieAppVersion = 0
  var appName = navigatorAppName
  
  if ((appName.indexOf("Microsoft Internet Explorer") > -1) && 
      ((primaryAppVersion >= 5) || (msieAppVersion >= 6)))
    return 1
  
  if ((appName.indexOf("Netscape") > -1) && 
      (primaryAppVersion >= 5))
    return 1

  return 0
  }
    if (! isCompatibleBrowser(navigator.appName,navigator.appVersion))
      alert("The ChemBank website officially supports the following browsers:" + "\n" +
        "  Mozilla Firefox 1.0.4 and higher" + "\n" +
        "  Safari 1.3.1 and higher" + "\n" +
        "  Internet Explorer 6.0 and higher" + "\n" +
        "All other browsers may experience inconsistencies or problems." + "\n" +
        "Please use one of the supported browsers.  Thank you.")
  </script>

