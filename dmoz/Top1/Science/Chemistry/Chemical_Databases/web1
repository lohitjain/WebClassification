<!-- outcomment the script for old browsers

function hiliteImage(imageName,hiliteImageName) {
	if (document.images) {
		document[imageName].src = eval(hiliteImageName+".src")
		}
	}

function hiliteOff(imageName) {
	if (document.images) {
		document[imageName].src = eval(imageName+".src")
		}
	}

if (document.images) {
	header2a = new Image(320,118)
	header2a.src = "header2a.gif"
	header2b = new Image(320,118)
	header2b.src = "header2b.gif"
