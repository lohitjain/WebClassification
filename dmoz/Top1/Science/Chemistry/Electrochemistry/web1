}
var whichImage = Math.round(Math.random()*(p-1));

function showImage(){
if(whichImage==0){
document.write('<a href ="/oa/"><img src="'+theImages[whichImage]+'" border=0 width=366 height=128></a>');
}
else if(whichImage==1){
document.write('<a href ="http://ecsdl.org/" target="_blank"><img src="'+theImages[whichImage]+'" border=0 width=366 height=128></a>');
}

}

