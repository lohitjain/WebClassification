    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta http-equiv="refresh" content="84400">
    <link rel="stylesheet" type="text/css" href="/stylesheets/blue_white.css" title="stylesheet">
<!-- 
I had to replace the /includes/header.shtml with this and header1.shtml so that 
I could set the correct title for the page the user is on.
 -->
      <TITLE>NMR Acronyms, Abbreviations, and Terms</TITLE>

<!-- 
I had to replace the /includes/header.shtml with this and header0.shtml so that 
I could set the correct title for the page the user is on.
 -->

    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script type="text/javascript">
        $( document ).ready( function() {

/* atach to jquery to make it global */
/* because jQuery.text() returns "syntax error" on anything other than [A-Za-z]. or over 3 lines. or something. */
            jQuery.strip_tags = function strip_tags( input ) {
                var tags = /<\/?([a-z][a-z0-9]*)\b[^>]*>/gi,
                commentsAndPhpTags = /<!--[\s\S]*?-->|<\?(?:php)?[\s\S]*?\?>/gi;
                return input.replace( commentsAndPhpTags, "" ).replace( tags, "" );
            }

/* .get() into a var & make that not asynchronous */
/* atach to jquery to make it global */
            jQuery.fetch_url = function fetch_url( url ) {
                var rc = "ajax error";
                $.ajax( {
                    "async" : false,
                    "global" : false,
                    "url" : url,
                    "success" : function( data ) { rc = data; },
                    "dataType" : "html"
                } );
                return rc;
            }

/* highlight table row -- because :hover only highlights the cell in firefox, not the whole row */
            $( ".hiliteonhover" ).hover( function() {
                if( ! $( this ).hasClass( "highlight_hover" ) )
                    $( this ).addClass( "highlight_hover" );
                }, function() {
                    if( $( this ).hasClass( "highlight_hover" ) )
                        $( this ).removeClass( "highlight_hover" );
            } );

/* accordion menus */
            $( ".expandingBody" ).hide();

            $( ".expandingHeader" ).bind( "click", function() {
                if( $( this ).hasClass( "Selected" ) ) {
                    $( this ).removeClass( "Selected" );
                    $( this ).addClass( "NotSelected" );
                }
                else {
                    $( this ).removeClass( "NotSelected" );
                    $( this ).addClass( "Selected" );
                }
                $( this ).next( ".expandingBody" ).toggle();
            } );

/* random "featured molecule" image for the sidebar */
            var rand_molecule = $.fetch_url( "/featuredSys/random.php" );
            $( "#random_molecule" ).html( rand_molecule );
        } );
    </script>

</head>

