      jQuery(document).ready(function(){
		$("#ethform").validate({
			highlight: function(element, errorClass){
			  $(element).closest('div.rowElem').addClass(errorClass).find('label:first').addClass(errorClass);
			},
			unhighlight: function(element, errorclass){
			  $(element).closest('div.rowElem').removeClass('error').find('label:first').removeClass('error');
			}
		});
		$('#accordion h3').click(function(){
		  if($(this).hasClass('show')) {
			$(this).removeClass('show');
			$(this).addClass('hide');
			$(this).next().hide('slow')
		  } else {
			$(this).removeClass('hide');
			$(this).addClass('show');
			$(this).next().show('slow');
		  }
                  alert($(this + ' ul.more_facet_topic').attr('id'));
                  return false;
		}).next().show();
		
