<script>

var show_more_options = false;

function show_create_list_form() {

	if(show_more_options) {
		
		show_more_options = false;
		$('#create-list-container-yo').css('display', 'inline');
	}
	else {
		
		show_more_options = true;
		$('#create-list-container-yo').css('display', 'none');
	}
}

function more_options() {
	
	/*if(show_more_options) {
		
		$('#createList_old').css('display', 'none');
		show_more_options = false;
	}
	else {
		
		$('#createList_old').css('display', 'inline');
		show_more_options = true;
	}
	
	setTimeout(calc_height, 2000);*/
}

function calc_height() {
	
	var left_height = $('#nobel-left-col').height();
	left_height = Math.round(left_height);
	var middle_height = $('#nobel-middle-col').height();
	middle_height = Math.round(middle_height);
	var right_height = $('#nobel-right-col').height();
	right_height = Math.round(right_height);
	
	// Calculate the height of the containers and make it look right
	var highest = left_height;
	
	if(middle_height > highest) {
		highest = middle_height;
	}
	
	if(right_height > highest) {
		highest = right_height;
	}
	
	var highest_mid = highest+20;
	$('#nobel-left-col').height(highest);
	$('#nobel-middle-col').height(highest_mid);
	$('#nobel-right-col').height(highest);
}

</script>

