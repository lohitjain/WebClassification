			track: true,
			delay: 0,
			extraClass: "ttl",
			showURL: false,
		
			fixPNG: true,
			opacity: 0.99,
			left: 5
		});
						
		$('.contentAccordion').accordion({
		  header: 'h6',
		  autoHeight: false,
		  active: false,
		  collapsible:true
		});        
        
		if($('li.box').length) {
		if (/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)){
			var iev=new Number(RegExp.$1) // capture x.x portion and store as a number
			if(iev >= 6) {
				Nifty("li.box","transparent top fixed-height small");
			}
		} else {
			Nifty("li.box","transparent top fixed-height small");
		}
		}
                // search action should consider the language setting
                if($("#searchform").length)$("#searchform").attr("action","/search/action/preferences.do?fn=change_lang&cmslang=" + cmsLang + "&vid=DADS&prefLang=" + primoLang + "&prefBackUrl="+location.protocol+"//"+location.host+encodeURI("/search/action/search.do?fn=go&ct=search"));
                $("#searchform").submit(function(){
                    $(this).attr("action",this.action + '%26vl(freeText0)='+encodeURI(encodeURI(document.getElementById('searchinput').value)));
                    return true;    
                });    
                // enhanced search link should consider the language setting    
                if($("#enhanced_search").length)$("#enhanced_search").attr("href","/search/action/preferences.do?fn=change_lang&cmslang=" + cmsLang + "&vid=DADS&prefLang=" + primoLang + "&prefBackUrl="+location.protocol+"//"+location.host+encodeURI($("#enhanced_search").attr("href")));
                /* Get the userinfo from primo, if Cookie JSESSIONID is set */
                var isSessionId = false;
                var aCookie = document.cookie.split(';');
                for(var i=0;i < aCookie.length;i++){
                    var c = aCookie[i];
                    while(c.charAt(0)==' ')c = c.substring(1,c.length);
                    if(c.indexOf("JSESSIONID=") == 0){
                        isSessionId = true;
                        break;
                    }
                }
                if(isSessionId)
                {
                    $.ajax({
                        url: "/userarea/search/action/search.do?vid=DADS&dscnt=1",
                        success: function(data){
                          // Cookie JSESSIONID is already set by Primo
                          var backurl;
                          if($(data).find("#exlidUserAreaRibbon").length){
                              $(".userarea-container #exlidUserAreaRibbon").empty();
                              $(".userarea-container #exlidUserAreaRibbon").append($(data).find("#exlidUserAreaRibbon").html());
                              $("#exlidUserAreaRibbon a").each(function(){
                                  if($(this).attr("href").indexOf("/search/action/")==-1)$(this).attr("href","/search/action/" + $(this).attr("href"));
                                  backurl = location.protocol+"//"+location.host;
                                  if(location.port)backurl += ":" + location.port;    
                                  backurl += encodeURI($(this).attr("href"));
                                  $(this).attr("href","/search/action/preferences.do?fn=change_lang&cmslang=" + cmsLang + "&vid=DADS&prefLang=" + primoLang + "&prefBackUrl="+backurl);
                              });
                          }
                          else
                          {
                            // links of userarea should consider language setting
                            $("#exlidUserAreaRibbon a").each(function(){
                                if($(this).attr("id")!= "login" ){
                                    backurl = location.protocol+"//"+location.host;
                                    if(location.port)backurl += ":" + location.port;    
                                    backurl += encodeURI($(this).attr("href"));
                                    $(this).attr("href","/search/action/preferences.do?fn=change_lang&cmslang=" + cmsLang + "&vid=DADS&prefLang=" + primoLang + "&prefBackUrl="+backurl);
                                }    
                            });
                          }  
                        }
                    }); 
                }
                /* event for language switch*/
                $("ul.languageswitch").bind("click",function(){
                    $(this).toggleClass("click_languageswitch");
                }); 
                /* placeholder of the searchfield */
                if(!Modernizr.input.placeholder){
                    $('[placeholder]').each(function(){
                      var input = $(this);
                      if(input.val() == ''){
                            input.val(input.attr('placeholder'));
                      }    
                    })
                    $('[placeholder]').focus(function(){
                      var input = $(this);
                      if(input.val() == input.attr('placeholder')){
                            input.val('');
                      }
                    });
                    $('[placeholder]').blur(function(){
                      var input = $(this);
                      if(input.val() == '' || input.val() == input.attr('placeholder')){
                            input.val(input.attr('placeholder'));
                      }
                    });
                    $('[placeholder]').parents('form').submit(function(){
                      $(this).find('[placeholder]').each(function(){
                            var input = $(this);
                            if(input.val() == input.attr('placeholder')){
                              input.val('');
                            }    
                      })
                    })
                };

        // Lightbox anfang
        
         var lightbox_closeText = 'Schliessen [ESC]';
         var lightbox_nextText = 'NÃ¤chstes Bild';
         var lightbox_prevText = 'Vorheriges Bild';
         var lightbox_pictureText = 'Bild';
         var lightbox_ofText = 'von';                   

        $(function() {	
            var lightbox1 = $("a.singleLightbox").ariaLightbox({
                useDimmer: true,
                animationSpeed: "fast",
                pos: "auto",
                em: false,
                zIndex:9999,
                closeText: lightbox_closeText,
                altText: function() {
                        return $(this).find("img").attr("alt");
                },
                descText: function() {
                        return $(this).find("img").attr("alt");
                }
            });
			
            var lightbox2 = $("#content_inner").ariaLightbox({
                imageArray: "a.seriesLightbox",
                useDimmer: true,
                animationSpeed: "fast",
                pos: "auto",
                em: false,
                zIndex:9999,
                nextText: lightbox_nextText,
                prevText: lightbox_prevText,
                pictureText: lightbox_pictureText,
                ofText: lightbox_ofText,
                closeText: lightbox_closeText,
                altText: function() {
                        return $(this).find("img").attr("alt");
                },
                descText: function() {
                        return $(this).find("img").attr("alt");
                },		
                onPrev: function(){
                    $("#ui-lightbox-close").focus();       
                },
                onNext: function(){
                    $("#ui-lightbox-close").focus();       
                },
                onShow: function(e, element){
                    $("#ui-lightbox-prev").on("touchstart",function(){lightbox2.ariaLightbox('prev');e.preventDefault();return false;});
                    $("#ui-lightbox-next").on("touchstart",function(){lightbox2.ariaLightbox('next');e.preventDefault();return false;});                             
                }
            });		
	});
        // Lightbox Ende
	// Test RIB-API Neuerwerbungen
	/*$.get("/layout/set/widget/content/view/new_acquisition/194",function(data){
	      $("#homeInner").append($(data));
	      $("#homeInner").css("height","auto");
	    })
	  .fail(function(){
	      
	    });
	*/
    });

</script>    	</head>    
