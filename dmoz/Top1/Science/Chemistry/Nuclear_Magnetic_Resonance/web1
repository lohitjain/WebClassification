<!-- content goes here -->
<div class="contentWrap">

  <TABLE class="content"  BORDER="0" CELLPADDING="0" CELLSPACING="0" ALIGN="LEFT" VALIGN="TOP" MARGIN="0">  <!-- content table -->
  <TR class="titleAndIcon">  
       <TD class="topContentTitle"> 
          <CENTER><H3>A Partial List of NMR Acronyms, Abbreviations, and Terms</H3></CENTER>
       </TD>
  </TR>
   <TR>
   <TD >
      <TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" ALIGN="LEFT">  <!-- layout table -->
      <!-- This is a two column table. It displays a side column menu and the main page -->
	 <TR>

	    <TD>
	       <TABLE class="content"  BORDER="0" CELLPADDING="0" CELLSPACING="0" ALIGN="LEFT">  <!-- content table -->
		  <TR class="titleAndIcon">  
		       <TD width="20%">
			     &nbsp;<BR>
			     <FORM method="GET"  action="/education/nmr_acronym.php" >
				 	<input style="margin-bottom:.5em;" type="text" name="query_id" size="15" maxlength="40" value="">
