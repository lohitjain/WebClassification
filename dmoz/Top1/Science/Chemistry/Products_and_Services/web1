<script type="text/javascript">
    function ShowAccess() {
        $("#Access").show();
    }
    function HideAccess() {
        $("#Access").hide();
    }
    function ShowAccount() {
        $("#AccouontList").show();
    }
    function HideAccount() {
        $("#AccouontList").hide();
    }
    function ShowBasket() {
        $("#divShoppingCart").focus();
        $("#divBasket").show();
    }
    function HideBasket() {
        $("#divBasket").hide();
    }
    function ToShoppingCart() {
        window.location.href = "/Cart/ShoppingCart.html";
    }
    function MsgAddToCartSuccess() {
        show_isven_popup_window('Successfully!', 400, 140, "AddToCartSuccessMsg.aspx");
    }
    function RefreshCanViewCount() {
        var url = "/Database/Handler/ChunkHandler.ashx?action=getCanViewCount";
        $.get(url, { t: new Date().getTime() }, function (res) {
            if (res == 0) {
                $("#CanViewCountBox").hide();
            } else {
                $("#CanViewCountBox [name=CanViewCount]").text(res);
                $("#CanViewCountBox").show();
            }
        });
    }
    $(function () {
        RefreshCanViewCount();
        document.onclick = HideBasket;
    });

    function jsSignIn() {
        window.location.href = "/MySignIn.html";
    }

    function jsRegister() {
        window.location.href = "/Join.html";
    }

    function jsSingOut() {
        location.href = "/SignOut.aspx";
    }
</script>

