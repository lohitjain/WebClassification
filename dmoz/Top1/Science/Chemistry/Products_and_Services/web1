var $j = jQuery.noConflict();
$j(document).ready(function(){
// Add hover to buttons
$j('button').hover(
	function () {
		$j(this).addClass("hover");
	}, 
	function () {
		$j(this).removeClass("hover");
	}
);
$j('body').addClass("style1");
// Add PIE selectors
    $j(function() {
        if (window.PIE) {
            $j('#rokmage-header-cart, #rokmage-login-content').each(function() {
                PIE.attach(this);
            });
        }
    });
});/* ]]> */
</script>

