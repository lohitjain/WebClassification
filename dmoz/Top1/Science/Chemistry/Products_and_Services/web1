<script language="JavaScript">
<!--

function Check(TheForm){
	if(!TheForm.custName.value) {
	alert ("Please enter your full name.");
	TheForm.custName.focus();
	return false;
	}

	if(!TheForm.custEmail.value) {
	alert ("Please enter your email address.");
	TheForm.custEmail.focus();
	return false;
	}

	if(!TheForm.delivery[0].checked && !TheForm.custAddress.value) {
	alert ("Please enter your mailing address.");
	TheForm.custAddress.focus();
	return false;
	}

	if(!TheForm.delivery[0].checked && !TheForm.custCity.value) {
	alert ("Please enter your city.");
	TheForm.custCity.focus();
	return false;
	}

	if(!TheForm.delivery[0].checked && !TheForm.custState.value) {
	alert ("Please enter your state.");
	TheForm.custState.focus();
	return false;
	}

	if(!TheForm.delivery[0].checked && !TheForm.custZip.value) {
	alert ("Please enter your zip code.");
	TheForm.custZip.focus();
	return false;
	}

return true;
}
//-->
</script>
