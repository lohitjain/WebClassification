        <script type="text/javascript">//<![CDATA[
            var $j = jQuery.noConflict();
            $j(document).ready(function(){
                var cartcount = $j('.rokmage-cart-box-container span.big').html();
                if(cartcount == 0) {
                    // Do Nothing
                } else {
                    if(cartcount == 1) {
                        $j('.footermenu li:eq(0)').html("<a href=\"http://synthonix.com/checkout/cart/\" class=\"footercart\">Cart (<strong>1</strong> item)</a>");
                    } else {
                        $j('.footermenu li:eq(0)').html("<a href=\"http://synthonix.com/checkout/cart/\" class=\"footercart\">Cart (<strong>" + cartcount + "</strong> items)</a>");
                    };
                };
            }); //]]>
        </script>

