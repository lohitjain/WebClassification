function getSmiles() {
  var drawing = document.JME.smiles();
  document.form.smi.value = drawing;
   
}

function getJmeFile() {
  var jme = document.JME.jmeFile();
  document.form.value = jme;
}

function getMolFile() {
  var mol = document.JME.molFile();
  document.form.mol_output.value = mol;
}

function processJme() {
  document.JME.readMolecule(document.form.jme_output.value);
}

function useMol() {
  document.JME.readMolFile(document.form.mol_output.value);
}


function verifTxtSearch()
  {
  if (document.layers)
    {
    formulaire = document.forms.form_search;
    }
  else
    {
    formulaire = document.form_search;
    }
  }



function remplirOperateursTxtSearch(code,listeaction)
{

// Alimentation liste 2 en fontion du contenu liste 1
var operateurs = new Array();
operateurs[0] = new Array("like", "not like", "equal");
operateurs[1] = new Array("like", "not like", "equal");
operateurs[2] = new Array("like", "not like", "equal");
operateurs[3] = new Array("like", "not like", "equal");
operateurs[4] = new Array("=", "<", ">","=~","<>");


	if (code==="cd_name") code=0;
	if (code==="cd_id") code=1;
	if (code==="cd_cas") code=2;
	if (code==="cd_formula") code=3;
	if (code==="cd_molweight") code=4;	

	verifTxtSearch();
  	var lesoperateurs = operateurs[code];

	if (listeaction==="Equal1")
	{
		
    	formulaire.Equal1.options.length = lesoperateurs.length;
   	 	for (i=0; i<lesoperateurs.length; i++)
		{
			
      		  formulaire.Equal1.options[i].value = lesoperateurs[i];
     		 formulaire.Equal1.options[i].text = lesoperateurs[i];
	 
      	}
    	//document.monFormulaire.Equal1.options.selectedIndex = 1;
 	}
	if (listeaction==="Equal2")   
	{
    		formulaire.Equal2.options.length = lesoperateurs.length;
			for (i=0; i<lesoperateurs.length; i++)
      		{
      			formulaire.Equal2.options[i].value = lesoperateurs[i];
      			formulaire.Equal2.options[i].text = lesoperateurs[i];;
      		}
	}
	
		if (listeaction==="Equal3")   
	{
    		formulaire.Equal3.options.length = lesoperateurs.length;
			for (i=0; i<lesoperateurs.length; i++)
      		{
      			formulaire.Equal3.options[i].value = lesoperateurs[i];
      			formulaire.Equal3.options[i].text = lesoperateurs[i];;
      		}
	}
	
		if (listeaction==="Equal4")   
	{
    		formulaire.Equal4.options.length = lesoperateurs.length;
			for (i=0; i<lesoperateurs.length; i++)
      		{
      			formulaire.Equal4.options[i].value = lesoperateurs[i];
      			formulaire.Equal4.options[i].text = lesoperateurs[i];;
      		}
	}
	
		if (listeaction==="Equal5")   
	{
    		formulaire.Equal5.options.length = lesoperateurs.length;
			for (i=0; i<lesoperateurs.length; i++)
      		{
      			formulaire.Equal5.options[i].value = lesoperateurs[i];
      			formulaire.Equal5.options[i].text = lesoperateurs[i];;
      		}
	}
 
}


