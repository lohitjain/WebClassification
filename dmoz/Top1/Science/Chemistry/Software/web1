<!-- Hide the script from old browsers --

// Jon Glassman (glassman@iquest.net)

// All rights reserved.  October 4, 1996

// This script may be used only with the permission of the author.

var mole=6.02E23

function calc()

{

document.boxes.result.value = eval (document.boxes.one.value)

}



function molmass()

{

document.boxes.result.value = document.boxes.two.value + " g/mol"

}



function gtomol()

{

document.boxes.result.value = 
eval(document.boxes.one.value/document.boxes.two.value) + " mol"

}



function gtopart()

{

document.boxes.result.value = 
eval((document.boxes.one.value*mole)/document.boxes.two.value) + "particles"

}



function moltog()

{

document.boxes.result.value = 
eval(document.boxes.one.value*document.boxes.two.value) + " g"

}





function parttog()

{

document.boxes.result.value = 
eval((document.boxes.one.value/mole)*document.boxes.two.value) + " g"

}



function abut(symb,amu)

{
document.write("<input type='button' value='" + aname + ", " + symb + "'onclick='disptwo(" + amu + ")'>")

}



function disptwo(amu)

{

var amount = 0-amu

if (document.boxes.two.value == "") {document.boxes.two.value = amu}

else {document.boxes.two.value = eval(document.boxes.two.value-amount)}

}



function moltopart()

{

document.boxes.result.value = eval(document.boxes.one.value*mole) + "particles"

}



function parttomol()

{

document.boxes.result.value = eval(document.boxes.one.value/mole) + " mol"

}



function molar()

{

document.boxes.result.value = 
eval(document.boxes.one.value/document.boxes.two.value) + " M"

}



function convert(amount,unit)

{

document.boxes.result.value = eval(document.boxes.one.value*amount) + unit

}



// --End Hiding Here -->

</script>

</head>

