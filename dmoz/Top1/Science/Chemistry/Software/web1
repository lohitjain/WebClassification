function verifJme()
  {
  if (document.layers)
    {
    formulaire = document.forms.form;
    }
  else
    {
    formulaire = document.form;
    }
  }



function remplirOperateursJme(code,listeaction)
{

// Alimentation liste 2 en fontion du contenu liste 1
var operateurs = new Array();
operateurs[0] = new Array("like", "not like", "equal");
operateurs[1] = new Array("like", "not like", "equal");
operateurs[2] = new Array("like", "not like", "equal");
operateurs[3] = new Array("like", "not like", "equal");
operateurs[4] = new Array("=", "<", ">","=~","<>");


	if (code==="cd_name") code=0;
	if (code==="cd_id") code=1;
	if (code==="cd_cas") code=2;
	if (code==="cd_formula") code=3;
	if (code==="cd_molweight") code=4;	

	verifJme();
  	var lesoperateurs = operateurs[code];
	

	if (listeaction==="operator1")
	{
	
    	formulaire.operator1.options.length = lesoperateurs.length;
		
   	 	for (i=0; i<lesoperateurs.length; i++)
		{
			
      		  formulaire.operator1.options[i].value = lesoperateurs[i];
     		 formulaire.operator1.options[i].text = lesoperateurs[i];
	 
      	}
    	//document.monFormulaire.operator1.options.selectedIndex = 1;
 }
	if (listeaction==="operator2")   
	{
    		formulaire.operator2.options.length = lesoperateurs.length;
			for (i=0; i<lesoperateurs.length; i++)
      		{
      			formulaire.operator2.options[i].value = lesoperateurs[i];
      			formulaire.operator2.options[i].text = lesoperateurs[i];;
      		}
	}
	
		if (listeaction==="operator3")   
	{
    		formulaire.operator3.options.length = lesoperateurs.length;
			for (i=0; i<lesoperateurs.length; i++)
      		{
      			formulaire.operator3.options[i].value = lesoperateurs[i];
      			formulaire.operator3.options[i].text = lesoperateurs[i];;
      		}
	}
	
		if (listeaction==="operator4")   
	{
    		formulaire.operator4.options.length = lesoperateurs.length;
			for (i=0; i<lesoperateurs.length; i++)
      		{
      			formulaire.operator4.options[i].value = lesoperateurs[i];
      			formulaire.operator4.options[i].text = lesoperateurs[i];;
      		}
	}
	
		if (listeaction==="operator5")   
	{
    		formulaire.operator5.options.length = lesoperateurs.length;
			for (i=0; i<lesoperateurs.length; i++)
      		{
      			formulaire.operator5.options[i].value = lesoperateurs[i];
      			formulaire.operator5.options[i].text = lesoperateurs[i];;
      		}
	}
 
}



