<script type="text/javascript">
function ActiveDesactive(champsaction,champs)
{
	if (document.getElementById(champsaction).disabled==true && document.getElementById(champs).value.length>0 )
		document.getElementById(champsaction).disabled=false;
	else {
		 // alert(document.getElementById(champs).value.length);
		if (document.getElementById(champs).value.length==0)
			document.getElementById(champsaction).disabled=true;
	
	}
	

}


</script>

		
