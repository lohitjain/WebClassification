<script language=javascript>
window.onresize=checkFrameSize;

function checkFrameSize() { 
	var minHeight = 300;
	var theIframes=document.getElementsByTagName("iframe");
	if (theIframes.length>0) {
		for (var i=0; i<theIframes.length; i++) {
			if (theIframes[i].resizable || (theIframes[i].hasAttribute && theIframes[i].hasAttribute("resizable"))) {
				// hasAttribute is not defined in internet explorer 6
				winH = 660;
				if (typeof(window.innerHeight) == 'number') {
					winH=window.innerHeight
				} else if (document.documentElement && document.documentElement.clientHeight) {
					winH=document.documentElement.clientHeight;
				} else if (document.body && document.body.clientHeight) {
					winH=document.body.clientHeight;
				}
				var height = winH-210;
				theIframes[i].height= height < minHeight ? minHeight : height;
			}
		}
	}
}

checkFrameSize();
jQuery(function(){
	jQuery('ul.menu.level1 > li > a').hover(menuItemHover);
	jQuery('ul.menu').not('.level1').mouseleave( menuItemMouseLeave );
})
function menuItemHover(item){
//	jQuery('ul.menu').not('.level1').hide();
	var li = jQuery(item.currentTarget).parent();
	li.find('ul').css('left', li.position().left).fadeIn(200);
}
function menuItemMouseLeave(item){
	jQuery(item.currentTarget).fadeOut();
}
var currentMsMass;
var parameters={};
function getChemCalc() {
	if ((parameters.mf==$("#mf").val()) && (parameters.resolution==$("#resolution").val()) && (parameters.referenceVersion==$("#referenceVersion").val())) return;
	parameters={mf: $("#mf").val(), resolution: $('#resolution').val(), referenceVersion: $('#referenceVersion').val()};
	jQuery.getJSON("/chemcalc", parameters, function(chemcalcResult) {
		if (chemcalcResult) {
			displayResult(chemcalcResult);
			currentMsMass=chemcalcResult.parts[0].msem;
			updateResolution();
		//	console.log(chemcalcResult);
		}
	})
}

function displayResult(chemcalcResult) {
	var result=[];
	result.push("<h2 width='100%' align='center'>"+chemcalcResult.options.mf.replace(/^(.{20}).*/,"$1...")+"</h2>");
	result.push("<table class='result'>");
	result.push("<tr><th>MF:</th><td>"+chemcalcResult.mf+"</td></tr>");
	result.push("<tr><th>MW:</th><td>"+chemcalcResult.mw+"</td></tr>");
	result.push("<tr><th>EM:</th><td>"+chemcalcResult.em.toFixed(5)+"</td></tr>");
	if (chemcalcResult.parts && chemcalcResult.parts.length==1) {
		if (chemcalcResult.parts[0].msem && chemcalcResult.parts[0].msem!=chemcalcResult.em) {
			result.push("<tr><th>Exp.&nbsp;EM:</th><td>"+chemcalcResult.parts[0].msem.toFixed(5)+"</td></tr>");
		}
		if (chemcalcResult.parts[0].unsaturation) {
			result.push("<tr><th>Unsat.:</th><td>"+chemcalcResult.parts[0].unsaturation+"</td></tr>");
		}
	}
	console.log(chemcalcResult);
	result.push("</table>");
	if (chemcalcResult.parts.length==1) { // Elemental analysis
		result.push("<table class='ea'><tr><th>Element</th><th>Count</th><th>Percent</th></tr>");
		for (var i=0; i<chemcalcResult.parts[0].ea.length; i++) {
			result.push("<tr><td>"+chemcalcResult.parts[0].ea[i].element+"</td>");
			result.push("<td>"+chemcalcResult.parts[0].ea[i].number+"</td>");
			result.push("<td>"+chemcalcResult.parts[0].ea[i].percentage.toFixed(3)+"%</td></tr>");
		}
		result.push("</table>");
	}

	$("#quickResult").html(result.join(""));
}

$("#mf").keyup($.debounce(250, function() {getChemCalc()}));
$("#peakWidth").keyup($.debounce(250, function() {
	$('#gaussianWidth').attr('checked', true);
	lastPeakWidth=true;
	updateResolution();
}));
$("#msResolution").keyup($.debounce(250, function() {
	$('#gaussianWidth').attr('checked', true);
	lastPeakWidth=false;
	updateResolution();
}));
$("#msResolutionSelect").change(function() {
	$('#gaussianWidth').attr('checked', true);
	$('#peakWidth').val($(this).val());
	$(this).val('');
	lastPeakWidth=true;
	updateResolution();
});
$("#referenceVersion").change($.debounce(250, getChemCalc));



function updateResolution() {
	if (lastPeakWidth) {
		if ($("#gaussianWidth").attr('checked')) {
			$("#resolution").val($("#peakWidth").val()/10);
		} else {
			$("#resolution").val($("#peakWidth").val());
		}
		if (currentMsMass) {
			$("#msResolution").val(Math.round(currentMsMass/$("#peakWidth").val()));
		} else {
			$("#msResolution").val("");
		}
	} else {
		if (currentMsMass) {
			$("#peakWidth").val(currentMsMass/$("#msResolution").val());
			if ($("#gaussianWidth").attr('checked')) {
                        	$("#resolution").val($("#peakWidth").val()/10);
                	} else {
                        	$("#resolution").val($("#peakWidth").val());
                	}
		} else {
			$("#resolution").val(0.001);
		}
	}	
}


var lastPeakWidth=true;
var resolution=0.001;
$("#resolution").val(resolution);
var peakWidth=0.001;
$("#peakWidth").val(peakWidth);
var referenceVersion=2013;
$("#referenceVersion").val(referenceVersion);
var gaussianWidth=0;
if (gaussianWidth>0) {
	$("#gaussianWidth").attr('checked', true);
}

getChemCalc();
</script>

</body>
</html>
