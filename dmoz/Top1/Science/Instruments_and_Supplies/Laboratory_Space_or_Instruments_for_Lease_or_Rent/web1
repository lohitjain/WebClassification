                        <script type="text/javascript">
                var currentSlideIndex = -1;
                var currentSliderHeight = 0;
                function flexslider_set_current(i){
                    if(currentSlideIndex == i){
                        return false;
                    }
                    currentSlideIndex = i;
                    jQuery('.flexslider_sidebar .active').removeClass('active');
                    jQuery('.flexslider_sidebar li:nth-child('+(i+1)+') a').addClass('active');
                    return true;
                }
                function flexslider_set_button_heights(){
                    var h = jQuery('.flexslider_images').height();
                    /*if(!currentSliderHeight && h<150){
                        setTimeout(flexslider_set_button_heights,100);
                    }else{*/
                    if(currentSliderHeight != h){
                        jQuery('.flexslider_sidebar').show();
                        jQuery('.flexslider_sidebar').height(h);
                        jQuery('.flexslider_sidebar span').height(h / 7 );
                        currentSliderHeight = h;
                    }else{

                    }
                    setTimeout(flexslider_set_button_heights,150);
                    //}
                }
                (function($) {
                    $(document).ready( function() {
                        $('.flexslider').flexslider({
                            //manualControls: '.flexslider_sidebar li a',
                            //controlsContainer: '.flexslider_wrapper',
                            slideshowSpeed: 3000,
                            directionNav: false,
                            controlNav: false,
                            start: function(slider){
                                flexslider_set_current(slider.currentSlide);
                                setTimeout(flexslider_set_button_heights,100);
                            },
                            after: function(slider){
                                flexslider_set_current(slider.currentSlide);
                                flexslider_set_button_heights();
                            }
                        });
                        $('.flexslider_sidebar a').mouseover(function(){
                            // which one is it?
                            var currentSlide = jQuery('.flexslider_sidebar a').index(this);
                            if(flexslider_set_current(currentSlide)){
                                var f = jQuery('.flexslider').data('flexslider');
                                f.vars.animationDuration = 10;
                                f.pause();
                                f.flexAnimate(currentSlide);
                            }

                        });
                    });
                })(jQuery);
            </script>
