<!-- form validation begins-->
<script language="javascript" type="text/javascript">
function echeck(str) {
		var at="@"
		var dot="."
		var lat=str.indexOf(at)
		var lstr=str.length
		var ldot=str.indexOf(dot)
		if (str.indexOf(at)==-1){
		   alert("Invalid E-mail ID")
		   return false
		}
		if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
		   alert("Invalid E-mail ID")
		   return false
		}
		if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
		    alert("Invalid E-mail ID")
		    return false
		}
		 if (str.indexOf(at,(lat+1))!=-1){
		    alert("Invalid E-mail ID")
		    return false
		 }
		 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
		    alert("Invalid E-mail ID")
		    return false
		 }
		 if (str.indexOf(dot,(lat+2))==-1){
		   alert("Invalid E-mail ID")
		    return false
		 }
		 if (str.indexOf(" ")!=-1){
		    alert("Invalid E-mail ID")
		    return false
		 }
 		 return true					
	}
	
	
function validation()
	{

	if(document.vinfo.subject.value=="")
		{
				alert("Please Enter your Subject!");
				document.vinfo.subject.focus();
				return false;
		} 
		
		
	if(document.vinfo.vinfo_detail.value=="")
		{
				alert("Please Enter your Full Name!");
				document.vinfo.vinfo_detail.focus();
				return false;
		}

	if(document.vinfo.vinfo_name.value=="")
		{
				alert("Please Enter your Full Name!");
				document.vinfo.vinfo_name.focus();
				return false;
		}
	if(document.vinfo.vinfo_co_name.value=="")
		{
				alert("Please Company Name !");
				document.vinfo.vinfo_co_name.focus();
				return false;
		} 
	if(document.vinfo.vinfo_address.value=="")
		{
				alert("Please Enter your Address!");
				document.vinfo.vinfo_address.focus();
				return false;
		} 
	if(document.vinfo.vinfo_city.value=="")
		{
				alert("Please Enter City Name!");
				document.vinfo.vinfo_city.focus();
				return false;
		}

   if (!document.getElementById("vinfo_phone_no").value.match(/^\d+$/)) 
		{
				alert("Please Enter Phone number in numberic from!");
				document.vinfo.vinfo_phone_no.value=="";
				document.vinfo.vinfo_phone_no.focus();
				return false;
		}
	if(document.vinfo.vinfo_email.value=="")
		{
				alert("Please Enter your Email ID  !");
				document.vinfo.vinfo_email.focus();
				return false;
		}
		
	if (echeck(document.vinfo.vinfo_email.value)==false)
		{
				document.vinfo.vinfo_email.value="";
				document.vinfo.vinfo_email.focus();
				return false;
		}
		
 if(document.vinfo.letters_code.value=="")
		{
				alert("Please enter Image data  !");
				document.vinfo.letters_code.focus();
				return false;
		} 
	}
</script>
