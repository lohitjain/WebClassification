                                                onCuepoint: [[200], function () {

                                                    if (firstPlay == 0) {

                                                        firstPlay = 1;
                                                        var url = "/video/addView.aspx?v=SSci_Ventana_001.flv";

                                                        xmlHttpVid.open("GET", url, true);
                                                        //alert('damn IE' + firstPlay);

                                                        xmlHttpVid.send(null);
                                                    }
                                                } ]
                                                // <- do not place a comma here 


                                            },

                                            plugins: {



                                                controls: {
                                                    url: 'flowplayer.controls-3.2.5.swf',

                                                    backgroundColor: "#add083",
                                                    backgroundGradient: "none",
                                                    autoHide: false,

                                                    sliderBorder: '0.5px solid rgba(160,160,160,0.7)',
                                                    volume: false,
                                                    volumeSliderColor: '#FFFFFF',
                                                    volumeBorder: '1.5px solid rgba(160,160,160,0.7)',
                                                    fullscreen: true,


                                                    durationColor: '#535353',

                                                    tooltipColor: 'rgba(255, 255, 255, 0.7)',
                                                    tooltipTextColor: '#000000'
                                                }
                                            }
                                        }).ipad();


                                    } //if

                                    else { //if not on ios use usual .flv video

                                        flowplayer("player", { src: "http://www.selectscience.net/flowplayer/flowplayer.commercial-3.2.7.swf", wmode: "opaque" }, {
                                            key: ['#$61f5bc9c9c866029202', '#$bf4e8fabf54938ca16b', '#$8bb0dc4ed088106974b'],

                                            // change the default controlbar to modern


                                            clip: {
                                                "url": "http://www.selectscience.net/videos/SSci_Ventana_001.flv",
                                                // these two configuration variables does the trick
                                                autoPlay: true,
                                                autoBuffering: true,
                                                onCuepoint: [[200], function () {

                                                    if (firstPlay == 0) {

                                                        firstPlay = 1;
                                                        var url = "/video/addView.aspx?v=SSci_Ventana_001.flv";

                                                        xmlHttpVid.open("GET", url, true);
                                                        //alert('damn IE' + firstPlay);

                                                        xmlHttpVid.send(null);
                                                    }
                                                } ]
                                                // <- do not place a comma here 


                                            },

