    <script type="text/javascript">
        //<![CDATA[
        function validateFirstName(element) {
            if (element.value == "") {
                element.value = "<First Name>";
                element.style.color = "#525E68";
            }
        }

        function validateLastName(element) {
            if (element.value == "") {
                element.value = "<Last Name>";
                element.style.color = "#525E68";
            }
        }

        function validateEmail(element) {
            var emailPattern = /^[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?$/;
            if (!emailPattern.test(element.value) && element.value != "<Email Address>")
            {
                element.style.color = "red";
            }
            else
            {
                element.style.color = "black";
            }
            if (element.value == "") {
                element.value = "<Email Address>";
                element.style.color = "#525E68";
            }

        }

        $(document).ready(function() {
            $("#cusSelect1").change(function() {
                $("#cusSelect1Text").css('color', '#525E68');
            }
                    );
        });

        //]]>
    </script>


