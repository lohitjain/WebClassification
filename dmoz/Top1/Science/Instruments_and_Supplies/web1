                   function fulltrim(str) {
                       return str.replace(/^\s+|\s+$/g, "");
                   }
                   function checkSearch()
                   {
		
			
                       if ((document.getElementById('fldSearch').value) == 'Search') { document.getElementById('page-loader').style.display ="none"; alert('Please enter a valid search term'); return false;  }
                       if ((document.getElementById('fldSearch').value.replace(/^\s\s*/, '').replace(/\s\s*$/, '').replace(/\"/g, '').replace(/\./g, '')) == '')
                       { document.getElementById('page-loader').style.display = "none"; alert('Please enter a valid search term'); return false; }
                       else {
                           document.getElementById('fldSearch').value = document.getElementById('fldSearch').value.replace(/\"/g, '').replace(/\./g, '');
                       }


                   }
                   function checkProdSearch() {


                       if ((document.getElementById('search').value) == 'Search') { document.getElementById('page-loader').style.display = "none"; alert('Please enter a valid search term'); return false; }
                       if ((document.getElementById('search').value.replace(/^\s\s*/, '').replace(/\s\s*$/, '').replace(/\"/g, '').replace(/\./g, '')) == '')
                       { document.getElementById('page-loader').style.display = "none"; alert('Please enter a valid search term'); return false; }
                       else {
                           document.getElementById('search').value = document.getElementById('search').value.replace(/\"/g, '').replace(/\./g, '');
                       }


                   }
