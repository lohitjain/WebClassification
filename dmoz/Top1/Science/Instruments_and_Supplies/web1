function check_search(frm)
{
				//Initial lines
				show_error[0]='Please make note of following things :\n';
				form_name='global_search_form';
				//End of initial lines
				validate_text('keyword','Keyword','string',1);
				validate_radio('nobs','Sellers or Buyers');
				//Initial lines
				error_counter('1');
				if(back_m==1){
								change_action_search(frm);
								disable();
								back_m=0;
								return true;
				}
				else{error_counter('2');error_counter('0');return false;}
				//End of initial lines
}
function change_action_search(frm) {
				var dir='Buyer';
				var type;
				if(frm.nobs[0].checked) {
								dir='Seller';
				}
				if(frm.criteria.selectedIndex == 0){
								type='prod';
				}else{
								type='company';
				}
				frm.action='http://www.tradeindia.com/' + dir + '/' + type + '_search.html';
				frm.submit();
				return true;
}
              </script>
