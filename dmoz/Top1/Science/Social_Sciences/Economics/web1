    <div class="home">
        <div id="container">
            
<script language="JavaScript">
function doSectSearch(){
	
	secVal = document.frmRightSector.cmbsec.value;
	if(secVal == -1){
		alert('Please select a sector');
		return false;
	}
	
	document.frmRightSector.action = 'http://www.miga.org/'+'projects/advsearchresults.cfm?srch=s&sector='+ secVal;
	document.frmRightSector.submit();
}

function doCntrySearch(){
	
	cntryVal = document.frmRightRegCntry.cmbhcntry.value;

	if(cntryVal == -1){
		alert('Please select a region/country');
		return false;
	}
	cntrySrchActn = '';

	param = fn_getCountry(cntryVal);	
	if (param != "")
		cntrySrchActn = cntrySrchActn + "&hctry="+ param;

	param = fn_countryCode(cntryVal,'c');	
	if (param != "")
		cntrySrchActn = cntrySrchActn + "&hcountrycode="+ param;
		
	param = fn_countryCode(cntryVal,'r');
	if (param != "")
		cntrySrchActn = cntrySrchActn + "&hregioncode="+ param;
	
	document.frmRightRegCntry.action = 'http://www.miga.org/'+'projects/advsearchresults.cfm?srch=s'+ cntrySrchActn;
	document.frmRightRegCntry.submit();
}

function  fn_getCountry(lst)
{
	var retval = "";
	tmp = lst;
	fpos = tmp.indexOf("-");
	spos = tmp.indexOf("-", fpos+1);
	retval = tmp.substring(0,spos);
	retval = replaceAll(retval,"-","");
	return retval;
}

function fn_countryCode(lst,cntryorreg)
{
	var retval = "";
	tmp = lst;
	fpos = tmp.indexOf("-");
	spos = tmp.indexOf("-", fpos+1);			
	if(tmp.charAt(fpos+1)==cntryorreg)
	  if(cntryorreg=="c")
		  retval = tmp.substring(spos+1,tmp.length);
	  else
		  retval = tmp.substring(0,spos-2);
	return retval;
}

function replaceAll (s, fromStr, toStr)
{
	var new_s = s;
	for (j = 0; j < 100 && new_s.indexOf (fromStr) != -1; j++)
	{
		new_s = new_s.replace (fromStr, toStr);
	}
	return new_s;
}

function chkEnterKey(event)
{
	var code = 0;
	if (document.layers)
		code = event.which;
	else
		code = event.keyCode;
	if (code==13)
	  {
		if(!fn_gensrch()){
			return false;
		}
	  }
}
	
function fn_gensrch()
{
	if (document.frmGs.txtsrch.value.length < 3)
	{
	  alert("Search Keyword should be minimum 3 characters \n");
	  return false;
	}  
  	else if (document.frmGs.txtsrch.value == "Search"){
		alert("Please enter a search Keyword \n");
		return false;
	}else{
		stxt = document.frmGs.txtsrch.value;
		document.frmGs.target=target="_self";	
		document.frmGs.action="http://www.miga.org/search/index.cfm?q=" + stxt + "&start=0";
		document.frmGs.submit();
	}
}

</script>
