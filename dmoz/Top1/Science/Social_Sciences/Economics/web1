<!--<script src="/include/jQuery/js/jquery.wslide.unpacked.js"  charset="utf-8"></script> -->
<script type="text/javascript" charset="utf-8">
	$(document).ready(function(){
		$('#slideshow').cycle({ 
		    prev:   '#prev', 
		    next:   '#next', 
		    timeout: 6000,
		    pauseOnPagerHover: 1,
		    pager:  '#slideshow-menu'
		    
		});

		$('#slideshow').mouseover(function() {
			$('#slideshow').cycle('pause');
		});

		$('#slideshow').mouseout(function() {
			$('#slideshow').cycle('resume');
		});

		$('#secondaryslide').cycle({ 
		    prev:   '#sprev', 
		    next:   '#snext', 
		    timeout: 6000,
		    pauseOnPagerHover: 1,
		    pager:  '#secondaryslide-menu'
		    
		});

		$('#secondaryslide').mouseover(function() {
			$('#secondaryslide').cycle('pause');
		});

		$('#secondaryslide').mouseout(function() {
			$('#secondaryslide').cycle('resume');
		});

		function pagerFactory(idx, slide) {
	        var s = idx > 2 ? ' style="display:none"' : 'style="text-decoration:none"';
			alert(slide.html);
	        return '<a href="#" '+ s + '>'+(idx+1)+'</a>';
	        
	    };
		
		$(function() {
    var zIndexNumber = 1000;
    $('div').each(function() {
        $(this).css('zIndex', zIndexNumber);
        zIndexNumber -= 10;
    });
});


	});

	
	

</script>
