	contentLoader.contentSettings["conf_2014"] = {"contentName":"conf_2014"};
	contentLoader.contentSettings["econ_harmony"] = {"contentName":"econ_harmony"};
	contentLoader.contentSettings["in_the_news_a"] = {"contentName":"in_the_news_a"};
	
		
	contentLoader.ajaxUrl = "http://www.aeaweb.org/ajax_interface/tab_content_get_content.php";
		
	contentLoader.setButtons();
	
	contentLoader.getContentStart("webcast_2014");

});

</script>

