<script type="text/javascript">
	var ecProdNav = {
		open : function(){
		    if( $(".loginSpan").is("*") == false )
		    {
		        var obj = document.getElementById("ecImgLnk");
		        var posX = obj.offsetLeft;var posY = obj.offsetTop;
                while(obj.offsetParent){
                if(obj==document.getElementsByTagName('body')[0]){break}
                else{
                posX=posX+obj.offsetParent.offsetLeft;
                posY=posY+obj.offsetParent.offsetTop;
                obj=obj.offsetParent;
                }
                }
		        $("#ecProdNavCont").css( "top", (posY + 20) );
		        $("#ecProdNavCont").css( "left", posX );
			    var oNav = document.getElementById("ecProdNavCont");

			    if (oNav.style.display == "none")
			    {
				    oNav.style.display = "block";
				    ecProdNav.addEvent("mousedown", ecProdNav.md);
			    }
			}
			else
			{
			    window.location = "/";
			}
		},

		close : function(){
			window.setTimeout(function(){document.getElementById("ecProdNavCont").style.display = "none";}, 200);
			ecProdNav.removeEvent("mousedown", ecProdNav.md);
		},

		md : function(e){ window.setTimeout(ecProdNav.close, 100); },

		addEvent : function(e,f){
			if (document.attachEvent)
				document.attachEvent('on'+e,f);
			else
				document.addEventListener(e,f,true);
		},

		removeEvent : function(e,f){
			if (document.detachEvent)
				document.detachEvent('on'+e,f);
			else
				document.removeEventListener(e,f,true);
		}
	};
	$(document).ready( function(){
	        $(".loginLogoutDiv").bind( "keypress", checkEnter );
	    });
	function checkEnter( e )
	{
	    var keycode = "";
        if (window.event)
        {
            keycode = window.event.keyCode;
        }
        else if(e)
        {
            keycode = e.which;
        }
        if( keycode == 13 )
        {
            submitHeaderLoginForm();
        }
        return true;
	}
	function submitHeaderLoginForm()
	{
	    if( $("#email").val() != "" && $("#password").val() != "" )
	    {
	        document.loginForm.submit();
	    }
	}
</script>


