            if (x) clearTimeout(x);
            jQuery("#nav ul ul").hide();
            jQuery(this).find('ul').show();

            jQuery('#nav ul li a').removeClass('opened');
            jQuery(this).find('a').addClass('opened');
            alignItems(jQuery(this));
        }).mouseout(function () {
            x = setTimeout('test()', 2000);
