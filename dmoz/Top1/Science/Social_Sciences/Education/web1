$(document).ready(function() {
	
$("ul#topnav li ").hover(function() { //Hover over event on list item
	$(this).css({ 'background' : '#1376c9 url(../images/media/submenu2.gif) center center repeat-x'}); //Add background color + image on hovered list item
	$(this).find("span").show(); //Show the subnav
