		  				"Zygote",
		  			  
		  				"All Journals"]
		jQuery("#cstitle").autocomplete({source : data});
		
		var type = '';
		if(type != '') {
			jQuery('#qscats').val(type)
			jQuery("input[name='quickSearchType']").val(type)
		}
		
	
		if (jQuery.browser.mozilla) {
		    jQuery('#cite-search div input').bind("keypress", function(e) {
				if(e.keyCode==13){
					submitSearch();
					return false;
				}
			}); 
		} else {
		   jQuery('#cite-search div input').bind("keydown", function(e) {
				if(e.keyCode==13){
					submitSearch();
					return false;
				}
			}); 
		}
		
		if (jQuery.browser.mozilla) {
		    jQuery('#qskeystage2').bind("keypress", function(e) {
				if(e.keyCode==13){
					quickSearch_submit();
					return false;
				}
			}); 
		} else {
		   jQuery('#qskeystage2').bind("keydown", function(e) {
				if(e.keyCode==13){
					quickSearch_submit();
					return false;
				}
			}); 
		}
	});
	
	var type = '';
	if(type != '') {
		jQuery('#qscats').val(type)
		jQuery("input[name='quickSearchType']").val(type)
	}
	
	function quickSearchFlag() {
		var option = 'search_combined';
		var flag = 'N';
		if(flag == 'Y'){
			jQuery('#qscats').val(option);
		}
	}

	$.ajaxSetup ({
    	// Disable caching of AJAX responses
    	cache: false
	});
	
	var count = 0;
	
	function checkToken(){
		if (count > 0){
			return false;
		}else{
			count = count + 1;
			return true;
		}
	}

	function changeSubmit(formName) {
		var form = window.document.forms[formName]; 
		var journalParam = jQuery("#cstitle").val();

		if(jQuery("#"+jQuery("#cstitle").val()).val()){
			form.journals.value = journalParam;
		} else {
			form.journals.value = 'All';
		}		
		
		if(form.volume.value == "" || form.volume.value == null) {
			form.volume.value = 'Volume';
		} else {
			if(isNaN(form.volume.value)) {
				form.volume.value = 'Volume';
			}	
		}
		
		if (form.issue.value != 'Issue'
			&& form.page.value != 'Page'
			&& (form.author.value == 'Author' || form.author.value == '')
			&& (form.volume.value == 'Volume' || form.volume.value == '')
			&& (form.year.value == 'Year' || form.year.value == '')) {
			alert('Please further define your search by adding more information.');
			return false;
		} else if (form.issue.value == 'Issue'
			&& form.page.value != 'Page'
			&& (form.author.value == 'Author' || form.author.value == '')
			&& (form.volume.value == 'Volume' || form.volume.value == '')
			&& (form.year.value == 'Year' || form.year.value == '')) {
			alert('Please further define your search by adding more information.');
			return false;
		} else {
			var advance_url = 'search?searchType=CITEADVANCE' +
				'&journals=' + journalParam +
				'&author=' + form.author.value +
				'&year=' + form.year.value +
				'&volume=' + form.volume.value +
				'&issue=' + form.issue.value+
				'&page=' +form.page.value;
			form.action = advance_url;
		}
		return true;
	}
	
	function quickSearch_submit() {
		var valid = false;
		var qsearchType = document.quickSearchForm.quickSearchType.value;
		var qform = jQuery("#qscats").closest("form");
		var issueId = jQuery("input[name=issId]").val();
		var inputField1 = jQuery('#qskeystage');
		var input = jQuery('#qskeystage').val();
		var journalSearchType = jQuery("#qscats option:selected").text();
		var journalTitle = jQuery("input#qskeystage2").val();
		
		if ((input != null && jQuery.trim(input) !='') && input.length > 0) {
			valid = true;
		}else if ((journalTitle != null && jQuery.trim(journalTitle) !='') && journalTitle.length > 0 && journalTitle != 'Type part of journal title here') {
			valid = true;
		}

		if (valid && qsearchType == 'search_combined'){ 
	    	qform.attr("action","/action/quickSearch?searchTypeFrom=quickSearch");  
	    	jQuery("input[name=journalSearchType]").val("all");
	    	qform.attr("target","_self");
	    	qform.submit();
		} else if (valid && qsearchType == 'search_journal'){	
	 		qform.attr("action","/action/quickSearch?searchTypeFrom=quickSearch");
	 		jQuery("input[name=journalSearchType]").val("all");
			qform.attr("target","_self");
			qform.submit();
		} else if (valid && qsearchType == 'search_journal_content_with_access'){	
	 		qform.attr("action","/action/quickSearch?searchTypeFrom=quickSearch");
	 		jQuery("input[name=journalSearchType]").val("all");
			qform.attr("target","_self");
			qform.submit();
		}   else if (valid && (qsearchType == 'search_mrs' || qsearchType == 'search_nutsoc' || qsearchType == 'search_apsa' )){	
	 		qform.attr("action","/action/quickSearch?searchTypeFrom=quickSearch");
	 		jQuery("input[name=journalSearchType]").val("all");	 		
			qform.attr("target","_self");
			qform.submit();
		} else if (valid && qsearchType == 'search_specific'){
			if(journalSearchType == 'This Issue'){
	 			qform.attr("action","/action/quickSearch?searchTypeFrom=quickSearch");
	 			if(issueId == '-1'){
	 				jQuery("input[name=journalSearchType]").val("volume");
				}else{
	 				jQuery("input[name=journalSearchType]").val("issue");
	 			}
	 		}else if(journalSearchType == 'This Journal'){
	 			qform.attr("action","/action/quickSearch?searchTypeFrom=quickSearch");
	 			jQuery("input[name=journalSearchType]").val("journal");
	 		}
			qform.attr("target","_self");
			qform.submit();
		} else if (valid && qsearchType == 'other_content'){
			qform.attr("action","/action/quickSearch?searchTypeFrom=search");  
			qform.attr("target","_self");
			jQuery('#qskeystage').after("<input type='hidden' name='cmd' value='search' />");
			qform.submit();
		} else if (valid && qsearchType == 'search_title'){
			qform.attr("action","/action/quickSearch?searchTypeFrom=quickSearch");
	 		jQuery("input[name=journalSearchType]").val("title");
			qform.attr("target","_self");
			qform.submit();
		} else {
			alert("Invalid search.");
		}
	}
	
	function quickSearchFlag() {
		var option = 'search_combined';
		var flag = 'N';
		if(flag == 'Y'){
			jQuery('#qscats').val(option);
		}
	}
	
	function submitSearch() {
		jQuery("input[name=searchType]").val("CITEADVANCE");
		jQuery('#topformheadersubmit2').closest("form").submit();
	}
	
	function loadStreamingNavigation(pageId, id) {
		jQuery.get("/action/loadStreamingNavigation", {pageId:pageId},
			function(data) {
				var jsonObj = jQuery.parseJSON(data);
				
				for(var i = 0; i <= jsonObj.page.length; i++) {
					if(jsonObj.page[i]) {
						var title = jsonObj.page[i].title;
						var url = jsonObj.page[i].url;
						jQuery("#" + id).append("<li><a title='" + title + "' alt='" + title + "' href='" + url + "'>" + title + "</a></li>");
					}
				}
			}
		);
	}
	
	function toggleSearchTitle(){
		var type = jQuery(this).val();
		if(type != 'search_title'){
			jQuery("input#qskeystage2").hide();
			jQuery("input#qskeystage").show();
		}else{
			jQuery("input#qskeystage").hide();
			jQuery("input#qskeystage2").show();
		}
	}
</script>

