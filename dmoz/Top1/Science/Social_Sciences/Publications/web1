		if (jQuery.browser.mozilla) {
		    jQuery("#qskeystage").bind("keypress", function(e) {
				if(e.keyCode==13){
					quickSearch_submit();
					return false;
				}
			}); 
		} else {
		    jQuery("#qskeystage").bind("keydown", function(e) {
				if(e.keyCode==13){
					quickSearch_submit();
					return false;
				}
			}); 
