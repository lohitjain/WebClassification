	<script>
		jQuery(document).ready(function() {
			var topHeaderContent = jQuery('#top-header').html();
			function getMastHead(id) {
				jQuery.post('/validator?'+Math.random(),{type:2,jid:id}, function(data) {
					topHeaderContent = data + topHeaderContent;
					displayMastHead();
				});
			}
			function displayMastHead() {
	    		jQuery('#top-header').html(topHeaderContent);
			}
	
			getMastHead('JBS');
		});
	</script>	
