<script type="text/javascript">
// <!--
	var hasNews=false;
	var hasEvents=false;
	var newsEffect='0';
	
	function MM_openBrWindowIs(theURL,winName,features) { //v2.0
		var h = window.open(theURL,winName,features);
		h.focus();
	}
	
	function changeTick() {
		ct++;
		if (ct>=tl) ct=0;
		tk.href = tlink[ct];
		if(tnewsType[ct]=='true') {
			jQuery('#newsType').removeClass('detailbox');
			jQuery('#newsType').addClass('detailbox_urgent');
		} else {
			jQuery('#newsType').removeClass('detailbox_urgent');
			jQuery('#newsType').addClass('detailbox');
		}
											
		runTick(0);
	}
	
	function runTick(cl) {
		nv = new String();
		nl = thead[ct].length+1;
		if(newsEffect == '')
			newsEffect = 0;
		if(parseInt(newsEffect)==0){
			if (cl<nl) {
				nv = thead[ct].substring(0,cl);
				if(cl<nl-1){
					tk.innerHTML = nv + "_";
				}else{
					tk.innerHTML = nv;
				}
				cl++;
				setTimeout('runTick("' + cl + '")',5);
			} else {
				setTimeout('changeTick()',5000);	
			}
		}else{
			nv = thead[ct];
			tk.innerHTML = nv + "";
			setTimeout('changeTick()',5000);
		}
	}
	
	function hChangeTick() {
		hct++;
		if (hct>=tl) ct=0;
		htk.href = htlink[ct];
		hRunTick(0);
	}
	
	function hRunTick(hcl) {
		hv = new String();
		hl = hthead[ct].length+1;
		if(newsEffect == '')
			newsEffect = 0;
		if(parseInt(newsEffect)==0){
			if (hcl<hl) {			
				nv = hthead[hct].substring(0,hcl);
				if (hcl<hl-1) {	
					htk.innerHTML = nv + "_";
				}else{
					htk.innerHTML = nv;
				}
				hcl++;
				setTimeout('hRunTick("' + hcl + '")',5);
			} else {
				setTimeout('hChangeTick()',5000);	
			}
		}else{
			nv = hthead[hct];
			htk.innerHTML = nv + "";
			setTimeout('hChangeTick()',5000);
		}
	}
	
	function changeTickE() {
		ctE++;
		if (ctE>=tlE) ctE=0;
		tkE.href = tlinkE[ctE];
		runTickE(0);
	}
	
	function runTickE(clE) {
		nvE = new String();
		nlE = theadE[ctE].length+1;
		if (clE<nlE) {
			nvE = theadE[ctE].substring(0,clE);
			if (clE<nlE-1) {
				tkE.innerHTML = nvE + "_";
			}else{
				tkE.innerHTML = nvE;
			}
			clE++;
			setTimeout('runTickE("' + clE + '")',5);
		} else {
			setTimeout('changeTickE()',5000);	
		}
	}
	
	function hChangeTickE() {
		hctE++;
		if (hctE>=tlE) ctE=0;
		htkE.href = htlinkE[ctE];
		hRunTickE(0);
	}
	
	function hRunTickE(hclE) {
		hvE = new String();
		hlE = htheadE[ctE].length+1;
		if (hclE<hlE) {
			nvE = htheadE[hctE].substring(0,hclE);
			if (hclE<hlE-1) {
				htkE.innerHTML = nvE + "_";
			}else{
				htkE.innerHTML = nvE;
			}
			hclE++;
			setTimeout('hRunTickE("' + hclE + '")',5);
		} else {
			setTimeout('hChangeTickE()',5000);	
		}
		
	}
	
	function initializeComputation(){
		jQuery.each(jQuery('div.ji-links'),function(index, value){
			var id = jQuery(this).attr('id').split("-")[1];
			loadArticleAccess(id);
		});
	}	

	function loadArticleAccess(componentId) {
		jQuery.get('/action/getComponentAccess;jsessionid=23C501298F8E57AF5861FA713D9F34E3.journals?' + new Date(),{compId:componentId,type:"article"},
			function(data){				
				jQuery('div#articleLinks-'+componentId).html(data);				
			}
		);
	}
