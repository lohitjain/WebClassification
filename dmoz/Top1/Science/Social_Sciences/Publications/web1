	<script type="text/javascript">
	// <!--
	var cache={};
	var requestSent = false;
	var requestTab = '';
	
	function changeTab(tabName) {
		jQuery('.ajaxContainer').hide();
		jQuery('#ajaxContainer-'+tabName).show();		
		jQuery('.tabelement').removeClass("current");
		jQuery('#tab'+tabName).addClass("current");
		jQuery.cookies.set('JBS_journaltab', tabName);
		jQuery.cookies.set('JBS_overridetab','N');
		if(requestSent && requestTab != tabName){
			requestSent = false;
		}
		
		if(!requestSent && requestTab != tabName){
			requestSent = true;
			requestTab = tabName;
			if(cache[tabName]!=true){
					jQuery('#ajaxContainer-'+tabName).html('<img align="center" src="/images/ajax-loader.gif"/> Loading articles...');
					jQuery.get('/action/displayJournalTab;jsessionid=23C501298F8E57AF5861FA713D9F34E3.journals?' + new Date(),
						{jid:'JBS', tab:tabName },
						function(data){
							jQuery('#ajaxContainer-'+tabName).html(data);
							if(tabName!='manuscripts'){
								jQuery('#ajaxContainer-'+tabName+" .preview").click(previewEvent);
								
								jQuery('#ajaxContainer-'+tabName+" .commentbox").bind('click',function(){
									animations_commentEvent(this);
								});
								jQuery('#ajaxContainer-'+tabName+" .comment-bttn").bind('click',function(){
									return postCommentEvent(this);								
								});
							}										
						}
					)
					.complete(function() {
						requestSent = false;
						requestTab = '';
						
							//JOU-14359 ndomingo 14JAN14 :: load mathjax when tab selected and content on div
							
							if('N' == 'Y'){
								if(typeof MathJax !== 'undefined'){
									MathJax.Hub.Queue(["Typeset",MathJax.Hub]);
								}
								else{
									loadMathjax();
								}
							}
						
						
					});
					
					if(tabName != 'currentissue' && tabName != 'firstview' && tabName != 'mostcited'){
					cache[tabName]=true;
					}
			}
				
	 		if(cache[tabName] != true){
	 			jQuery('.ajaxContainer').promise().done(function() {
	 				window.setTimeout('reloadAltmetric()', 1000);
	 			});	
	 		}
		}
	}
	function changeOpenIssue(componentId) {
		jQuery('#ajaxContainer-openissue').html('<img align="center" src="/images/ajax-loader.gif"/> Loading articles...');
		jQuery.get('/action/displayJournalTab;jsessionid=23C501298F8E57AF5861FA713D9F34E3.journals?' + new Date(),
			{jid:'JBS', tab:'openissue', compId:componentId},
			function(data){
				jQuery('#ajaxContainer-openissue').html(data);
				jQuery('#ajaxContainer-openissue .preview').click(previewEvent);
				
				jQuery('#ajaxContainer-openissue .commentbox').bind('click',function(){
					animations_commentEvent(this);
				});
				jQuery('#ajaxContainer-openissue .comment-bttn').bind('click',function(){
					return postCommentEvent(this);								
				});
				
			}
		);
	}
	jQuery(document).ready(function() {
		var override = 'Y';
	    var tabmenu=jQuery.cookies.get('JBS_journaltab');
	    var overrideTab = jQuery.cookies.get('JBS_overridetab');
	    if(null != tabmenu && tabmenu != 'null'){
			if(override == 'Y' && overrideTab == null){
		 		changeTab('firstview');
		 		window.location =String(window.location).replace(/#.*$/, "") + "#tab";
			}else{
				jQuery('.tb').removeClass('active');
				jQuery('#tab'+tabmenu).addClass('active');
				if('firstview' == 'openIssue'){
					changeTab('openissue');
				}
				else{
			    	changeTab(tabmenu);
				}
		 	}
		}else{
			if(false) {
				tabmenu = "backissue";
				jQuery('.tb').removeClass('active');
				jQuery('#tab'+tabmenu).addClass('active');
				changeTab(tabmenu);
			} else {
				changeTab('firstview');
			} 
		}
	});
	
	jQuery(document).ajaxStop(function() {
			setTimeout("reloadAltmetric()",1000);
			if('N' == 'Y'){
			setTimeout("loadMathjax()",2000);
			}
		//initializeComputation();	
		//this is to load mathjax after articles has been loaded -MRT	
	});
	
	function reloadAltmetric(){
		jQuery("div.altmetric-embed a").remove();
		_altmetric_embed_init();
	}
	
	function loadMathjax(){
		var script = document.createElement("script");
		  script.type = "text/javascript";
		  script.src  = "/js/mathjax/MathJax.js?config=TeX-AMS_HTML,default";

		  var config = 'MathJax.Hub.Startup.onload();';

		  if (window.opera) {script.innerHTML = config}
		               else {script.text = config}

		  document.getElementsByTagName("head")[0].appendChild(script);
		}
	// -->
	</script>
