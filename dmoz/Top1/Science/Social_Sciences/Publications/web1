	function facebookRegister(){
		facebookAuth("registration");
	}
	function facebookQuickRegister(){
		facebookAuth("quickRegistration");
	}
	function facebookLogin(){
		facebookAuth("login");
	}
	function facebookAuth(fromPage){
		FB.login(function(response) {
		    if (response.authResponse) {
		       //alert("user is logged in and granted some permissions.");
		       var token = response.authResponse.accessToken;
		           window.location = 'http://journals.cambridge.org/action/socialMediaAuth?media=fb&fromPage='+fromPage+'&token='+token;
		        // user is logged in and granted some permissions.
		        // perms is a comma separated list of granted permissions		      
		    } else {
		    	//alert("user is not logged in");
		      // user is not logged in
		    }
		 }, {scope: 'user_about_me,email,publish_actions'});
	}
	
</script>







