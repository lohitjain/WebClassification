<script>
$(function(){
	$(".sitemapbut1").click(function(){
		$(".sitemapbut").click();
	});
	$(".sitemapbut").toggle(function(){
			$(".sitemap").fadeIn();
			$(".sitemapbut").text("[-] Sitemap");
	},function(){
		$(".sitemap").hide();
		$(".sitemapbut").text("[+] Sitemap");
		$(".top123").click();
	});
});
</script>
