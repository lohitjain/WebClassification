	GL_getObjCss("Menu3b15ca2").visibility = "hidden";
	GL_getObjCss("Menu3b15ca3").visibility = "hidden";
	GL_getObjCss("Menu3b15ca4").visibility = "hidden";
}
function GL3b15ca_doTop(WMid,id) {
GLV_menuOn["3b15ca"].left = (GLV_menuXY["3b15ca"][id][0]-(-300))+"px";
GLV_menuOn["3b15ca"].top = (125)+"px";
}
function GL3b15ca_restoreMenu() {

}
function GL3b15ca_resetSubMenus() {
	var resetSubMenu=1;

	return resetSubMenu;
}

GLV_timeout_pointers[GLV_timeout_count] = "3b15ca";
GLV_timeout_count++;
// -->
	/*]]>*/
</script>

</head>
