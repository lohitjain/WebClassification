			<div id="spotlightnew">
				<h3>Alumni &amp; Student Spotlights</h3>
				<div id="button-container">
					<div id="leftmove" class="inactive-left" onClick="moveleft()"><br /></div>
					<div id="rightmove" class="active-right" onClick="moveright()"><br /></div>
				</div>
			</div>
            



            
            
			<ul id="spotlightcontainer">
            <script>
				function writespotlights(feed){
					var relatedNews = new Array();
					
					var count = Math.floor(Math.random()* feed.length);

					for (i = 0; i < feed.length; i++){
						count++;
						if(count == feed.length){
							count = 0;
						}
						relatedNews[count] = feed[count];
						var fulltext = feed[count].description;
						var imgurl = fulltext.substr(fulltext.indexOf("(END2)") + 6);
						var title = feed[count].title;
						
						
						var description = fulltext.substr(fulltext.indexOf("(END)") + 5, fulltext.indexOf("(END2)") - fulltext.indexOf("(END)") - 5);
						var pbody = fulltext.substr(0, fulltext.indexOf("(END)"));
						var thisLink = feed[count].link;
						
						var LI = document.createElement("LI");
						
						if(i > 1){
							LI.className = "spotlight";
							LI.style.display = "none";
						}
						else if (i ==1){
							LI.className = "spotlight right";
						}
						else{
							LI.className = "spotlight left";
						}
						
						var DIV = document.createElement("DIV");
						DIV.className = "spotlight-image";
						
						var Aimg = document.createElement("A");
						Aimg.setAttribute('href', thisLink);
						Aimg.className = "openlightwindow";
						
						var IMG = document.createElement("IMG");
						IMG.width = "54px";
						IMG.height = "54px";
						IMG.src = imgurl;
						
						LI.appendChild(DIV);
						DIV.appendChild(Aimg);
						Aimg.appendChild(IMG);
						
						var Atitle = document.createElement("A");
						Atitle.setAttribute('href', thisLink);
						Atitle.innerHTML = title;
						Atitle.className = "openlightwindow";
						
						var H2 = document.createElement("H2");
						LI.appendChild(H2);
						H2.appendChild(Atitle);
						
						var Pdescription = document.createElement("P");
						Pdescription.className = "description";
						Pdescription.innerHTML = description;
						LI.appendChild(Pdescription);
						
						var Pbody = document.createElement("P");
						Pbody.className = "clear-space";
						Pbody.innerHTML = pbody;
						
						var A = document.createElement("A");
						A.setAttribute('href', thisLink);
						A.innerHTML = "View More";
						A.className = "openlightwindow";
						
						var BR = document.createElement("BR");
						
						LI.appendChild(Pbody);
						Pbody.appendChild(BR);
						Pbody.appendChild(A);
						
						
						
						document.getElementById('spotlightcontainer').appendChild(LI);

					}
				}
            </script>
            <script src="http://webapp.stthomas.edu/homepage/json.js?callback=writespotlights&feed=http://stthomas.edu/engineering/spotlight/spotlight.xml" type="text/javascript"></script>
            
			
			</ul>
			<script type="text/javascript" language="javascript">
				var leftposition = 0;
				var UL = $('spotlightcontainer');
				var lis = UL.getElementsByTagName('LI');
				
				function moveright(){
					if(leftposition == lis.length - 2){
						
					}
					else{
						lis[leftposition].style.display = 'none';
						removeandaddfloat(lis[leftposition + 1], 'left')
						removeandaddfloat(lis[leftposition + 2], 'right')
						lis[leftposition + 2].style.display = '';
						leftposition++;
						
						if(leftposition == lis.length - 2){
							jscss('swap', $('rightmove'), 'active-right', 'inactive-right');
						}
						if(leftposition == 1){
							jscss('swap', $('leftmove'), 'active-left', 'inactive-left');
						}
					}
				}
				function moveleft(){
					if(leftposition == 0){
						
					}
					else{
						lis[leftposition + 1].style.display = 'none';
						removeandaddfloat(lis[leftposition - 1], 'left')
						removeandaddfloat(lis[leftposition], 'right')
						lis[leftposition - 1].style.display = '';
						leftposition--;
						if(leftposition == 0){
							jscss('swap', $('leftmove'), 'active-left', 'inactive-left');
						}
						if(leftposition == lis.length - 3){
							jscss('swap', $('rightmove'), 'active-right', 'inactive-right');
						}						
					}
				}
				function removeandaddfloat(object, endfloat){
					jscss('remove', object, 'left');
					jscss('remove', object, 'right');
					jscss('add', object, endfloat);
				}
				function jscss(a,o,c1,c2){
				  switch (a){
					case 'swap':
					  o.className=!jscss('check',o,c1)?o.className.replace(c2,c1): o.className.replace(c1,c2);
					break;
					case 'add':
					  if(!jscss('check',o,c1)){o.className+=o.className?' '+c1:c1;}
					break;
					case 'remove':
					  var rep=o.className.match(' '+c1)?' '+c1:c1;
					  o.className=o.className.replace(rep,'');
					break;
					case 'check':
					  return new RegExp('\\b'+c1+'\\b').test(o.className)
					break;
				  }
				}
			</script>
		</div>
        
