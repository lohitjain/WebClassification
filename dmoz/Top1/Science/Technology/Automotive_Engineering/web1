			$('#filters a').click(function(){
			var $this = $(this);
				if ( $this.hasClass('selected') ) {
				$(this).removeClass('selected');
				$container.isotope({ filter: '*' });
				return false;
				}
			  var selector = $(this).attr('data-option-value');
			  $container.isotope({ filter: selector });
			  $("#filters a").addClass("selected").not(this).removeClass("selected");
			  return false;
			});
			
	      
	    });

		
	$(document).ready(function(){
	// Fancybox		
			$(".video a").fancybox({
				'padding'		: 10,
				'autoScale'		: false,
				'transitionIn'	: 'fade',
				'transitionOut'	: 'none',
				'overlayOpacity': 0.9,
				'overlayColor'	: '#000000',
				'titleShow'		: false,
				'type'			: 'iframe'

			});
	
	});


