	  $(window).load(function(){
	    $('.flexslider').flexslider({
	      animation: "slide",
		slideToStart: 0,   
		slideshow: false, 
	      animationLoop: false,
	      itemWidth: 285,
	      itemMargin: 0,
	      minItems: 1,
	      maxItems: 3,
	      start: function(slider){
	        $('body').removeClass('loading');
	      }

	    }); 



	    
	    //change feed-items on rollover
	    $('.feed-item').mouseover(function(){
		    $("h1", this).css('color', '#ff6600');
	    	$("h3", this).css('color', '#ff6600');
	    	$(".play_video", this).attr('src','/templates/_img/home/play_over.gif');
	    })			     		  
	    $('.feed-item').mouseout(function(){
	    	$("h1", this).css('color', '#000');
	    	$("h3", this).css('color', '#000');
	    	$(".play_video", this).attr('src','/templates/_img/home/play_video.gif');
	    })
	  });
	</script>


	

