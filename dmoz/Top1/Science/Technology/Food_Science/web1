			<script type="text/javascript" language="javascript">
				function showSuggestions(field) {
					
					$.ajax({
						type: "POST",
						url: "handler.php",
						data: "action=showSuggestions&value="+field.value,
					
						success: function(msg){
							if(msg != "") {
								document.getElementById('ajax-suggestions').innerHTML = msg;
								showSuggestionsBox();
							}			
							
						}
					});
				}
				
				function hideSuggestions() {
					document.getElementById('ajax-suggestions').style.display = 'none';
				}
				
				function showSuggestionsBox() {
					document.getElementById('ajax-suggestions').style.display = 'block';
				}
								
			</script>			
