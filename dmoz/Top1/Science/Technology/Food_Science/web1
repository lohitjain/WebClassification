var b1dis = "<a href=\"";
var bdis = "\"></a><img src=\"";
var edis = " width=\"350\" height=\"303\" alt=\"Science of Cooking\" border=\"0\"]></a>";
var rnumb = "";
var img = "";

rnumb += Math.floor(Math.random()*7);
img = rnumb;
if (img == "7") {
document.write(b1dis+ "webcasts/cheese_cast.html" +bdis+ "images/main-cheese.gif\"" +edis);
}

if (img == "6") {
document.write(b1dis+ "webcasts/donut_cast.html" +bdis+ "images/main-donuts.jpg\"" +edis);
}
if (img == "5") {
document.write(b1dis+ "seasoning/index.html" +bdis+ "images/main-quiz.gif\"" +edis);
}
if (img == "4") {
document.write(b1dis+ "pickles/index.html" +bdis+ "images/main-pickle.gif\"" +edis);
}
if (img == "3") {
document.write(b1dis+ "candy/index.html" +bdis+ "images/main-taffy.jpg\"" +edis);
}
if (img == "2") {
document.write(b1dis+ "candy/index.html" +bdis+ "images/main-candy.jpg\"" +edis);
}
if (img == "1") {
document.write(b1dis+ "seasoning/index.html" +bdis+ "images/main-quiz.gif\"" +edis);
}
if (img == "0") {
document.write(b1dis+ "webcasts/bread_cast.html" +bdis+ "images/main-bread.gif\"" +edis);
}


