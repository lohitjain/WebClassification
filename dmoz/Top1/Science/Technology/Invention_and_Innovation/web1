function searchClick(){
var t = document.getElementById('txtSearch').value;
location.href='/search/node/' + t;
}
function doSearch(e){
  e = e || window.event;
  var code = e.keyCode || e.which;
  if(code == 13) searchClick(); 
}
</script>
