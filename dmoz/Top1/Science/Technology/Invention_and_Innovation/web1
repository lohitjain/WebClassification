                  slider: '.slider',
                  slide: '.slide',
                  addNav: false,
                  addPagination: true,
                  speed: 400 // ms.
          });
					// Centers tabs
					$('.carousel-tabs').wrap('<div class="carousel-tabs-wrap" />');
          /*//News Feed 
          $('.feedwrap ul').wrap('<div class="newsfeed" />');
          $('.newsfeed').jCarouselLite({
              vertical: true,
              hoverPause:true,
              visible: 3,
              auto:3000,
              speed:1000
          });*/
					//top button, animated scroll
					$('.top').click(function() {	
						$('html, body').animate({ scrollTop: 0 }, 600);
						return false;
					});
      });
    </script>

    
