<script type="text/javascript">
//<![CDATA[

var Page_ValidationActive = false;
if (typeof(ValidatorOnLoad) == "function") {
    ValidatorOnLoad();
}

function ValidatorOnSubmit() {
    if (Page_ValidationActive) {
        return ValidatorCommonOnSubmit();
    }
    else {
        return true;
    }
}
        Sys.Application.initialize();
Sys.Application.add_init(function() {
    $create(Sys.UI._UpdateProgress, {"associatedUpdatePanelId":"dnn_ctr546_Banners_UP","displayAfter":500,"dynamicLayout":true}, null, null, $get("dnn_ctr546_Banners_UP_Prog"));
});

document.getElementById('dnn_ctr536_NewletterSignUp_RequiredFieldValidator1').dispose = function() {
    Array.remove(Page_Validators, document.getElementById('dnn_ctr536_NewletterSignUp_RequiredFieldValidator1'));
}

document.getElementById('dnn_ctr536_NewletterSignUp_RegularExpressionValidator1').dispose = function() {
    Array.remove(Page_Validators, document.getElementById('dnn_ctr536_NewletterSignUp_RegularExpressionValidator1'));
}
Sys.Application.add_init(function() {
    $create(AjaxControlToolkit.TextBoxWatermarkBehavior, {"ClientStateFieldID":"dnn_ctr536_NewletterSignUp_TextBoxWatermarkExtender1_ClientState","WatermarkText":"Enter email","id":"dnn_ctr536_NewletterSignUp_TextBoxWatermarkExtender1"}, null, null, $get("dnn_ctr536_NewletterSignUp_txtEMail"));
});
Sys.Application.add_init(function() {
    $create(Sys.UI._UpdateProgress, {"associatedUpdatePanelId":"dnn_ctr536_NewletterSignUp_UP","displayAfter":500,"dynamicLayout":true}, null, null, $get("dnn_ctr536_NewletterSignUp_UP_Prog"));
});
//]]>
</script>
</form>
</body>
</html>
