<script type="text/javascript">
    function showLogin() {
        var x = document.getElementById("dimmer");
        x.style.display = "block";
        var y = document.getElementById("tblLogin");
        y.style.display = "table";
    }
    function hideLogin() {
        var x = document.getElementById("dimmer");
        x.style.display = "none";
        var y = document.getElementById("tblLogin");
        y.style.display = "none";
    }
</script>

