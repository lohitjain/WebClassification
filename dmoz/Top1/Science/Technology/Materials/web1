            	function updateCSS(e) {
            		// requires jQuery
            		$(document).ready(function(){
            			if (!e.success) {		
            				dept = "mse"; 
            				if (dept == "cee" || dept == "mae") {
            					$("#identity").css({ "height" : "236px" });
            					$("#header").css({ "height" : "236px" });
            				}
            				if (dept == "ece") {
									$("#intro-text .CS_Textblock_Caption").css({
										"left" : 0,
										"top" : -115
										});
									$("#intro-text .CS_Textblock_Caption").html("Welcome to <span>ECE</span>");
            				}
            				else {
            					$("#intro-text .CS_Textblock_Caption").css({
										"left" : 0,
										"top" : -48
										});
            				}
            			}
            		});
            	}
            </script>


            
