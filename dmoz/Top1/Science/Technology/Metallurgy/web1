    <script type="text/javascript">
        function showhide(show) {
            var elementname = "detail" + show;
            var pgname = "p" + show;
            var i = 1;
            for (i = 1; i < 7; i++) {
                if (i == show) {
                    document.getElementById(elementname).style.visibility = "visible";
                    document.getElementById(elementname).style.display = "block";
                    document.getElementById(pgname).style.color = "#5f5f5f";
                }
                else {
                    var element = "detail" + i;
                    var pg = "p" + i;
                    document.getElementById(element).style.visibility = "hidden";
                    document.getElementById(element).style.display = "none";
                    document.getElementById(pg).style.color = "#adadad";
                }
            }
        }
        function mouseover(show, hide1, hide2, showimg, hideimg1, hideimg2) {
            var showname = show + "div";
            var hidename1 = hide1 + "div";
            var hidename2 = hide2 + "div";

            var show_imgelement = show + "img";
            var hide1_imgelement = hide1 + "img";
            var hide2_imgelement = hide2 + "img";


            document.getElementById(hidename1).style.visibility = 'hidden';
            document.getElementById(hidename1).style.display = 'none';
            document.getElementById(hidename2).style.visibility = 'hidden';
            document.getElementById(hidename2).style.display = 'none';
            document.getElementById(showname).style.visibility = 'visible';
            document.getElementById(showname).style.display = 'block';

            document[show_imgelement].src = 'images/' + showimg;
            document[hide1_imgelement].src = 'images/' + hideimg1;
            document[hide2_imgelement].src = 'images/' + hideimg2;
        }
    </script>

