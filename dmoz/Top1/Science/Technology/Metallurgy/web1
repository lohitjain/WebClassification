    <script type="text/javascript">
        $(function() {
            $('#container-1 > ul').tabs();
        });

        function showComingSoon() {
            document.getElementById('comingsoonlight').style.display = 'block';
            document.getElementById('fade').style.display = 'block';
        }
        function show(rownumber) {
            if (document.getElementById("record" + rownumber).style.display == '' || document.getElementById("record" + rownumber).style.display == 'block') {
                $("#record" + rownumber).fadeOut('slow', function() {
                    $("#recordcolspan" + rownumber).hide();
                    document.getElementById("img" + rownumber).src = 'http://www.corrosionsource.com//images/ques-open.jpg';
                    document.getElementById("img" + rownumber).alt = "Show";
                });

                if (document.getElementById("tag" + rownumber) != null) {
                    $("#tag" + rownumber).fadeOut('slow', function() {
                        $("#tagcolspan" + rownumber).hide();
                    });
                }

                if (document.getElementById("cloud" + rownumber) != null) {
                    $("#cloud" + rownumber).fadeOut('slow', function() {
                        $("#cloudcolspan" + rownumber).hide();
                    });
                }
            }
            else {
                $("#record" + rownumber).fadeIn('slow', function() {
                    $("#recordcolspan" + rownumber).show();
                    document.getElementById("img" + rownumber).src = 'http://www.corrosionsource.com/images/que-close.jpg';
                    document.getElementById("img" + rownumber).alt = "Hide";
                });

                if (document.getElementById("tag" + rownumber) != null) {
                    $("#tag" + rownumber).fadeIn('slow', function() {
                        $("#tagcolspan" + rownumber).show();
                    });
                }

                if (document.getElementById("cloud" + rownumber) != null) {
                    $("#cloud" + rownumber).fadeIn('slow', function() {
                        $("#cloudcolspan" + rownumber).show();
                    });
                }
            }
        }

        function HideThis(rownumber) {

            $("#record" + rownumber).fadeOut('slow', function() {
                $("#recordcolspan" + rownumber).hide();
                document.getElementById("img" + rownumber).src = 'http://www.corrosionsource.com/images/ques-open.jpg';
                document.getElementById("img" + rownumber).alt = "Show";
            });

            if (document.getElementById("tag" + rownumber) != null) {
                $("#tag" + rownumber).fadeOut('slow', function() {
                    $("#tagcolspan" + rownumber).hide();
                });
            }

            if (document.getElementById("cloud" + rownumber) != null) {
                $("#cloud" + rownumber).fadeOut('slow', function() {
                    $("#cloudcolspan" + rownumber).hide();
                });
            }
        }
    </script>

