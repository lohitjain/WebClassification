    <script type="text/javascript" language="javascript">

        function OnFocus(elementId, defaultText) {
            if (document.getElementById(elementId).value == defaultText) {
                document.getElementById(elementId).className = "normal";
                document.getElementById(elementId).value = "";
            }
        }

        function OnBlur(elementId, defaultText) {
            var textValue = document.getElementById(elementId).value;

            if (textValue == defaultText || textValue.length == 0) {
                document.getElementById(elementId).className = "watermark";
                document.getElementById(elementId).value = defaultText;
            }
            else
                document.getElementById(elementId).className = "normal";
        }

        function ShowAddDialog() {
            $find('ctl00_loginWebDialogWindow').set_windowState($IG.DialogWindowState.Normal);
        }

        function HideDialog() {
            $find('ctl00_loginWebDialogWindow').set_windowState($IG.DialogWindowState.Hidden);
        }

        function ValidateValues() {

            var loginText = document.getElementById('ctl00_loginWebDialogWindow_tmpl_UserName');
            var pwdText = document.getElementById('ctl00_loginWebDialogWindow_tmpl_Password');
            var failureText = document.getElementById('ctl00_loginWebDialogWindow_tmpl_FailureText');

            if ((loginText.value.length == 0) || (pwdText.value.length == 0)) {
                alert("Enter appropriate login credentials.");
                return false;
            }
            return true;
        }
        function ValidateSearchQuery() {

            var queryText = document.getElementById('ctl00_query');

            if (queryText.value.length == 0) {
                return false;
            }
            return true;
        }

        function ValidateNewUser() {

            var newUserIDText = document.getElementById('ctl00_newUserID');
            var newUserPasswordText = document.getElementById('ctl00_newUserPassword');
            var newUserVerifyPasswordText = document.getElementById('ctl00_newUserVerifyPassword');
            var newUserEmailText = document.getElementById('ctl00_newUserEmail');
            var secretquestionText = document.getElementById('ctl00_secretquestion');
            var secretAnswerText = document.getElementById('ctl00_secretAnswer');
            var acceptTermsChecker = document.getElementById('ctl00_acceptTerms');

            if ((newUserIDText.value.length == 0) || (newUserPasswordText.value.length == 0) ||
                (newUserVerifyPasswordText.value.length == 0) || (newUserEmailText.value.length == 0) ||
                (secretquestionText.value.length == 0) || (secretAnswerText.value.length == 0) ||
                (acceptTermsChecker.value == false)) {

                alert("Enter appropriate details. All fields are compulsory");

                return false;
            }
            return true;
        }
    </script>

    
