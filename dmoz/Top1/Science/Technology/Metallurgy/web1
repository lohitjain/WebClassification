<script>
    $(window).load(function () {
        
        $('#home-features').cycle({
            pager:'#feature-nav ul',
            //pagerAnchorBuilder: function(idx, slide) {return '<li><a href="#"> </a></li>';},
            pause:true,
            slideResize:false,
            timeout: 5000,
            //before: function (curr, next, opts) {
                //$('#feature-counter').html(opts.nextSlide+1 + " of " + opts.slideCount);
             after: function(currSlideElement, nextSlideElement, options, forwardFlag) {
                $('#feature-counter').text((options.currSlide + 1) + ' of ' + (options.slideCount));
             },
            prev: '#control-previous',
            next: '#control-next'
        });
        
        $('#control-pause').click(function(event) {
            $('#home-features').cycle('pause');
            $('#control-play').show();
            $(this).hide();
        
            return false; 
        });
        
        $('#control-play').click(function(event) { 
            $('#home-features').cycle('resume');
            $('#control-pause').show();
            $(this).hide();

            return false;   
        });
        
    });
</script>

