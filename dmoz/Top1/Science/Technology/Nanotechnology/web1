            setTimeout((function sliiide() {
                var s = (window.getComputedStyle) ? parseInt(getComputedStyle(sliderHolder).right) : parseInt(sliderHolder.currentStyle.right);
                if (s <= 0) {
                    sliderHolder.style.right = (s + 6) + 'px';
