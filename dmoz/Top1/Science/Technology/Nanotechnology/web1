  mm_menu_0101010416_0.addMenuItem("SPPE&nbsp;Technology&nbsp;Licensing","window.open('LS_1.htm', '_self');");
  mm_menu_0101010416_0.addMenuItem("Customization&nbsp;&&nbsp;Integration&nbsp;Services","window.open('LS_2.htm', '_self');");
   mm_menu_0101010416_0.hideOnMouseOut=true;
   mm_menu_0101010416_0.bgColor='#555555';
   mm_menu_0101010416_0.menuBorder=1;
   mm_menu_0101010416_0.menuLiteBgColor='#FFFFFF';
   mm_menu_0101010416_0.menuBorderBgColor='#777777';

mm_menu_0101010416_0.writeMenus();
} // mmLoadMenus()
			//rollover script
			function swapImage(obj) {
				if (obj.src.indexOf('_on') > -1) {
					obj.src = obj.src.substring(0,obj.src.indexOf('_on'))+'_off'+obj.src.substring(obj.src.lastIndexOf('.'));
				} else if (obj.src.indexOf('_off') > -1) {
					obj.src = obj.src.substring(0,obj.src.indexOf('_off'))+'_on'+obj.src.substring(obj.src.lastIndexOf('.'));
				}
			}
</script>
