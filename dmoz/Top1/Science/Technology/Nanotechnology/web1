"<a href=\"http://f1000.com/posters\"><img src=\"http://api.ning.com:80/files/Go*nQJ2dIT8PPiqs67jr5uLSQs1oggYrSa3sfXMIIwJMXOfbl9fMLd1k7FG0DMJQ8SMEtIXbpMJW8VlOCYmZpTscvgyVHUNJ/ad_Posters_728x90.jpg\" width=\"468\" height=\"60\" alt=\"\" border=\"0\"><\/a>",
"<a href=\"http://www.nanopaprika.eu/page/partner-webpages-of-the\"><img src=\"\"http://api.ning.com/files/4IAmTFQW5ssW8kTgmzToy9CQVOqv9lmiN1XEJKYZAGhnITVGK9JM*QGHXQzOSjIXfZ6pwAmrk4PeDnfKxmPVGYOk*2yU8oaC/partner486.jpg\" width=\"468\" height=\"60\" alt=\"Click Here\" border=\"0\"><\/a>"
]
randomNumber = Math.random()
var show_mybanners = mybanners[Math.floor(randomNumber * mybanners.length)]
document.write(show_mybanners);
// -->
</script>
