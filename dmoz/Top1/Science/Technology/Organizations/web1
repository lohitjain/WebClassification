                    <h3>
                        Login</h3>
                    <div class="formContainer">
                        Email
                        <input name="ctl11$Header$txtUser" type="text" id="ctl11_Header_txtUser" />
                        &nbsp; Password
                        <input name="ctl11$Header$txtPwd" type="password" id="ctl11_Header_txtPwd" />
                    </div>
                    &nbsp;&nbsp;<input type="image" name="ctl11$Header$ibtnLogin" id="ctl11_Header_ibtnLogin" src="/Images/btn_signin.png" style="border-width:0px;" />
                    <span id="ctl11_Header_rfvUser" controlToValidateType="TextBox" headingControlId="ctl11_Header_heading_txtUser" messageControlId="ctl11_Header_message_rfvUser" validatorType="RequiredFieldValidator" style="color:Red;display:none;"></span>
                    <span id="ctl11_Header_rfvPassword" controlToValidateType="TextBox" headingControlId="ctl11_Header_heading_txtPwd" messageControlId="ctl11_Header_message_rfvPassword" validatorType="RequiredFieldValidator" style="color:Red;display:none;"></span>
