                <div id="main-content">

                  
                  
                                      <div id="content">
                      <div id="block-system-main" class="block block-system no-title" >  
  
  <article id="node-333" class="node node-page node-promoted node-teaser article iat-l clearfix" about="/hindu" typeof="foaf:Document" role="article">
  <div class="node-inner">

      
          <header class="node-header">
                  <h1 property="dc:title" datatype="" class="node-title" rel="nofollow">
            <a href="/hindu" rel="bookmark">M.S/Ph.d Advertisement</a>
          </h1>
              </header>
      
      
    <div class="node-content">
      <div class="field field-name-body field-type-text-with-summary field-label-hidden view-mode-teaser view-mode-teaser"><div class="field-items"><div class="field-item even" property="content:encoded"><p><a href="https://research.iitm.ac.in/"><img src="/dpaf/hindu.jpg" alt="hindu" title="M.S/Ph.d Advertisement" /></a></p>
