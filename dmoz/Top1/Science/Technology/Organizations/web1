//<![CDATA[
 function filtrare()
 {
   var paglinkh='?searh=da';
   var editura=document.getElementById('edituraf').value; if(editura!='') paglinkh=paglinkh+'&editura='+editura;
   var categorie=document.getElementById('categorie').value; if(categorie!='') paglinkh=paglinkh+'&categorie='+categorie;
   var titlu=document.getElementById('titlu_cautare').value; if(titlu!='') paglinkh=paglinkh+'&titlu_cautare='+titlu;
   $("#filtru").load("/includes/left/cautare_carti1.php"+paglinkh);
 }
//]]>
</script>
