<script type="text/javascript">
//<![CDATA[

					function DoSearch()
					{
						var elem = document.getElementById('ctl00_ctrlCommonFunctions_InputKeywords');
						if (elem) 
						{
							// window.location = '/en/Sidor/search.aspx?k=' + encodeURI(elem.value);
							location.replace('/en/Sidor/search.aspx?k=' + encodeURI(elem.value));
							return false;
						}
					}			

					function SearchIfEnter(e)
					{
						var keycode;

						if (window.event) 
												keycode = window.event.keyCode;
						else if (e) 
												keycode = e.which;
						else 
												return true;

						if (keycode == 13)
						{
							DoSearch();
							try {if(null != event) event.returnValue = false;} catch (err) {}
							return false;
						} 
						   
						return true;
					}var g_defaultSearchText = 'Search';
                        function ClearInputKeywords(elementId)
                        {
                            if( elementId == undefined || elementId == '')
                                return;

                            var elem = document.getElementById(elementId);
                            if(g_defaultSearchText == elem.value)
                            {
                                elem.value = '';
                                elem.style.color = '#000';
                            }
                            else if(elem.value == '')
                            {
                                elem.value = g_defaultSearchText;
                                elem.style.color = '#b3b3b3';
                            }
                        }
                    //]]>
</script>
