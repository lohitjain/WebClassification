            <input id="callbackForAutoSave_ParamField" name="callbackForAutoSave_ParamField" type="hidden" value="" /><div id="callbackForAutoSave" style="overflow:auto;"></div><script type="text/javascript">
//<![CDATA[
/*** ComponentArt.Web.UI.CallBack 2011_1_1124_35 callbackForAutoSave ***/
window.ComponentArt_Init_callbackForAutoSave = function() {
if(!window.ComponentArt_CallBack_Loaded)
	{setTimeout('ComponentArt_Init_callbackForAutoSave()', 50); return; }

window.callbackForAutoSave = new ComponentArt_CallBack('callbackForAutoSave');
callbackForAutoSave.CallbackPrefix = 'https://www.ashrae.org/';
callbackForAutoSave.CallbackParamDelimiter = '?';
callbackForAutoSave.ClientEvents = null;
callbackForAutoSave.LoadingPanelFadeDuration = 0;
callbackForAutoSave.LoadingPanelFadeMaximumOpacity = 100;
callbackForAutoSave.Parameter = '';
callbackForAutoSave.Postback = function() { __doPostBack('callbackForAutoSave','') };
callbackForAutoSave.PostState = 1;
callbackForAutoSave.Initialize();

}
ComponentArt_Init_callbackForAutoSave();

//]]>
