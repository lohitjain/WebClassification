<script language="javascript" type="text/javascript">
    jQuery.redirect = function (obj, url, params) {
        url = url || window.location.href || '';
        url = url.match(/\?/) ? url : url + '?';

        for (var key in params) {
            var re = RegExp(';?' + key + '=?[^&;]*', 'g'); //RegExp('[;&]' + key + '=?[^&;]*', 'g')

            url = url.replace(re, '');
            url += '&' + key + '=' + params[key];
        }
        url = url.replace(/[;&]$/, '');
        url = url.replace(/\?[;&]/, '?');
        url = url.replace(/[&]{2}/g, '&');
        url = url.replace(/[;&]{2}/g, ';');
        obj.href = url;
    };
    $(document).ready(function () {
        $("a[href*='techstreet.com']").each(function () {
            var token = '';
            //if(token != '')
            $.redirect(this, this.href, { ashrae_auth_token: token });
        });
    });

</script>


<script type="text/javascript">
    var refreshIntervalId;
    jQuery(document).ready(function () {
        jQuery("#content1").addClass("active");
        jQuery("#content1").css("display", "block");
        jQuery("#button1").parent().parent().attr('class', 'on');
        jQuery("#currentTopicId").val("content1");
        refreshIntervalId = setInterval(function () { swapImages(); }, 10000);
    });
    function switchContent(id, contentId, buttonId) {
        clearInterval(refreshIntervalId);
        $('#LinkNumbers a').each(function () {
        //    $(this).attr('class', '');
            $(this).parent().parent().attr('class', '');
        });
      //  $('#' + buttonId).attr('class', 'activeSlide');
        $('#' + buttonId).parent().parent().attr('class', 'on');
        var currentTopicId = jQuery("#currentTopicId").val();
        //jQuery("#" + currentTopicId).hide('slide');
        jQuery("#" + currentTopicId).fadeOut('slow');
        jQuery("#" + currentTopicId).css("display", "none");
        jQuery("#" + currentTopicId).removeClass('active');
        jQuery("#" + contentId).fadeIn('slow');
       // jQuery("#" + contentId).show('slide');
        jQuery("#" + contentId).css("display", "block");
        jQuery("#" + currentTopicId).addClass('active');
        jQuery("#currentTopicId").val(contentId);
        //refreshIntervalId = setInterval(function () { swapImages(); }, 2000);
    }
    function swapImages() {
        var $active = $('#BannerImages .active');
        var $next = ($('#BannerImages .active').next().length > 0) ? $('#BannerImages .active').next() : $('#BannerImages #content1');
        $active.fadeOut(function () {
            $active.css('display', 'none');
            $active.removeClass('active');
            $next.fadeIn().addClass('active');
            $next.css('display', 'block');
        });

//        $active.hide('slide');
//            $active.css('display', 'none');
//            $active.removeClass('active');
//            $next.show('slide').addClass('active');
//            $next.css('display', 'block');
    
        var id = $next.attr("id");
        var buttonId = id.replace('content', 'button');
        jQuery("#currentTopicId").val(id);
        $('#LinkNumbers a').each(function () {
      //      $(this).attr('class', '');
            $(this).parent().parent().attr('class', '');
     
        });
      //  $('#' + buttonId).attr('class', 'activeSlide');
        $('#' + buttonId).parent().parent().attr('class', 'on');
  
    }
    </script>
