     <!-- slider content -->
   <div class="slider clear-block border">
    <div class="slider-inner clear-block border">
                  <div class="slide clear-block border">
        <div class="slide-inner clear-block border">
        <img src="http://www.eu-japan.eu/sites/eu-japan.eu/files/imagecache/300x200/wcm.jpg" alt="Activities &amp;amp; Mission  " title=""  class="imagecache imagecache-300x200" width="300" height="200" />        
         <div class="slide-text slide-text-vertical slide-text-left clear-block border">
