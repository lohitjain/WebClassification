	this.sub_expand_image_offy = 'undefined'






   /*---------------------------------------------
   Global Menu Styles
   ---------------------------------------------*/

	//Main Menu

	this.main_container_styles = "background-color:#008000; border-style:solid; border-color:#008000; border-width:1px; padding:0px; "
	this.main_item_styles = "background-color:#c0c0c0; color:#333333; text-align:center; font-family:Arial; font-size:12px; font-weight:bold; text-decoration:none; border-style:none; border-color:#000000; border-width:0px; padding:2px 5px; "
	this.main_item_hover_styles = "background-color:#008000; color:#ffffff; text-decoration:underline; "
	this.main_item_active_styles = ""



	//Sub Menu

	this.subs_container_styles = "background-color:#e6ede7; border-style:solid; border-color:#008000; border-width:6px; padding:5px; margin:0px; "
	this.subs_item_styles = "color:#111111; text-align:left; font-size:11px; font-weight:normal; text-decoration:none; border-style:none; border-color:#000000; border-width:1px; padding:2px 5px; "
	this.subs_item_hover_styles = "background-color:#ec9d68; color:#ffffff; "
	this.subs_item_active_styles = ""



}</script>


