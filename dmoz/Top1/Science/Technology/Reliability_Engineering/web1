function verifyRequired3281() {
  if (icpForm3281["fields_fname"].value == "") {
    icpForm3281["fields_fname"].focus();
    alert("The First Name field is required.");
    return false;
  }
  if (icpForm3281["fields_lname"].value == "") {
    icpForm3281["fields_lname"].focus();
    alert("The Last Name field is required.");
    return false;
  }
  if (icpForm3281["fields_email"].value == "") {
    icpForm3281["fields_email"].focus();
    alert("The Email field is required.");
    return false;
  }

    if (
      !icpForm3281["listid:13579"].checked &&
      !icpForm3281["listid:15379"].checked &&
      true)  {
    alert("The Lists field is required.");
    return false;
  } 

return true;
