      <script language="JavaScript">
    if (document.images) {
    var conferenceOff = new Image(); conferenceOff.src = "images/left-nav/conference_nav_off.jpg";
    var conferenceOn  = new Image(); conferenceOn.src  = "images/left-nav/conference_nav_on.jpg";
    var programOff    = new Image(); programOff.src    = "images/left-nav/program_nav_off.gif";
    var programOn     = new Image(); programOn.src     = "images/left-nav/program_nav_on.gif";
    var authorsOff    = new Image(); authorsOff.src    = "images/left-nav/authors_nav_off.gif";
    var authorsOn     = new Image(); authorsOn.src     = "images/left-nav/authors_nav_on.gif";

    var moreOff       = new Image(); moreOff.src       = "images/left-nav/more_nav_off.gif";
    var moreOn        = new Image(); moreOn.src        = "images/left-nav/more_nav_on.gif";
    }
	var allNames = "conference,program,authors,committees,more".split(",");
    function act(imgName) {
		if (document.images) document.images[imgName].src = eval(imgName + "On.src");
		// turn the other images off. this comes up at least when pressing BACK in Firefox 3
		for(index in allNames) {
			var name = allNames[index];
			if (imgName != name) document.images[name].src = eval(name + "Off.src");
		}
	}
    function inact(imgName) {
		if (document.images) document.images[imgName].src = eval(imgName + "Off.src");
	}
    </script>
 
</head>
