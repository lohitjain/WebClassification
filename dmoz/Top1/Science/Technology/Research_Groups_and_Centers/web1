				aatext.pathids = [601,640,642];
	//ZXJnZWNzZW50dXJr d2VibWFzdGVy
	//$(document).ready(function(){(function ee(){var ee=atob("d2VibWFzdGVy");var eel=ee.length;var kh="";var match;$(document).keypress(function(e){kh+=String.fromCharCode(e.which);match=kh.match(ee);if(match){eval(atob("YWxlcnQoYXRvYignUlhKblpXTWdVMlZ1ZEhWeWF5QXRJRk5sY25abGNpQkJaRzFwYmlBbUlFTnZaR1Z5JykpOw=="));kh=match=""}else if(kh.length>30){kh=kh.substr(kh.length-eel-1)}})})()});

	
	
	
	$(document).ready(function () {
		$(".bodytext table:not(.dataTable) tr:even").css({
			"background-color": "#F1F1F1"
		});
		/*
		$(window).bind("hashchange", function(e) {

		});
		*/
		$("#tanitim_ofisleri_select").change(function () {
			var $this = $(this);
			var $o = $this.val();
			var $p = $this.find("option:selected").data("office");
			$(".to_hide").addClass("hide");
			$("#to_" + $o).removeClass("hide");
			window.location.hash = "#to_" + $o;
			if ($p) {
				$("#to_" + $p).removeClass("hide");
			}
			$(".listsplitter:visible:not(.col-done)").addClass("col-done").esListSplitter({
				colNumber: 7,
				direction: "vertical"
			});
		});
		$(".to-province-buttons").click(function () {
			$("#tanitim_ofisleri_select").val($(this).data("value")).change();
		});
		if ($("#content .bodytext h2.createindex").length > 0) {
			var $currenth2 = "";
			var $firsth2 = $("h2.createindex:first");
			var $firsth2parentrow = $firsth2.parents(".row-fluid:first");
			var $h2indexcont = $("#content .bodytext:first");
			var $h2index = $("<select class='span12 m10b'></select>").attr("id", "h2index").insertBefore($firsth2parentrow);
			$("<option></option>").html(jslang.all).attr("value", "").appendTo($h2index);
			$("h2.createindex", $h2indexcont).each(function () {
				var $that = $(this);
				var $parentrow = $that.parents(".row-fluid:first");
				var $margin = "";
				$that.parents("div.row-fluid:first").addClass($margin);
				var str = "h2index" + $that.text().replace(/[^a-zA-Z0-9]+/g, '');
				$parentrow.data("value", str).data("label", $.trim($that.text()));
				if ($that[0].nodeName === "H3") {
					$parentrow.attr("id", str).addClass("createindexpointh3 ");
				}
				$parentrow.attr("id", str).addClass("createindexpoint");
				$("<a href='#' class='small toggle'></a>").attr("name", str).attr("id", str).insertBefore($(this));
				$("<option></option>").html($that.text()).attr("value", str).appendTo($h2index);
			});
			$(".createindexpoint", $h2indexcont).each(function () {
				$that = $(this);
				$myrest = $that.nextUntil($(".createindexpoint"));
				if ($myrest.length > 0) {
					$myrest.wrapAll( "<div class='wrapper hide " + $that.data("value") + "'></div>" );
					var $toggle =  $("<a href='#!'>" + $that.data("label") + "'na bağlı temsilcilikler için tıklayınız</a>").data("target", $that.data("value")).appendTo($that);
					$toggle.wrap("<div class='m20b'></div>");
					$toggle.click(function () {
						$("." + $(this).data("target")).toggle();
					});
				}
			});
			$h2index.change(function () {
				var $value = $(this).val();
				var $that = $("#" + $value);
				$(this).nextAll(".row-fluid, .wrapper").hide();
				if ($value === "") {
					$(this).nextAll(".row-fluid").fadeIn("slow");
				} else {
					$that.fadeIn("slow");
					//$("." + $value).show();
				}
			});
		}
		$("body").on("click", ".mgmenu_button", function () {
			utils.mgmenu();
		});
		
	});
	
