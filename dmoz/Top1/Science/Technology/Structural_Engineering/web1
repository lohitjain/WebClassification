	<!-- Script for the drop down menu-->
				<script type="text/javascript">
					 $(document).ready(function(){  
					   	var timeout    = 500;
						var nav_delay = 10000;
						var closetimer = 0;
						var ddmenuitem = 0;						

						function main_menu_open()
						{  
						   main_menu_canceltimer();
						   $(this).find('div:first').css('visibility', 'visible');
						   main_menu_close();
						   ddmenuitem = $(this).find('ul:first').css('visibility', 'visible');						   
						   $('li').bind('mouseover', childHover);
						   $('li').bind('mouseout', hoverOutTime);
						  }
						    
						  function childHover()
						  {
						     $('li').mouseover(function(){
							        $(this).find('div:first').css('visibility', 'visible');
									$(this).find('ul:first').css('visibility', 'visible');
							 })
						  }
						  function hoverOutTime()
						  {
						         for(var i=0;i<nav_delay;i++)
								 {
								 }
								 childHoverout();
						  }
						  function childHoverout()
						  {
						     $('li').mouseout(function(){
							       
							        $(this).find('div:first').css('visibility', 'hidden');
									$(this).find('ul:first').css('visibility', 'hidden');
							 })
						  }

						function main_menu_close()
						{  
							if(ddmenuitem) {
								ddmenuitem.css('visibility', 'hidden');
								
							}
						
						}

						function main_menu_timer()
						{  closetimer = window.setTimeout(main_menu_close, timeout);}

						function main_menu_canceltimer()
						{  if(closetimer)
						   {  window.clearTimeout(closetimer);
							  closetimer = null;}}

						$(document).ready(function()
						{  
						   $('#main_menu > li').bind('mouseover', main_menu_open)
						   $('#main_menu > li').bind('mouseout',  main_menu_timer)});
						   
						   document.onclick = main_menu_close;
					 }); 
				
				
				
				</script>
				
				
				
