</a>
<script type="text/javascript" language="javascript">
  imageLinkPrefix = "/images/banners/";
  bannerImages    = new Array("Ramjackbanner.gif","ramadv.gif","jobsbanner.gif","Ramjackbanner.gif","ramssd.gif","SEsite.gif","Ramjackbanner.gif","ramcon.gif");
  bannerTitles    = new Array("Arizona RAM Jack","RAM Advanse","SEsite JOB Postings","Arizona RAM Jack","RAM Structural System","SEsite","Arizona RAM Jack","RAM Concept");
  urlLinkPrefix   = "http://www.";
  bannerURLs      = new Array("azrj.com","ramint.com","sesite.com/jobs","azrj.com","ramint.com","sesite.com/forms/AdForm2e.pdf","azrj.com","ramint.com");
  numSeconds      = 3;

  var d=new Date();
  var thisBanner=d.getSeconds() % bannerImages.length;
  var isIE = (navigator.appName == "Microsoft Internet Explorer");

  function rotateBanners(){
    document.id_bannerImage.src = imageLinkPrefix + bannerImages[thisBanner];
    document.id_bannerImage.title = bannerTitles[thisBanner];
    if (!isIE) {
      document.anchors["id_bannerURL"].href = urlLinkPrefix + bannerURLs[thisBanner];
      document.anchors["id_bannerURL"].title = bannerTitles[thisBanner];
    }
    else {
      id_bannerURL.href = urlLinkPrefix + bannerURLs[thisBanner];
      id_bannerURL.title = bannerTitles[thisBanner];
    }
    setTimeout("rotateBanners()", numSeconds*1000);
    thisBanner = (thisBanner + 1) % bannerImages.length;
  }

  if (isIE) {
    window.onload = rotateBanners;
  }
  else {
    window.onload = rotateBanners();
  }
</script>
