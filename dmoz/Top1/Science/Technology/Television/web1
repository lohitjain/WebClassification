		<td width="105" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">+6.5MHz </font></b><!--mstheme--></font></td>
		<td width="124" align="center" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">Pos </font></b><!--mstheme--></font></td>
		<td width="71" align="center" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">Yes </font></b><!--mstheme--></font></td>
	</tr>
