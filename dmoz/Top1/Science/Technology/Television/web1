		<td width="105" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">25 </font></b><!--mstheme--></font></td>
		<td width="82" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">625 </font></b><!--mstheme--></font></td>
		<td width="99" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">UHF </font></b><!--mstheme--></font></td>
		<td width="105" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">+6.5MHz </font></b><!--mstheme--></font></td>
		<td width="124" align="center" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">Neg </font></b><!--mstheme--></font></td>
		<td width="71" align="center" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">Yes </font></b><!--mstheme--></font></td>
	</tr>
	<tr>
		<th width="98" bgcolor="#808080"><!--mstheme--><font face="Arial">L
 		</th>
		<td width="105" bgcolor="#808080"><!--mstheme--><font face="Arial"><b><font size="2">25 </font></b><!--mstheme--></font></td>
