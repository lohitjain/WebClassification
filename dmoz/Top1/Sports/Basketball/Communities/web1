<script type="text/javascript">
function validZip() 
{
	checked = true;
	if(theForm.LodgeZipCode.value == '' || isNaN(theForm.LodgeZipCode.value) || theForm.LodgeZipCode.value.length < 5)
	{
		alert("Please provide a valid ZIP code.");
		theForm.LodgeZipCode.focus();
		checked = false;
	}
	
	if(checked)
		theForm.submit();
	return checked;
}
</script>
