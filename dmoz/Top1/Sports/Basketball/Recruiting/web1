<script>
var sBasketballScheduleSelected = 'basketballschedulenov';
function ChangeBasketballScheduleBackGround(stype, obj){
	if (obj.id != sBasketballScheduleSelected){
		if (stype == 'on')
			obj.style.backgroundColor = '#999999';
		else
			obj.style.backgroundColor = '#CBCBCB';
	}
}
function ChangeBasketballScheduleDisplay(stype){
	var obj = null;
	var obj2 = null;
	if (stype == 'mar'){
		obj = document.getElementById('basketballscheduletablenov');
		obj2 = document.getElementById('basketballschedulenov');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletabledec');
		obj2 = document.getElementById('basketballscheduledec');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablejan');
		obj2 = document.getElementById('basketballschedulejan');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablefeb');
		obj2 = document.getElementById('basketballschedulefeb');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablemar');
		obj2 = document.getElementById('basketballschedulemar');
		if (obj != null)
			obj.style['display'] = 'inline';
		if (obj2 != null)
			obj2.style.backgroundColor = '#990000';
		sBasketballScheduleSelected = 'basketballschedulemar';
	}
	else if (stype == 'feb'){
		obj = document.getElementById('basketballscheduletablenov');
		obj2 = document.getElementById('basketballschedulenov');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletabledec');
		obj2 = document.getElementById('basketballscheduledec');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablejan');
		obj2 = document.getElementById('basketballschedulejan');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablefeb');
		obj2 = document.getElementById('basketballschedulefeb');
		if (obj != null)
			obj.style['display'] = 'inline';
		if (obj2 != null)
			obj2.style.backgroundColor = '#990000';
		obj = document.getElementById('basketballscheduletablemar');
		obj2 = document.getElementById('basketballschedulemar');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		sBasketballScheduleSelected = 'basketballschedulefeb';
	}
	else if (stype == 'jan'){
		obj = document.getElementById('basketballscheduletablenov');
		obj2 = document.getElementById('basketballschedulenov');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletabledec');
		obj2 = document.getElementById('basketballscheduledec');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablejan');
		obj2 = document.getElementById('basketballschedulejan');
		if (obj != null)
			obj.style['display'] = 'inline';
		if (obj2 != null)
			obj2.style.backgroundColor = '#990000';
		obj = document.getElementById('basketballscheduletablefeb');
		obj2 = document.getElementById('basketballschedulefeb');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablemar');
		obj2 = document.getElementById('basketballschedulemar');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		sBasketballScheduleSelected = 'basketballschedulejan';
	}
	else if(stype == 'dec'){
		obj = document.getElementById('basketballscheduletablenov');
		obj2 = document.getElementById('basketballschedulenov');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletabledec');
		obj2 = document.getElementById('basketballscheduledec');
		if (obj != null)
			obj.style['display'] = 'inline';
		if (obj2 != null)
			obj2.style.backgroundColor = '#990000';
		obj = document.getElementById('basketballscheduletablejan');
		obj2 = document.getElementById('basketballschedulejan');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablefeb');
		obj2 = document.getElementById('basketballschedulefeb');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablemar');
		obj2 = document.getElementById('basketballschedulemar');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		sBasketballScheduleSelected = 'basketballscheduledec';
	}
	else{
		obj = document.getElementById('basketballscheduletablenov');
		obj2 = document.getElementById('basketballschedulenov');
		if (obj != null)
			obj.style['display'] = 'inline';
		if (obj2 != null)
			obj2.style.backgroundColor = '#990000';
		obj = document.getElementById('basketballscheduletabledec');
		obj2 = document.getElementById('basketballscheduledec');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablejan');
		obj2 = document.getElementById('basketballschedulejan');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablefeb');
		obj2 = document.getElementById('basketballschedulefeb');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		obj = document.getElementById('basketballscheduletablemar');
		obj2 = document.getElementById('basketballschedulemar');
		if (obj != null)
			obj.style['display'] = 'none';
		if (obj2 != null)
			obj2.style.backgroundColor = '#CBCBCB';
		sBasketballScheduleSelected = 'basketballschedulenov';
	}
}
</script>
