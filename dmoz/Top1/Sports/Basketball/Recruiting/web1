<script type="text/javascript">
var fadeimages=new Array()
fadeimages[0]=["http://www.eurobasket.com/AcPhotos_Players/Moon_Jamario.jpg", "Greece/basketball.asp", ""]
fadeimages[1]=["http://www.eurobasket.com/AcPhotos_Players/1Rozic_Marin.jpg", "http://www.eurobasket.com/Adriatic-League/basketball.asp", ""]
</script>
<script type="text/javascript">

var fadebgcolor="white"
 
////NO need to edit beyond here/////////////
 
var fadearray=new Array() //array to cache fadeshow instances
var fadeclear=new Array() //array to cache corresponding clearinterval pointers
 
var dom=(document.getElementById) //modern dom browsers
var iebrowser=document.all
 
function fadeshow(theimages, fadewidth, fadeheight1, fadeheight2 , borderwidth, delay, pause, displayorder){
	this.pausecheck=pause
	this.mouseovercheck=0
	this.delay=delay
	this.degree=10 //initial opacity degree (10%)
	this.curimageindex=0
	this.nextimageindex=1
	fadearray[fadearray.length]=this
	this.slideshowid=fadearray.length-1
	this.canvasbase="canvas"+this.slideshowid
	this.curcanvas=this.canvasbase+"_0"
	//if (typeof displayorder!="undefined"){
	//	alert("it is undifined")
		//theimages.sort(function() {return 0.5 - Math.random();}) //thanks to Mike (aka Mwinter) :)
	//	theimages.sort(function() {return 0.5;}) //thanks to Mike (aka Mwinter) :)
	//}
	this.theimages=theimages
	this.imageborder=parseInt(borderwidth)
	this.postimages=new Array() //preload images
	for (p=0;p<theimages.length;p++){
		this.postimages[p]=new Image()
		this.postimages[p].src=theimages[p][0]
	}
 
	var fadewidth=fadewidth+this.imageborder*2
	//var fadeheight1=fadeheight1+this.imageborder*2
	//var fadeheight2=fadeheight2+this.imageborder*2
	this.fadeheight1=fadeheight1+this.imageborder*2
	this.fadeheight2=fadeheight2+this.imageborder*2

	
	//alert("fadeheight1:[" + fadeheight1 + "] fadeheight2:[" + fadeheight2 + "]") 
	//alert("hDifference =[" + 9 + "]" )
	
	if (iebrowser&&dom||dom){ //if IE5+ or modern browsers (ie: Firefox)
		//document.write('<div id="master'+this.slideshowid+'" style="position:relative;width:'+fadewidth+'px;height:'+(fadeheight1)+'px;overflow:hidden;"><div id="'+this.canvasbase+'_0" style="position:absolute;width:'+fadewidth+'px;height:'+500+'px;top:0;left:0;filter:progid:DXImageTransform.Microsoft.alpha(opacity=10);-moz-opacity:10;-khtml-opacity:10;background-color:'+fadebgcolor+'"></div><div id="'+this.canvasbase+'_1" style="position:absolute;width:'+fadewidth+'px;height:'+200+'px;top:0;left:0;filter:progid:DXImageTransform.Microsoft.alpha(opacity=10);-moz-opacity:10;background-color:'+fadebgcolor+'"></div></div>')
		document.write('<div id="master'+this.slideshowid+'" style="font-family:Arial; position:relative;width:'+fadewidth+'px;height:'+(fadeheight1)+'px;overflow:hidden;"><div id="'+this.canvasbase+'_0" style="font-family:Arial; position:absolute;width:'+fadewidth+'px;height:'+(fadeheight1)+'px;top:0;left:0;filter:progid:DXImageTransform.Microsoft.alpha(opacity=10);-moz-opacity:10;-khtml-opacity:10;background-color:'+fadebgcolor+'"></div><div id="'+this.canvasbase+'_1" style="font-family:Arial; position:absolute;width:'+fadewidth+'px;height:'+(fadeheight1)+'px;top:0;left:0;filter:progid:DXImageTransform.Microsoft.alpha(opacity=10);-moz-opacity:10;background-color:'+fadebgcolor+'"></div></div>')
	}
	else{
		document.write('<div><img name="defaultslide'+this.slideshowid+'" src="'+this.postimages[0].src+'"></div>')
	}
 
	if (iebrowser&&dom||dom) //if IE5+ or modern browsers such as Firefox
		this.startit()
	else{
		this.curimageindex++
		setInterval("fadearray["+this.slideshowid+"].rotateimage()", this.delay)
	}
}

function fadepic(obj){
	if (obj.degree<100){
		obj.degree+=10
		if (obj.tempobj.filters&&obj.tempobj.filters[0]){
			if (typeof obj.tempobj.filters[0].opacity=="number") //if IE6+
				obj.tempobj.filters[0].opacity=obj.degree
			else //else if IE5.5-
				obj.tempobj.style.filter="alpha(opacity="+obj.degree+")"
		}
		else if (obj.tempobj.style.MozOpacity)
			obj.tempobj.style.MozOpacity=obj.degree/101
		else if (obj.tempobj.style.KhtmlOpacity)
			obj.tempobj.style.KhtmlOpacity=obj.degree/100
	}
	else{
		clearInterval(fadeclear[obj.slideshowid])
		obj.nextcanvas=(obj.curcanvas==obj.canvasbase+"_0")? obj.canvasbase+"_0" : obj.canvasbase+"_1"
		obj.tempobj=iebrowser? iebrowser[obj.nextcanvas] : document.getElementById(obj.nextcanvas)
		obj.populateslide(obj.tempobj, obj.nextimageindex)
		obj.nextimageindex=(obj.nextimageindex<obj.postimages.length-1)? obj.nextimageindex+1 : 0
		setTimeout("fadearray["+obj.slideshowid+"].rotateimage()", obj.delay)
		}
	}
	 
	fadeshow.prototype.populateslide=function(picobj, picindex){
		var slideHTML=""
		slideHTML+='<div style="width:150px;">'
		if (this.theimages[picindex][1]!="") //if associated link exists for image
			slideHTML='<a href="'+this.theimages[picindex][1]+'" target="'+this.theimages[picindex][2]+'">'
		slideHTML+='<img src="'+this.postimages[picindex].src+'" border="'+this.imageborder+'px" width="150"'
		//slideHTML+='<img src="'+this.postimages[picindex].src+'" border="'+this.imageborder+'px" width="150">'
		if (picindex==0){
			//alert("picindex=0: " + this.fadeheight1)
			
			slideHTML+= 'height="' + (this.fadeheight1-17) + '">'
			if (this.theimages[picindex][1]!="") {//if associated link exists for image
				slideHTML+='</a>'
			}
			//alert("this is:"+ (this.fadeheight1-17))
			//slideHTML+= '<p><font size=1>Jamario Moon</font></p>'
			slideHTML+= '<table border=0 width=150  bgcolor="white" align="center" cellpadding=0 cellspacing=0><tr>'
			slideHTML+= '<td valign=top align="center">'
			slideHTML+= '<font family="Arial" size="1">'
//			if("Greece" != "1"){
//				slideHTML+= 'Greece: '
//			}
			slideHTML+= 'Jamario Moon</font></td>'
			slideHTML+= '</tr></table>'	
		}else{
			//alert("picindex=" + this.fadeheight2  )
			//slideHTML+= 'height=' + this.fadeheight2 + '> this is a test2'
			slideHTML+= 'height="' + (this.fadeheight2-17) + '">'
			if (this.theimages[picindex][1]!="") {//if associated link exists for image
				slideHTML+='</a>'
			}
			//alert("this is:"+ (this.fadeheight2-17))
			//slideHTML+= '<p><font size=1>Marin Rozic</font></p><p><table border=0 width=150  bgcolor="white" align="center"><tr><td align="center" height=9><a href="http://www.eurobasket.com/search.asp"><img src="http://www.eurobasket.com/sysImg/buttons/btnDB2.jpg"></a></td></tr></table></p>'
			//alert("hDifference =[" + 9 + "]" )
			//slideHTML+= '<p><font size=1>Marin Rozic</font>' 
			slideHTML+= '<table border=0 width=150  bgcolor="white" align="center" cellpadding=0 cellspacing=0><tr>'
//			slideHTML+= '<td valign=top align="center"><font family="Arial" size="1">Adriatic League: Marin Rozic</font></td>'
			slideHTML+= '<td valign=top align="center"><font family="Arial" size="1">'
//			if("Adriatic League" != "1"){
//			slideHTML+= 'Adriatic League: '
//			}
			slideHTML+= 'Marin Rozic</font></td>'
			slideHTML+= '</tr></table>'	
			slideHTML+= '<table border=0 width=150  bgcolor="white" align="center" cellpadding=0 cellspacing=0><tr>'
			if(9 >= 20 && 9 <= 29){			
				slideHTML+=	'<td valign=top align="center" height=9><img src="http://www.eurobasket.com/images/DBase150_20.jpg" alt="Database" onclick=\'location.href="http://www.eurobasket.com/basketball-search.asp"\'></td>'		
			}else if(9 >= 30 && 9 <= 39){			
				slideHTML+=	'<td valign=top align="center" height=9><img src="http://www.eurobasket.com/images/DBase150_30.jpg" alt="Database" onclick=\'location.href="http://www.eurobasket.com/basketball-search.asp"\'></td>'		
			}else if(9 >= 40 && 9 <= 49){			
				slideHTML+=	'<td valign=top align="center" height=9><img src="http://www.eurobasket.com/images/DBase150_20.jpg" alt="Database" onclick=\'location.href="http://www.eurobasket.com/basketball-search.asp"\'><br><img src="http://www.eurobasket.com/images/JMarket150_20.jpg" alt="Job Market" onclick=\'location.href="http://www.eurobasket.com/job/index.asp"\'></td>'		
