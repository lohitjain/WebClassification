				<script type="text/javascript">
				var xmlhttp;
				var xmlPutInID;
				function loadXMLDoc(url,xdivi)
				{
				xmlPutInID=xdivi
				xmlhttp=null;
				if (window.XMLHttpRequest)
				  {// code for IE7+, Firefox, Chrome, Opera, Safari
				  xmlhttp=new XMLHttpRequest();
				  }
				else
				  {// code for IE6, IE5
				  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
				  }
				xmlhttp.onreadystatechange=state_Change;
				xmlhttp.open("GET",url,true);
				xmlhttp.send(null);
				}
				
				function state_Change()
				{
				if (xmlhttp.readyState==4)
				  {// 4 = "loaded"

				  if (xmlhttp.status==200)
				    {// 200 = "OK"
					
					if (xmlhttp.responseText === null) return;
					
				   if (xmlhttp.responseText.length > 30)  {
				    document.getElementById(xmlPutInID).innerHTML=xmlhttp.responseText;
				    document.getElementById(xmlPutInID).style.display="block";
				    }
					}
				  else
				    {
				    	//alert("Problem retrieving data:" + xmlhttp.statusText);
				    }
				  }
				}
				loadXMLDoc("http://www.eurobasket.com/NotifyUsers.asp","notifymembsMSG");
				</script>
				<div id="notifymembsMSG" style="display: none; margin: 0px;" align="center"></div>

