  <script>
      var s_account = "premiumtvnbltaipans";
  </script>
  <script src="/js/global/js/core/omniture.js"></script>
  <script>
  s.pageName="Home";
  s.channel="";
  s.server="";
  s.pageType="";
  s.prop1="http://www.taipans.com/";
  s.prop2="";
  s.prop3="";
  s.prop4="";
  s.prop5="";
  s.prop6="";
  s.prop7="";
  s.prop8="";
  s.prop9="";
  s.prop10="";
  /* E-commerce Variables */
  s.campaign="";
  s.state="";
  s.zip="";
  s.events="";
  s.products="";
  s.purchaseID="";
  s.currencyCode="";
  s.eVar1="";
  s.eVar2="";
  s.eVar3="";
  s.eVar4="";
  s.eVar5="";
  s.eVar6="";
  s.eVar7="";
  s.eVar8="";
  s.eVar9="";
  s.eVar10="";


  function updateOmnitureVideoStarted(duration, prop31StrSuffix) {
    var eventsStr="event11";
    var productsStr=";;;;event16=0.00|event17=0";
    var prop33Str=getDurationBucket(duration);
    
    sendOmnitureLinkRequest(eventsStr, productsStr, prop33Str, prop31StrSuffix);
  }

  function getProp31StrSuffix(duration, hasAdvertising) {
    var suffix = "";
    if (duration < 30 && hasAdvertising) {
      suffix = " Pre-Roll";
    }
    
    return suffix;
  }

  function sendOmnitureLinkRequest(eventsStr, productsStr, prop33Str, prop31StrSuffix) {
    //we always append event16 & event 17
    var eventsStrSuffix = "event16,event17";
    var prop31Str=""; 
    prop31Str += prop31StrSuffix;
    
    var prop32Str = "/home";

    if (eventsStr != "") {
      eventsStr += ("," + eventsStrSuffix);
    }
   
    s.products=productsStr;
    s.linkTrackVars = "prop31,prop32,prop33,eVar31,eVar32,eVar33,events,products";
    s.linkTrackEvents = eventsStr;
    s.events=eventsStr;
    s.prop31=s.eVar31=prop31Str; 
    s.prop32=s.eVar32=prop32Str;
    s.prop33=s.eVar33=prop33Str;
    s.tl(true, 'o', prop31Str);
  }
  
  function sendMilestoneEvent(mileStone, duration, hasAdvertising) {    
    var prop31StrSuffix = getProp31StrSuffix(duration, hasAdvertising);
    var eventsStr = "";
    var prop33Str=getDurationBucket(duration);
    var percent25 = round(duration * 0.25);
    var productsStr = getProductsStr(0.25, percent25);
    
    if (mileStone == '0.00') {
      updateOmnitureVideoStarted(duration, prop31StrSuffix);
    } else if (mileStone == '0.25') {
      eventsStr = "event12";
      sendOmnitureLinkRequest(eventsStr, productsStr, prop33Str, prop31StrSuffix);
    } else if (mileStone == '0.50') {
      eventsStr = "event13";
      sendOmnitureLinkRequest(eventsStr, productsStr, prop33Str, prop31StrSuffix);
    } else if (mileStone == '0.75') {
      eventsStr = "event14";
      sendOmnitureLinkRequest(eventsStr, productsStr, prop33Str, prop31StrSuffix);
    } else if (mileStone == '1.00') {
      eventsStr = "event15";
      sendOmnitureLinkRequest(eventsStr, productsStr, prop33Str, prop31StrSuffix);
    }
  }
 
  function updateOmnitureVideoFinished(percentage, secondsWatched, duration, prop31StrSuffix) {
    var eventsStr="event15";
    var productsStr=getProductsStr(percentage, secondsWatched);
    var prop33Str=getDurationBucket(duration);
    
    sendOmnitureLinkRequest(eventsStr, productsStr, prop33Str, prop31StrSuffix)
  }

  function getProductsStr(percentage, secondsWatched) {
    var productsStrPreffix = ";;;;";

    return productsStrPreffix + "event16="+percentage+"|event17="+secondsWatched;
  }
  
  function getDurationBucket(duration) {
    var bucketStr = "";

    var minuteInSeconds = 60;

    if (duration <= 10)
      bucketStr = "0 - 10 Seconds";
    else if (duration > 10 && duration <= 30)
      bucketStr = "10 - 30 Seconds";
    else if (duration > 30 && duration <= 60)
      bucketStr = "30 - 60 Seconds";
    else if (duration > minuteInSeconds && duration <= (minuteInSeconds*2))
      bucketStr = "1 - 2 Minutes";
    else if (duration > (minuteInSeconds*2) && duration <= (minuteInSeconds*5))
      bucketStr = "2 - 5 Minutes";
    else if (duration > (minuteInSeconds*5) && duration <= (minuteInSeconds*10))
      bucketStr = "5 - 10 Minutes";
    else if (duration > (minuteInSeconds*10) && duration <= (minuteInSeconds*30))
      bucketStr = "10 - 30 Minutes";
    else if (duration > (minuteInSeconds*30) && duration <= (minuteInSeconds*60))
      bucketStr = "30 - 60 Minutes";
    else if (duration > (minuteInSeconds*60))
      bucketStr = "60+ Minutes";
    else
      bucketStr="undefined"; 

    return bucketStr;
  }
  
  function round(numberToRound) {
    return Math.round(numberToRound*100)/100;
  }
  

