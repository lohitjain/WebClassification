<script>  
$(document).ready(function() {
                $('a.tab').bind('click', function() {
                $('#tabs li').removeClass("selected");
                $(this).parent().addClass("selected");
                var link =  $(this).attr("href");
                $('.tabContent').hide();
                $(link).show();             
                return false;          
                });
});  
</script>  
