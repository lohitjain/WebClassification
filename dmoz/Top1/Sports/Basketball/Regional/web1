<script>

$(document).ready(function() {
  $('#newstabs ul').each(function(){
    var $active, $content, $links = $(this).find('a');

    $active = $($links.filter('[href="'+location.hash+'"]')[0] || $links[0]);
    $active.addClass('active');
    $content = $($active.attr('href'));


    $links.not($active).each(function () {
        $($(this).attr('href')).hide();
    });


    $(this).on('click', 'a', function(e){

        $active.removeClass('active');
        $content.hide();


        $active = $(this);
        $content = $($(this).attr('href'));


        $active.addClass('active');
        $content.show();


        e.preventDefault();
    });
    });
    
 $('#videotable').find('a.video ').live('click', function() {  
        var vidId = $(this).attr('data-videouuid');
        var url = '/fragment/nblClubSites/fragments/assets/vod/player/ajax/true/video/' + vidId  + '/width/940/height/540/wrapper/vodPlayer/autoplay/true';
        videoclick(vidId,url);
        return false;
        });

       var myClose=function(hash) { $('object#flashPlayer').remove(); hash.w.fadeOut('2000',function(){ hash.o.remove(); });  }; 
       function videoclick(vidId,Url){  
       $('#ex2').jqm({ajax: Url, trigger: 'a.ex2trigger', overlay:90, onHide:myClose });
       }
});
</script>



