 <script>  
      function createPlayer(clipId) {
    var configXmlPath = "/configXml";
    if (swfobject.hasFlashPlayerVersion("10")) {
        var params = {
            wmode: "transparent",
            allowScriptAccess: "always",
            base: "/flash/unifiedplayer/",
            bgcolor: "#ffffff",
            allowFullScreen: "true"
        };

        var flashVars = {
            configXML: configXmlPath,
            resourceLocalePath: "/ptvFlash/unifiedplayer/assets/resources/",
            basePath: "/",
            selectedBandwidth: "med",
            clipId: clipId,
            locale: "en_GB",
            setPlayerType: "vod",

        };
        swfobject.embedSWF("/ptvFlash/unifiedplayer/adaptive_vod/UnifiedPlayer.swf", "videoPlayer"+clipId, "488", "359", "10", false, flashVars, params, {});
    }
    swfobject.registerObject("videoPlayer", "10.0.0", "/ptvFlash/SWFObject/SWFObject2_2/expressInstall.swf");
   }

  $(document).ready(function() {
    $("#newsreaderDisplay li a.play-icon").click(function(){
      if( $(this).parent().find(".play-icon").length ){
        $(this).parent().find(".play-icon").hide();
        var playerElement = $(this).parent().find(".play-icon");
        var clipId = playerElement.attr("clipId");
        $.newsreader.pause();
        $(this).parent().find(".play-icon").after('<div id="videoPlayer'+ clipId +'" class="video-player" style="visibility:visible;"></div>');
        createPlayer( clipId );
        $(".newsreaderMain .newsreader #newsreaderDisplay li object").css("visibility", "visible");
        return false;
      }else if( $(this).parent().find("object").length ){
        $(this).parent().find(".play-icon").hide();
        $(this).parent().find("object").show();
        return false;
      }
    });
  
    $('#newsreaderNav a, #newsreaderNav li').click(function(){
      $('.newsreaderMain .newsreader #newsreaderDisplay .play-icon').show();
    });
  
    if( $.browser.msie ){
      $('#newsreaderNav ul').css({
        float: 'left',
        position: 'absolute',
        left: '50%'
      });
      $('#newsreaderNav ul').css({
        marginLeft: '-'+($('#newsreaderNav ul').width()/2)+'px'
      });
    }
  });            
</script>    <span class="left" style="top:370px; left:1px;"></span>
