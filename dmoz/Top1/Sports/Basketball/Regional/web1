<script  language="JavaScript" type="text/javascript">
$(function() {
      $('a.photo').click( function() {  
        var uuid = $(this).attr("data-videoUuid");
        var Url = '/fragment/nblClubSites/fragments/index/gallery/ajax/true/autoplay/true/uuid/'+uuid;
        //console.log(Url);
        $.ajax({
                url: Url                
               }).done(function( html ) {
                $("#ex3").html(html);     
                $('#ex3').jqm({overlay:90}).jqmShow()
              });
                
        
        return false;
        });
});


</script>  
  
