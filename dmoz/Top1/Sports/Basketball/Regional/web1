jQuery(window).load(function(){
    jQuery("#footer .search-form").submit(function( event ) {
        var searchterm = jQuery(this).find('input[type="text"]').val();
        localStorage.setItem('searchname', searchterm);
        if (localStorage["searchname"]) {
            _gaq.push(['news._trackEvent', 'searchterm', searchterm]);
        }
    });
});
