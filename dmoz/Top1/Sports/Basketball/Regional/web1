            function validate(f) {
            var err={};
            if (f.search_value.value=="") {
            err.message="Please enter a postcode or association name";
            err.field=f.search_value;
            }
            if (err.message) {
            alert(err.message);
            err.field.focus();
            return false;
            }
            window.open ("", "NewWindow","menubar=0,location=no,resizable=1,scrollbars=1,width=865,height=700");
            return true;
            }
            </script>

