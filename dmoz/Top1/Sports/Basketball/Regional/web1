<script>  
	$(document).ready(function() {
    	$('a.tab').bind('click', function() {
        	$('#tabs li').removeClass("selected");
            $(this).parent().addClass("selected");
            var link =  $(this).attr("href");
            $('.videosTabContent').hide();
            
            /**
             * Specific behaviour for club tab 
             */
            if(link == "#tab-3") {
            	var club = $('#clubFilter').val();
            	filterByClub(club);
            }
            
            $(link).show();             
            return false;          
        });
        
        $('#clubFilter').change(function(val) {
        	var club = $('#clubFilter').val();
           	filterByClub(club);
            
            $('#tabs li').removeClass('selected');
            $('#tabs-3').parent().addClass('selected');
            
            $('.videosTabContent').hide();
            $('.nbltvCarousel #tab-3').show();
        });
	});  
    
    function filterByClub(club) {
    	var filterUrl = "";
        if(club == "all") {
        	filterUrl = "/fragment/nblClubSites/fragments/index/dvrEventList/carouselContainer/clubFilterCar"
        } else {
            filterUrl = "/fragment/nblClubSites/fragments/index/dvrEventList/filter/" + club + " Home," + club + " Away/carouselContainer/clubFilterCar"
        }
        
        $.ajax({
        	url: filterUrl,
            success: function(data) {
            	$('#clubFilterCar').html(data);
            }
            
        });
    }
</script>  
            
