prite.png") repeat scroll 0 -287px transparent;width: 130px;}}@media (max-width: 767px){#f-tdrive-wrap,.wider #f-tdrive-wrap{margin: 40px 0 20px;width: 100%;}.f-tdrive,.wider .f-tdrive{width: 48%;}.f-tdrive.two,.wider .f-tdrive.two{display: block;}#f-links .two,#f-links .three{display: none;}#f-links,.wider #f-links{width: 45%;}}@media (max-width: 629px){.f-tdrive,.wider .f-tdrive{width: 100%;}.f-tdrive.two,.wider .f-tdrive.two{display: none;}#footer-wrap #f-powered,.wider #footer-wrap #f-powered{background-position: left -286px;background-repeat: no-repeat;margin-left: 0;width: 25%;}#f-links,.wider #f-links{width: 33%;}}@media (max-width: 479px){.f-divide{display: none;}#f-admin{float: right;}#f-links,.wider #f-links{width: 30%;}#f-links .four{display: none;}#footer-bottom{height: auto;}#f-social{width: 100%;}#footer-wrap #f-powered,.wider #footer-wrap #f-powered{width:30%;}}@media (max-width: 409px){#f-admin{margin: 40px 0 0;width: 100%;}.wider #searchname-foot{width: 75%;}#footer-wrap #f-powered,.wider #footer-wrap #f-powered{background-position: left -286px;background-repeat: no-repeat;margin-left: 0;width: 40%;}#f-links,.wider #f-links{width: 30%;}}</style>
