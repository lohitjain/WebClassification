        <script type="text/javascript" charset="utf-8">
	var moduleId = 183;
	var RSFSearch183;

var results_box = document.getElementById('search_suggest183');
var searchReq = getXmlHttpRequestObject();

function searchSuggest183(){
	if(searchReq.readyState == 4 || searchReq.readyState == 0){
		var str = escape(document.getElementById('rsf_inp183').value);
		searchReq.open("GET", '/index.php?option=com_rssearch&view=results&layout=ajax&module_id=183&search='+str, true);
		searchReq.onreadystatechange = function(){
			if(searchReq.readyState == 4){
				var ss = document.getElementById('search_suggest183')

				ss.innerHTML = '';
				if (searchReq.responseText.indexOf('---RSSEARCH_SEPARATOR---') > -1){
					var str = searchReq.responseText.split("---RSSEARCH_SEPARATOR---");
					var suggest = '';
					var results = str.length;
					var limit = 5;
				
					if(results < limit){
						var max = results;
					}else {
						var max = limit;
					}

					for(i=0; i<max; i++){
						suggest += '<div onmouseover="javascript:suggestOver183(this);" ';
						suggest += 'onmouseout="javascript:suggestOut183(this);"';
						suggest += 'class="suggest_link">' + str[i] + '</div>';
					}
					if(results > limit){
						suggest += '<p style="text-align:center;cursor:pointer;" onclick="javascript:setSearch183(this.innerHTML);">View all results</a></p>';
					}
					suggest += '<p style="position:absolute;bottom:5px;cursor:pointer;right:10px;padding:2px;margin:0px auto;" onclick="javascript:closeSearch183();"><img src="http://www.nswbasketball.net.au/modules/mod_rssearch/assets/images/close.png" alt="" /></p>';
					ss.setHTML(suggest);					RSFSearch183.slideIn();
				}
			}
		}; 
		searchReq.send(null);
	}
}

function suggestOver183(div_value){
	div_value.className = 'suggest_link_over';
}

function suggestOut183(div_value){
	div_value.className = 'suggest_link';
}

function setSearch183(value){
	document.getElementById('search_suggest183').innerHTML = '';
	document.getElementById('frmSearch'+183).submit();
}
function closeSearch183(){
	RSFSearch183.slideOut();
}

window.addEvent('domready', function(){
	$('search_suggest183').setStyle('height','auto');
	RSFSearch183 = new Fx.Slide('search_suggest183').hide();
	var parent = $('search_suggest183').getParent('div').setStyle('width','210px');
});
</script>

<div id="rssearch" class="rssearch_box">
	<form id="frmSearch183" action="/index.php" method="GET" accept-charset="utf-8" class="rsf_form">

		<input type="hidden" name="option" value="com_rssearch" />
		<input type="hidden" name="view" value="results" />
		<input type="hidden" name="layout" value="default" />
		<input type="hidden" name="module_id" value="183" />
		<input type="text" id="rsf_inp183" name="search" alt="Enter keywords" onkeyup="searchSuggest183();" autocomplete="off" style="width:210px" />

		 <input type="submit" id="cmdSearch" name="cmdSearch" value="Search" />		<input type="hidden" name="Itemid" value="1" />
		<br />

		<div id="search_suggest183" class="rsfsuggestions" style="width:210px">

		</div>
	</form>
