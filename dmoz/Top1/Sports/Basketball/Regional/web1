<script type="text/javascript">

  function mycarousel_initCallback(carousel) {
    // Pause autoscrolling if the user moves with the cursor over the clip.

    carousel.clip.hover(function() {
        carousel.stopAuto();
    }, function() {
        carousel.startAuto();
    });

  jQuery('#photogal205271Next').click(function() {
    carousel.prev();
    carousel.startAuto(0);
    return false;
    });

    jQuery('#photogal205271Prev').click(function() {
        carousel.next();
        carousel.startAuto(0);
        return false;
    });
  };

  jQuery(document).ready(function() {
    jQuery('#photogal205271').jcarousel({
      auto: 9,
      scroll: 1,
      wrap: 'circular',
      initCallback: mycarousel_initCallback,
      buttonPrevHTML: null,
      buttonNextHTML: null
    });
});
</script>





