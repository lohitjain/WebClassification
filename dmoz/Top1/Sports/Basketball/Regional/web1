				jQuery(function() {
					jQuery.getJSON("http://gdata.youtube.com/feeds/api/users/basketballaus/uploads?max-results=1&orderby=published&v=2&alt=jsonc&callback=?", function(data) {
						var dataContainer = jQuery("#play-video_28235592");
						jQuery.each(data.data.items, function(i, val) {
							if (typeof(val.player) !== 'undefined' && typeof(val.title) !== 'undefined') {
								dataContainer.append("<iframe width='300' height='220' src='http://www.youtube.com/embed/"+val.id+"?rel=0&autohide=1&amp;wmode=transparent'  frameborder='0'  name='vid-frame' id='vid-fade_28235592'  allowfullscreen></iframe>");
							}
						});
					}); 
					jQuery.getJSON("http://gdata.youtube.com/feeds/api/users/basketballaus/uploads?max-results=4&orderby=published&v=2&alt=jsonc&callback=?", function(data) {
						var dataContainer = jQuery("#video-thumbs_28235592");
						jQuery.each(data.data.items, function(i, val) {
							if (typeof(val.player) !== 'undefined' && typeof(val.title) !== 'undefined') {
								dataContainer.append("<div class='vid-thumb thumb-id-"+ (i+1) +"'><a href = 'http://www.youtube.com/embed/"+val.id+"?rel=0' target = 'vid-frame' id='click-vid_28235592'><img src='"+val.thumbnail.sqDefault+"' width='68'></a></div>");
							}
						});
					});     	
				});
				</script>
				
