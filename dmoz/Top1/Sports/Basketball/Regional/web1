	<script type="text/javascript">

	var crs_array	= new Array();

	var crs_obj	= '';

	var crs_scrollPos 	= '';

	var crs_numScrolls	= '';

	var crs_heightOfElm = '86'; // Height of each element (px)

	var crs_numberOfElm = '1';

	var crs_scrollOn 	= 'true';

	function vsra_createscroll() 

	{

		crs_array[0] = '<div class=\'crs_div\' style=\'height:82px;padding:2px 0px 10px 0px;\'><a href=\'index.php?option=com_content&view=article&id=573\'>2014 Waratah Championship League Draw<br><p>\r\n	<a href=\"http://www.nswbasketball.net.au/media/files/2014%20Waratah%20League%20Draw(2).pdf\">Click Here</a></p></a></div>'; crs_array[1] = '<div class=\'crs_div\' style=\'height:82px;padding:2px 0px 10px 0px;\'><a href=\'index.php?option=com_content&view=article&id=558\'>Aussie Hoops - Term 1 2014<br><p>\r\n	<a href=\"http://www.nswbasketball.net.au/media/files/2014%20Term%201%20.pdf\"><strong>Click here</strong></a></p></a></div>'; crs_array[2] = '<div class=\'crs_div\' style=\'height:82px;padding:2px 0px 10px 0px;\'><a href=\'index.php?option=com_content&view=article&id=576\'>DSR Sports Taping Workshop<br><p>\r\n	<a href=\"http://www.nswbasketball.net.au/media/files/Sports%20Taping%20workshop%20March%20to%20May%202014-1.pdf\"><strong>Click here</strong></a></p></a></div>'; crs_array[3] = '<div class=\'crs_div\' style=\'height:82px;padding:2px 0px 10px 0px;\'><a href=\'index.php?option=com_content&view=article&id=570\'>Full Court Press - February Edition<br><p>\r\n	<a href=\"http://www.nswbasketball.net.au/media/files/newsletter%20Feb%202014%20final%20-%20web.pdf\">Click Here</a></p></a></div>'; crs_array[4] = '<div class=\'crs_div\' style=\'height:82px;padding:2px 0px 10px 0px;\'><a href=\'index.php?option=com_content&view=article&id=572\'>Level 2 Coaching Course - March 1 & 2<br><p>\r\n	<a href=\"http://www.nswbasketball.net.au/media/files/2014%20L2%20Coach%20Course%20Flyer.pdf\"><strong>Click here</strong></a></p></a></div>'; 
		crs_obj	= document.getElementById('crs_Holder');

		crs_obj.style.height = (crs_numberOfElm * crs_heightOfElm) + 'px'; // Set height of DIV

		vsra_content();

	}

	</script>

