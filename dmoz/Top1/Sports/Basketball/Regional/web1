<script>
// below gets the last game match and then adds 48 hours to the timestamp. If it is past that period increment the game week

(function() {
            var lastMatchTimeStamp = 1393124400+(48 * 60 * 60);
            var currentTimeStamp = 1393360659;
            var currentMatchWeek = 19;
       
     if (currentTimeStamp > lastMatchTimeStamp && currentMatchWeek <= 25) {
            currentMatchWeek = currentMatchWeek + 1;
            }
       
         $('#fixtureCarousel').jcarousel({
                                       size:23,
                                       start: currentMatchWeek,
                                       scroll:1
         });
       
      })();
    
</script>   

       
  
