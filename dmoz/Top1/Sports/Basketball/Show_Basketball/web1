<script language="JavaScript">
  var interval = 15000;
  var features = new Array();
  var featureCounter = 0;
  var featureIndex = 0;
  var featureTimer;
  var preloadIndex = 0;
  function previousfeature() {
    featureIndex = (featureIndex - 1) % features.length;
    if (featureIndex < 0) {
      featureIndex = featureIndex + features.length;
    }
    document.getElementById('featureContent').innerHTML = features[featureIndex]
    clearTimeout(featureTimer);
  }
  function pausefeatures() {
    clearTimeout(featureTimer);
  }
  function nextfeature() {
    if (features.length > 0) {
      featureIndex = (featureIndex + 1) % features.length;
      document.getElementById('featureContent').innerHTML = features[featureIndex]
      preloadIndex = (featureIndex + 1) % features.length;
      document.getElementById('featurePreload').innerHTML = features[preloadIndex]
      clearTimeout(featureTimer);
      featureTimer = setTimeout('nextfeature()', interval);
    }
  }
  function playfeatures() {
    nextfeature();
  }
</script>
