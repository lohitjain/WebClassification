		result = "Ergebnis ansehen";
		if (vote == "Submit")
			vote = "abstimmen";
	  }
	  if ( siteName == "brasil" )
	  {
		result = "Ver Resultados";
		if (vote == "Submit")
			vote = "Enviar";
	  }
	 
	if(poll > "") 
	{
	  
