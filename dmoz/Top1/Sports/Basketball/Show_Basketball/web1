function popupWindow (url) 
{
	popupWin = window.open(url, 'pollWindow', 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,width=' + tpWidth + ',height=' + tpHeight);
	popupWin.opener.top.name = "opener";
	popupWin.focus();
}
