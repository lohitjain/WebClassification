  <script>




var canDo = false;








function ss (inMsg)




{




    window.status = inMsg;




    return true;




}









function hiLite (inWhich, inRem) // highlights images, sets status




{




    if (canDo)




    {




        document.images[inWhich].src = "images/" + inWhich + "-on.gif";




    }




    return ss (inRem);




}









function dim (inWhich) // un-highlights images, clears status




{




    if (canDo)




    {




        document.images[inWhich].src = "images/" + inWhich + ".gif";




    }




    return ss ("");




}









function littleLite (inWhich, inRem) // highlights images, sets status




{




    if (canDo)




    {




        document.images[inWhich].src = "../images/small-on.gif";




    }



    return ss (inRem);




}









function littleDim (inWhich) // un-highlights images, clears status




{




    if (canDo)




    {




        document.images[inWhich].src = "../images/small.gif";




    }




    return ss ("");




}









function makeLink (inWhich, inRem) // writes out the image links




{




    var outPut = "<a href=\"" + inWhich + ".shtml\"" +




                 " onMouseOver=\"return hiLite('" + 




		 inWhich + "', '" + inRem +




		 "');\" onMouseOut=\"return dim('" + inWhich + 




		 "');\"><img name=\"" + inWhich + 




		 "\" height=90 width=108 alt=\"" +




		 inRem + "\" border=0 src=\"images/" + inWhich + 




		 ".gif\"></a>";




    document.write (outPut);




}




  </script> 
               
