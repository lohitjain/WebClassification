<!--//
var intActiveNewsIndex = 0;  // initialize active main news index
var objNewsRotatorTimer;  // variable for the news rotator timer interval

// Toggle Functions
function ToggleTab(intTab) {
	for(i=1; i<5; i++) {
	document.getElementById("cellTab" + i).className = "TabHeader CursorHand";
	}
	document.getElementById("cellTab" + intTab).className = "TabHeaderActive CursorHand";
}

function ToggleBox(intTab) {
	for(i=1; i<5; i++) {
	document.getElementById("divTab" + i).style.display = "none";
	}
	document.getElementById("divTab" + intTab).style.display = "block";
	document.getElementById("ActiveTab").value = intTab;
	// the following line is required for the video to display in firefox
	document.getElementById("frmVideo").src = "http://www.dailymotion.com/swf/5Up8xlAPxjL893LNo";
}

function RestoreTab() {
	intTab = document.getElementById("ActiveTab").value
	for(i=1; i<5; i++) {
	document.getElementById("cellTab" + i).className = "TabHeader CursorHand";
	}
	document.getElementById("cellTab" + intTab).className = "TabHeaderActive CursorHand";

}

// toggles Advanced Fighter Search On/Off
function ToggleFighterSearch() {
	if (document.getElementById("FighterSearch1").className == "ToggleOff") {
		for(i=1; i<5; i++) {
		document.getElementById("FighterSearch" + i).className = "ToggleOn";
		}
		document.getElementById("AdvSearch").innerHTML = "Basic Search";
	} else {
		for(i=1; i<5; i++) {
		document.getElementById("FighterSearch" + i).className = "ToggleOff";
		}
		document.getElementById("AdvSearch").innerHTML = "Advanced Search";
	}
}

// pause news rotator - stop timer
function PauseNews() 
{
	if (objNewsRotatorTimer != null)
	{
		window.clearTimeout(objNewsRotatorTimer);
		document.getElementById("imgPause").src = "_images/buttons/paused.gif";
		objNewsRotatorTimer = null;
	}
	else 
	{
		document.getElementById("imgPause").src = "_images/buttons/pause.gif";
		objNewsRotatorTimer = window.setTimeout(ForwardNews, 9000);
	}
}

// forward news
function ForwardNews() 
{
	if (objNewsRotatorTimer != null) 
	{
		window.clearTimeout(objNewsRotatorTimer);
	}
	HideNews(intActiveNewsIndex);
	intActiveNewsIndex = (intActiveNewsIndex + 1) % 5;
	ShowNews(intActiveNewsIndex);
	objNewsRotatorTimer = window.setTimeout(ForwardNews, 9000);	
}

// backward news
function BackwardNews() 
{
	if (objNewsRotatorTimer != null) 
	{
		window.clearTimeout(objNewsRotatorTimer);
	}
	HideNews(intActiveNewsIndex);
	intActiveNewsIndex = (intActiveNewsIndex - 1) % 5;
	if (intActiveNewsIndex < 0 )
	{
		intActiveNewsIndex = 4;
	}
	ShowNews(intActiveNewsIndex);
	objNewsRotatorTimer = window.setTimeout(ForwardNews, 9000);
}

// show the news divs with the specified index
function ShowNews(intIndex)
{
	document.getElementById("divNews" + intIndex).style.display = "block";
	document.getElementById("divSubNews" + intIndex).style.display = "block";
}

// hide the news divs with the specified index
function HideNews(intIndex) 
{
	document.getElementById("divNews" + intIndex).style.display = "none";
	document.getElementById("divSubNews" + intIndex).style.display = "none";
}

// onload - start timer for news rotator
function body_onload() 
{
	objNewsRotatorTimer = window.setTimeout(ForwardNews, 9000);
}

//-->
